{
  "title": "Terminal",
  "rules": [
{
  "description": "Leftcommand + R_miを開く",
  "manipulators": [
    {
      "type": "basic",
      "from": <%= from("r", ["left_control"]) %>,
      "to": <%= set_shell_command(["open -a 'mi'"]) %>
    }
  ]
},
{
  "description": "LeftAlt + J_chromeを開く",
  "manipulators": [
    {
      "type": "basic",
      "from": <%= from("j", ["left_alt"]) %>,
      "to": <%= set_shell_command(["open -a 'Google Chrome.app'"]) %>
    }
  ]
},
{
  "description": "None",
  "manipulators": [
    {
      "type": "basic",
      "from": <%= from("l", ["left_alt"]) %>,
      "to": <%= set_shell_command(["open -a 'Firefox.app'"]) %>
    }
  ]
},
{
  "description": "LeftAlt + K_vscodeを開く",
  "manipulators": [
    {
      "type": "basic",
      "from": <%= from("k", ["left_alt"]) %>,
      "to": <%= set_shell_command(["open -a 'Visual Studio Code.app'"]) %>
    }
  ]
},
{
  "description": "LeftAlt + semicolon_Path Finderを開く",
  "manipulators": [
    {
      "type": "basic",
      "from": <%= from("semicolon", ["left_alt"]) %>,
      "to": <%= set_shell_command(["open -a 'Path Finder.app'"]) %>
    }
  ]
},
{
  "description": "LeftCommand + X_ターミナルを開く",
  "manipulators": [
    {
      "type": "basic",
      "from": <%= from("x", ["left_control"]) %>,
      "to": <%= set_shell_command(["open -a 'Terminal.app'"]) %>
    }
  ]
},
{
  "description": "LeftAlt + l_vivaldiを開く",
  "manipulators": [
    {
      "type": "basic",
      "from": <%= from("l", ["left_alt"]) %>,
      "to": <%= set_shell_command(["open -a 'Vivaldi.app'"]) %>
    }
  ]
}
    ]
}