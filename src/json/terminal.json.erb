},
    {
      "description": "使用しないRightAlt + W_ActiveParellDeskWindows(iterm2Script)",
      "manipulators": [
        {
          "type": "basic",
          "from": <%= from("w", ["right_alt"], ["caps_lock"]) %>,
          "to": <%= set_shell_command(["open -a 'Parallels Desktop.app'"]) %>
        }
      ]
    },
    {
      "description": "RightAlt + S_ActiveParellDeskWindowsSublime(iterm2Script)",
      "manipulators": [
        {
          "type": "basic",
          "from": <%= from("s", ["right_alt"], ["caps_lock"]) %>,
          "to": <%= set_shell_command(["open -a 'Sublime Text 3.app'"]) %>
        }
      ]
    },
    {
      "description": "RightAlt + E_ActiveParellDeskWindowsExploer(iterm2Script)",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "e",
            "modifiers": {
              "mandatory": [
                "right_alt"
              ],
              "optional": [
                "caps_lock"
              ]
            }
          },
          "to": [
                 {
              "shell_command": "open -a 'Sublime Text 3.app'"
            },
            {
              "key_code": "e",
              "modifiers": "right_alt"
            }
          ]
        }
      ]
    }
    ]
}