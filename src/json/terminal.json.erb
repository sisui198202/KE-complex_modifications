{
  "title": "Terminal_ver1",
  "rules": [
{
  "description": "Leftcommand + R_miを開く(Terminal)",
  "manipulators": [
    {
      "type": "basic",
      "from": <%= from("r", ["left_control"]) %>,
      "to": <%= set_shell_command(["open -a 'mi'"]) %>
    }
  ]
},
{
  "description": "LeftAlt + J_chromeを開く(Terminal)",
  "manipulators": [
    {
      "type": "basic",
      "from": <%= from("j", ["left_alt"]) %>,
      "to": <%= set_shell_command(["open -a 'Google Chrome.app'"]) %>
    }
  ]
},
{
