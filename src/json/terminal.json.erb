{
  "title": "iterm2Script1",
  "rules": [
{
  "description": "Leftcommand + R_miActiveWindow(iterm2Script)",
  "manipulators": [
    {
      "type": "basic",
      "from": <%= from("r", ["left_control"], ["caps_lock"]) %>,
      "to": <%= set_shell_command(["open -a 'mi'"]) %>
    }
  ]
},
{
  "description": "LeftAlt + J_ChromeActiveWindow(iterm2Script)",
  "manipulators": [
    {
