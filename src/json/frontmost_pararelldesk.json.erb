{
    "title": "FrontmostIfPararell",
    "rules": [
    {
      "description": "保留_RightAlt + W_ActiveParellDeskWindows(Pararell)",
      "manipulators": [
        {
          "type": "basic",
          "from": <%= from("w", ["right_alt"], ["caps_lock"]) %>,
          "to": <%= set_shell_command(["open -a 'mi'"]) %>,
          "conditions": [ <%= frontmost_application_if("pararell") %> ]
        }
      ]
    }
   ]
}