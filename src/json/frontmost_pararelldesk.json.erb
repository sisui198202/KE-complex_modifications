{
    "title": "FrontmostIfPararell97",
    "rules": [
    {
      "description": "使用しない設定済みLeftCommand + D_SelectAll(Pararell)",
      "manipulators": [
        {
          "type": "basic",
          "from": <%= from("d", ["left_control"], ["caps_lock"]) %>,
          "to": <%= to([["a", ["left_control"]]]) %>,
          "conditions": [ <%= frontmost_application_if("pararell") %> ]
        }
      ]
    },
{
      "description": "LeftCommand + Quote_Single(Pararell)",
      "manipulators": [
        {
          "type": "basic",
          "from": <%= from("quote", ["left_control"], ["caps_lock"]) %>,
          "to": <%= to([["1", ["right_alt", "left_shift"]]]) %>,
          "conditions": [ <%= frontmost_application_if("pararell") %> ]
        }
      ]
    },
{
                        "description": "コマンドキーを単体で押したときに、英数・かなキーを送信する。（左コマンドキーは英数、右コマンドキーはかな）(Pararell)",
                        "manipulators": [
                            {
                                "from": {
                                    "key_code": "left_control",
                                    "modifiers": {
                                      "optional": [
                                        "caps_lock"
                                      ]
                                    }
                                },
                                "to": [
                                    {
                                        "key_code": "left_control",
                                        "lazy": true
                                    }
                                ],
                                "to_if_alone": [
            {
              "key_code": "f12",
              "modifiers": [
                "left_control"
              ]
            }
          ],
"conditions": [
            {
              "type": "frontmost_application_if",
              "bundle_identifiers": [
                "^com.parallels.winapp.7d56d4002964a683f165ae699d4ad1ba.dc4dfc2676c848d5bb27640140167936$"
              ]
            }
          ],
                                "type": "basic"
                            },
                            {
                                "from": {
                                    "key_code": "right_command",
                                    "modifiers": {
                                      "optional": [
                                        "caps_lock"
                                      ]
                                    }
                                },
                                "to": [
                                    {
                                        "key_code": "right_command",
                                        "lazy": true
                                    }
                                ],
                                "to_if_alone": [
            {
              "key_code": "f9",
              "modifiers": [
                "left_control"
              ]
            }
          ],
"conditions": [
            {
              "type": "frontmost_application_if",
              "bundle_identifiers": [
                "^com.parallels.winapp.7d56d4002964a683f165ae699d4ad1ba.dc4dfc2676c848d5bb27640140167936$"
              ]
            }
          ],
                                "type": "basic"
                            }
                        ]
},
{
      "description": "LeftAlt_Copy(Pararell)",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "left_alt",
            "modifiers": {
              "optional": [
                "caps_lock"
              ]
            }
          },
          "to": [
            {
              "key_code": "left_alt"
            }
          ],
          "to_if_alone": [
            {
              "key_code": "c",
              "modifiers": [
                "left_gui"
              ]
            }
],
"conditions": [
            {
              "type": "frontmost_application_if",
              "bundle_identifiers": [
                "^com.parallels.winapp.7d56d4002964a683f165ae699d4ad1ba.dc4dfc2676c848d5bb27640140167936$"
              ]
            }
          ]
        }
      ]
    },
{
      "description": "LeftCommand + i_Copy(Pararell)",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "i",
            "modifiers": {
              "mandatory": [
                "left_control"
              ],
              "optional": [
                "caps_lock"
              ]
            }
          },
          "to": [
            {
              "key_code": "c",
              "modifiers": [
                "right_gui"
              ]
            }
          ],
"conditions": [
            {
              "type": "frontmost_application_if",
              "bundle_identifiers": [
                "^com.parallels.winapp.7d56d4002964a683f165ae699d4ad1ba.dc4dfc2676c848d5bb27640140167936$"
              ]
            }
          ]
        }
      ]
    },
{
      "description": "RightAlt_Paste(Pararell)",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "right_alt",
            "modifiers": {
              "optional": [
                "caps_lock"
              ]
            }
          },
          "to": [
            {
              "key_code": "left_alt"
            }
          ],
          "to_if_alone": [
            {
              "key_code": "v",
              "modifiers": [
                "right_gui"
              ]
            }
],
"conditions": [
            {
              "type": "frontmost_application_if",
              "bundle_identifiers": [
                "^com.parallels.winapp.7d56d4002964a683f165ae699d4ad1ba.dc4dfc2676c848d5bb27640140167936$"
              ]
            }
          ]
        }
      ]
    }
    ]
}