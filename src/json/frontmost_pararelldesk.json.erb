{
    "title": "FrontmostIfVirtual_Pararelldesk_Single2",
    "rules": [
    {
      "description": "使用しない-LeftCommand + R_メモ帳アクティブ(VirtualMachine)",
      "manipulators": [
        {
      "type": "basic",
      "from": <%= from("r", ["left_control"], ["caps_lock"]) %>,
      "to": <%= to([["0", "left_option"]]) %>,
      "conditions": [ <%= frontmost_application_if("virtual_machine") %>]
      }
      ]
    },
    {
      "description": "うまくいかない-LeftShift + ;_colon(VirtualMachine)",
      "manipulators": [
        {
          "type": "basic",
          "from": <%= from("semicolon", ["left_shift"], ["caps_lock"]) %>,
          "to": <%= to([["semicolon", "left_shift"]]) %>,
          "conditions": [ <%= frontmost_application_if("virtual_machine") %>]
        }
      ]
    },
   {
      "description": "アクセスキーできない_fn_alt(VirtualMachine)",
      "manipulators": [
        {
          "type": "basic",
          "from": <%= from("right_control") %>,
          "to": <%= to([["left_option"]]) %>,
          "conditions": [ <%= frontmost_application_if("virtual_machine") %> ]
        }
      ]
    },
    {
      "description": "検討中_LeftAlt_Copy(VirtualMachine)",
      "_comment": "エクスプローラー_bf73",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "left_alt"
          },
          "to": [
            {
              "key_code": "left_alt"
            }
          ],
          "to_if_alone": [
            {
              "key_code": "japanese_eisuu"
            },
            {
              "key_code": "c",
              "modifiers": "right_gui"
            }
          ],
"conditions": [
            {
              "type": "frontmost_application_if",
              "bundle_identifiers": [
    "^com\.vmware\.fusion$",
    "^com\.vmware\.horizon$",
    "^com\.vmware\.view$",
    "^com\.parallels\.desktop$",
    "^com\.parallels\.vm$",
    "^com\.parallels\.desktop\.console$",
    "^org\.virtualbox\.app\.VirtualBoxVM$",
    "^com\.vmware\.proxyApp\.",
    "^com\.parallels\.winapp\."
              ]
            }
          ]
        }
      ]
    },
{
  "description": "使用していない_LeftOption + J_ChromeActiveWindow(VirtualMachine)",
  "manipulators": [
    {
      "type": "basic",
      "from": <%= from("j", ["left_alt"], ["caps_lock"]) %>,
      "to": <%= to([["4", ["left_option"]]]) %>,
      "conditions": [ <%= frontmost_application_if("virtual_machine") %> ]
    }
  ]
},
  {
      "description": "うまくいかないF8_StepIn(VirtualMachine)",
      "manipulators": [
        {
          "type": "basic",
          "from": <%= from("f8", ["caps_lock"]) %>,
          "to": <%= to([["9", ["right_option"]]]) %>,
          "conditions": [ <%= frontmost_application_if("virtual_machine") %> ]
        }
      ]
    },
      {
        "description": "うまくいかないfn_Rename(VirtualMachine)",
        "manipulators": [
          {
            "type": "basic",
            "from": <%= from("fn", ["caps_lock"]) %>,
            "to": <%= to([["f2"]]) %>,
            "conditions": [ <%= frontmost_application_if("virtual_machine") %>]
          }
        ]
      },
{
        "description": "AccessのVBEで反応しない_RightAlt + V_Paste(VirtualMachine)VBE",
        "manipulators": [
          {
            "type": "basic",
            "from": <%= from("v", ["right_alt"], ["caps_lock"]) %>,
            "to": <%= to([["v", "right_control"]]) %>,
            "conditions": [ <%= frontmost_application_if("virtual_machine") %> ]
          }
        ]
  }
    ]
}