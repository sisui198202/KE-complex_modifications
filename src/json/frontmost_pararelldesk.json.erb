{
    "title": "FrontmostIfPararell",
    "rules": [
    {
      "description": "使用しない設定済みLeftCommand + D_SelectAll(Pararell)",
      "manipulators": [
        {
          "type": "basic",
          "from": <%= from("d", ["left_control"], ["caps_lock"]) %>,
          "to": <%= to([["a", ["left_control"]]]) %>,
          "conditions": [ <%= frontmost_application_if("pararell") %> ]
        }
      ]
    },
{
      "description": "使用しない設定済みLeftCommand + Quote_Single(Pararell)",
      "manipulators": [
        {
          "type": "basic",
          "from": <%= from("quote", ["left_control"], ["caps_lock"]) %>,
          "to": <%= to([["1", ["right_alt", "left_shift"]]]) %>,
          "conditions": [ <%= frontmost_application_if("pararell") %> ]
        }
      ]
    },
{
      "description": "LeftCommand + Z_ RowCursolNextline(Pararell)",
      "manipulators": [
        {
          "type": "basic",
          "from": <%= from("z", ["left_control"], ["caps_lock"]) %>,
          "to": <%= to([["z", ["right_control", "left_alt"]]]) %>,
          "conditions": [ <%= frontmost_application_if("pararell") %> ]
        }
      ]
    },
{
      "description": "使用しないLeftCommand + W_ CurrentCursol-EndRowSelect(Pararell)",
      "manipulators": [
        {
          "type": "basic",
          "from": <%= from("w", ["left_control"], ["caps_lock"]) %>,
          "to": <%= to([["down_arrow", ["left_shift"]], ["left_arrow", "left_shift"], ["left_arrow", "left_shift"]]) %>,
          "conditions": [ <%= frontmost_application_if("pararell") %> ]
        }
      ]
    },
