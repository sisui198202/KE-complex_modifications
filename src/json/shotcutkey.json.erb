{
  "title": "ShotCutKey98",
  "rules": [
  {
      "description": "使用しない_LeftCommand + Period_AppsSelect-next",
      "manipulators": [
        {
          "type": "basic",
          "from": <%= from("period", ["left_control"], ["caps_lock"]) %>,
          "to": <%= to([["tab", "right_gui"]]) %>,
          "conditions": [ <%= frontmost_application_unless("pararell") %> ]
        }
      ]
    },
    {
      "description": "RightCommand + J_1RowSelection_LeftArrow",
      "manipulators": [
        {
          "type": "basic",
          "from": <%= from("j", ["right_gui"], ["caps_lock"]) %>,
          "to": <%= to([["a", ["left_control"]], ["down_arrow", "left_shift"], ["left_arrow", "left_shift"]]) %>
        }
      ]
    },
    {
      "description": "LeftCommand + LeftShift_/MacWindowShare/",
      "manipulators": [
        {
          "type": "basic",
          "from": <%= from("left_shift", ["left_control"], ["caps_lock"]) %>,
          "to": <%= set_shell_command(["open -a 'Path Finder.app' '/Users/user/MacWindowShare/img/'"]) %>,
          "conditions": [ <%= frontmost_application_unless("virtual_machine") %> ]
        }
      ]
    },
    {
      "description": "使用しない_RightAlt + X_AppWindowSelect",
      "manipulators": [
        {
          "type": "basic",
          "from": <%= from("x", ["right_alt"], ["caps_lock"]) %>,
          "to": <%= to([["grave_accent_and_tilde", ["right_gui"]]]) %>,
          "conditions": [ <%= frontmost_application_unless("pararell") %> ]
        }
      ]
    }
  ]
}