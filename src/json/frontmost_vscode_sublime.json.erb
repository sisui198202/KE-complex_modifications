{
    "title": "FrontmostIfVscodeSublime",
    "rules": [
    {
      "description": "LeftCommand + Z_Insert Line Below(vscode_sublime)",
      "manipulators": [
        {
          "type": "basic",
          "from": <%= from("z", ["left_control"], ["caps_lock"]) %>,
          "to": <%= to([["return_or_enter", ["right_gui"]]]) %>,
          "conditions": [ <%= frontmost_application_if("vscode_sublime") %> ]
        }
      ]
    },
        {
      "description": "RightAlt + Insert Line Above(vscode_sublime)",
      "manipulators": [
        {
          "type": "basic",
          "from": <%= from("z", ["right_alt"], ["caps_lock"]) %>,
          "to": <%= to([["return_or_enter", ["right_gui", "left_shift"]]]) %>,
          "conditions": [ <%= frontmost_application_if("vscode_sublime") %> ]
        }
      ]
    },
  {
      "description": "RightCommand + O_SameWordExpandSelection(vscode_sublime)",
      "manipulators": [
        {
          "type": "basic",
          "from": <%= from("o", ["right_gui"], ["caps_lock"]) %>,
          "to": <%= to([["0", ["right_gui", "right_alt"]]]) %>,
          "conditions": [ <%= frontmost_application_if("vscode_sublime") %> ]
        }
      ]
    },
  {
      "description": "LeftCommand + O_Column2SplitEditerScreen(vscode_sublime)",
      "manipulators": [
        {
          "type": "basic",
          "from": <%= from("o", ["left_control"], ["caps_lock"]) %>,
          "to": <%= to([["o", ["left_control", "left_shift"]], ["i", ["right_gui", "right_alt"]], ["n", ["right_gui"]]]) %>,
          "conditions": [ <%= frontmost_application_if("vscode_sublime") %> ]
        }
      ]
    },
  {
      "description": "RightCommand + I_FocusNeighboringGroup(vscode_sublime)",
      "manipulators": [
        {
          "type": "basic",
          "from": <%= from("i", ["right_gui"], ["any"]) %>,
          "to": <%= to([["i", ["right_gui", "right_alt"]]]) %>,
          "conditions": [ <%= frontmost_application_if("vscode_sublime") %> ]
        }
      ]
    },
  {
      "description": "LeftCommand + Quote_Single Column Editor Layout(vscode_sublime)",
      "manipulators": [
        {
          "type": "basic",
          "from": <%= from("quote", ["left_control"], ["caps_lock"]) %>,
          "to": <%= to([["h", ["right_control", "left_shift"]]]) %>,
          "conditions": [ <%= frontmost_application_if("vscode_sublime") %> ]
        }
      ]
    }
    ]
}