{
    "title": "FrontmostIfVscodeSublime1",
    "rules": [
    {
      "description": "LeftCommand + Z_RowCursolNewline(vscode_sublime)",
      "manipulators": [
        {
          "type": "basic",
          "from": <%= from("z", ["left_control"]) %>,
          "to": <%= to([["return_or_enter", ["right_gui"]]]) %>,
          "conditions": [ <%= frontmost_application_if("vscode_sublime") %> ]
        }
      ]
    },
  {
      "description": "RightCommand + O_SameWordExpandSelection(vscode_sublime)",
      "manipulators": [
        {
          "type": "basic",
          "from": <%= from("o", ["right_gui"]) %>,
          "to": <%= to([["0", ["right_gui", "right_alt"]]]) %>
        }
      ]
    },
  {
      "description": "LeftCommand + O_2SplitEditerScreen(vscode_sublime)",
      "manipulators": [
        {
          "type": "basic",
          "from": <%= from("o", ["left_control"], ["any"]) %>,
          "to": <%= to([["o", ["left_control", "left_shift"]], ["i", ["right_gui", "right_alt"]], ["n", ["right_gui"]]]) %>
        }
      ]
    },
  {
      "description": "RightCommand + I_FocusNeighboringGroup(vscode_sublime)",
      "manipulators": [
        {
          "type": "basic",
          "from": <%= from("i", ["right_gui"], ["any"]) %>,
          "to": <%= to([["i", ["right_gui", "right_alt"]]]) %>
        }
      ]
    },
  {
      "description": "LeftCommand + Semicolon_1EditerScreen(vscode_sublime)",
      "manipulators": [
        {
          "type": "basic",
          "from": <%= from("semicolon", ["left_control"]) %>,
          "to": <%= to([["h", ["right_control", "left_shift"]]]) %>
        }
      ]
    },
  {
      "description": "LeftCommand + T_ToggleSidebarOnOff1(vscode_sublime)",
      "manipulators": [
        {
          "type": "basic",
          "from": <%= from("t", ["left_control"]) %>,
          "to": <%= to([["t", ["right_control", "left_shift"]]]) %>,
          "conditions": [ <%= frontmost_application_if("vscode") %> ]
        }
      ]
    },
    {
      "description": "X_LeftAlt + N_prettifyコード整形(vscode_sublime)",
      "manipulators": [
        {
          "type": "basic",
          "from": <%= from("n", ["left_alt"]) %>,
          "to": <%= to([["h", ["right_gui", "right_shift"]]]) %>,
          "conditions": [ <%= frontmost_application_if("sublime") %> ]
        }
      ]
    }
    ]
}