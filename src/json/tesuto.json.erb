{
    "title": "tesuto_case",
    "rules": [
        {
            "description": "Leftoption + Space to Enter(!Finder)_ver1",
            "type": "basic",
            "from": {
                "key_code": "print_screen"
            },
            "to": [
                {
                    "key_code": "fn"
                }
            ],
            "to_if_alone": [
                {
                    "shell_command": "screencapture ~/Desktop/screenshot-`date +%Y%m%d-%H%M%S`.png"
                }
            ]
        },
        {
            "type": "basic",
            "from": {
                "key_code": "p",
                "modifiers": {
                    "mandatory": [
                        "fn"
                    ]
                }
            },
            "to": [
                {
                    "shell_command": "screencapture -P ~/Desktop/screenshot-`date +%Y%m%d-%H%M%S`.png"
                }
            ]
        },
{
  "description": "無変換/英数 + 「スペース/かな」で「Return/Escape」",
  "manipulators": [
    {
      "type": "basic",
      "from": {
        "key_code": "spacebar",
        "modifiers": {
          "mandatory": [ "fn" ],
          "optional": [ "shift", "control", "caps_lock" ]
        }
      },
      "to": [
        { "key_code": "return_or_enter" }
      ]
    },
    {
      "type": "basic",
      "from": {
        "key_code": "international5",
        "modifiers": {
          "optional": [ "caps_lock" ]
        }
      },
      "to": [
        { "key_code": "fn" }
      ],
      "to_if_alone": [
        { "key_code": "japanese_eisuu" }
      ]
    }
  ]
}
    ]
}