{
    "title": "FrontmostIfFinder44",
    "rules": [
    {
      "description": "RightCommand + L_前のタブを表示(Finder)",
      "manipulators": [
        {
          "type": "basic",
          "from": <%= from("l", ["right_gui"]) %>,
          "to": <%= to([["1", ["left_shift","left_gui"]]]) %>,
          "conditions": [ <%= frontmost_application_if("finder") %> ]
        }
      ]
    },
    {
      "description": "RightCommand + セミコロン_次のタブを表示(Finder)",
      "manipulators": [
        {
          "type": "basic",
          "from": <%= from("semicolon", ["right_gui"]) %>,
          "to": <%= to([["2", ["left_shift","left_gui"]]]) %>,
          "conditions": [ <%= frontmost_application_if("finder") %> ]
        }
      ]
    },
    {
      "description": "LeftOption + M_GoogleDrive(Finder)",
      "manipulators": [
        {
          "type": "basic",
          "from": <%= from("m", ["left_option"]) %>,
          "to": <%= to([["3", ["left_shift","left_control"]]]) %>,
          "conditions": [ <%= frontmost_application_if("finder") %> ]
        }
      ]
    },
    {
      "description": "LeftOption + Z_最近の項目(Finder)",
      "manipulators": [
        {
          "type": "basic",
          "from": <%= from("z", ["left_control"]) %>,
          "to": <%= to([["4", ["left_shift","left_control"]]]) %>,
          "conditions": [ <%= frontmost_application_if("finder") %> ]
        }
      ]
    },
    {
      "description": "LeftOption + U_カラム(Finder)",
      "manipulators": [
        {
          "type": "basic",
          "from": <%= from("u", ["left_option"]) %>,
          "to": <%= to([["3", ["left_gui"]]]) %>,
          "conditions": [ <%= frontmost_application_if("finder") %> ]
        }
      ]
    },
    {
      "description": "LeftOption + i_リスト(Finder)",
      "manipulators": [
        {
          "type": "basic",
          "from": <%= from("i", ["left_option"]) %>,
          "to": <%= to([["2", ["left_gui"]]]) %>,
          "conditions": [ <%= frontmost_application_if("finder") %> ]
        }
      ]
    },
    {
      "description": "LeftCommand + オウ_デュアルモードの切替(Finder)",
      "manipulators": [
        {
          "type": "basic",
          "from": <%= from("o", ["left_control"]) %>,
          "to": <%= to([["u", ["left_gui"]]]) %>,
          "conditions": [ <%= frontmost_application_if("finder") %> ]
        }
      ]
    },
    {
      "description": "LeftCommand + T_サイドバー切替(Finder)",
      "manipulators": [
        {
          "type": "basic",
          "from": <%= from("t", ["left_control"]) %>,
          "to": <%= to([["s", ["left_gui","left_control"]]]) %>,
          "conditions": [ <%= frontmost_application_if("finder") %> ]
        }
      ]
    },
    {
      "description": "RightCommand + A_新規タブ作成(Finder)",
      "manipulators": [
        {
          "type": "basic",
          "from": <%= from("a", ["right_gui"]) %>,
          "to": <%= to([["t", ["left_shift","left_control"]]]) %>,
          "conditions": [ <%= frontmost_application_if("finder") %> ]
        }
      ]
    },
    {
      "description": "RightCommand + i_アクティブなパネルの切替(Finder)",
      "manipulators": [
        {
          "type": "basic",
          "from": <%= from("i", ["right_gui"]) %>,
          "to": <%= to([["tab", ["left_option"]]]) %>,
          "conditions": [ <%= frontmost_application_if("finder") %> ]
        }
      ]
    },
    {
      "description": "LeftOption + D_Desktop(Finder)",
      "manipulators": [
        {
          "type": "basic",
          "from": <%= from("d", ["left_option"]) %>,
          "to": <%= to([["d", ["left_shift","left_gui"]]]) %>,
          "conditions": [ <%= frontmost_application_if("finder") %> ]
        }
      ]
    },
   {
      "description": "F2_Rename(Finder)",
      "manipulators": [
        {
          "type": "basic",
          "from": <%= from("f2") %>,
          "to": <%= to([["return_or_enter"]]) %>,
          "conditions": [ <%= frontmost_application_if("finder") %> ]
        }
      ]
    },
    {
      "description": "RightCommand + Space_FileDelete(Finder)",
      "manipulators": [
        {
          "type": "basic",
          "from": <%= from("spacebar", ["right_gui"]) %>,
          "to": <%= to([["delete_or_backspace", ["left_gui"]]]) %>,
          "conditions": [ <%= frontmost_application_if("finder") %> ]
        }
      ]
    },
    {
      "description": "LeftOption + SpaceBar_FileOpen(Finder)",
      "manipulators": [
        {
          "type": "basic",
          "from": <%= from("spacebar", ["left_option"]) %>,
          "to": <%= to([["o", ["right_gui"]]]) %>,
          "conditions": [ <%= frontmost_application_if("finder") %> ]
        }
      ]
    },
 {
      "description": "LeftAlt + A_FolderCreate(Finder)",
      "manipulators": [
        {
          "type": "basic",
          "from": <%= from("a", ["left_alt"]) %>,
          "to": <%= to([["n", ["right_gui","right_shift"]]]) %>,
          "conditions": [ <%= frontmost_application_if("finder") %> ]
        }
      ]
    },
  {
      "description": "RightCommand + A_FrontICollectionsFileCreate(Finder)",
      "manipulators": [
        {
          "type": "basic",
          "from": <%= from("a", ["right_gui"]) %>,
          "to": <%= set_shell_command(["touch '/Users/user/Desktop/iCollections/TmpFile/z.txt'"]) %>,
          "conditions": [ <%= frontmost_application_if("finder") %> ]
        }
      ]
    },
    {
      "description": "LeftControl_Rename(finder)",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "apple_vendor_top_case_key_code":"keyboard_fn"
          },
          "to": [
            {
              "key_code": "return_or_enter"
            }
          ],
          "conditions": [
            {
              "type": "frontmost_application_if",
              "bundle_identifiers": [
                "^com\\.apple\\.finder$"
              ]
            }
          ]
        }
      ]
    }
    ]
}