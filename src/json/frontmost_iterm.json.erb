      ]
    },
  {
      "description": "LeftAlt + c_ClearBuffer(iterm2)",
      "manipulators": [
        {
          "type": "basic",
          "from": <%= from("c", ["left_alt"]) %>,
          "to": <%= to([["k", ["right_gui"]]]) %>,
          "conditions": [ <%= frontmost_application_if("iterm2") %> ]
        }
      ]
    }
    ]
}