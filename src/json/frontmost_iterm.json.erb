{
    "title": "FrontmostIfIterm2",
    "rules": [
    {
      "description": "LeftCommand + O_SplitVertically(iterm2)",
      "manipulators": [
        {
          "type": "basic",
          "from": <%= from("o", ["left_control"]) %>,
          "to": <%= to([["d", ["right_gui"]]]) %>,
          "conditions": [ <%= frontmost_application_if("iterm2") %> ]
        }
      ]
    },
    {
      "description": "LeftAlt + 0_Horizontally(iterm2)",
      "manipulators": [
        {
          "type": "basic",
          "from": <%= from("0", ["left_alt"]) %>,
          "to": <%= to([["d", ["right_gui","left_shift"]]]) %>,
          "conditions": [ <%= frontmost_application_if("iterm2") %> ]
        }
      ]
    },
   {
      "description": "RightCommand + I_SelectNextPanel(iterm2)",
      "manipulators": [
        {
          "type": "basic",
          "from": <%= from("i", ["right_gui"]) %>,
