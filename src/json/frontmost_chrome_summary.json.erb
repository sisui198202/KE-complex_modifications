{
    "title": "FrontmostIfChrome_summary",
    "rules": [
      {
        "description": "FrontmostIfChrome_summary",
        "manipulators": [
          {
            "description": "RightCommand + i_検証-ページにフォーカス",
            "type": "basic",
            "from": <%= from("i", ["right_gui"], ["caps_lock"]) %>,
            "to": <%= to([["2", ["right_gui","right_shift"]]]) %>,
            "conditions": [ <%= frontmost_application_if("chrome") %> ]
          },
          {
            "description": "LeftAlt + N_検証-ノードのスクリーンショットをキャプチャ",
            "type": "basic",
            "from": <%= from("n", ["left_alt"], ["caps_lock"]) %>,
            "to": <%= to([["7", ["right_shift","right_gui"]]]) %>,
            "conditions": [ <%= frontmost_application_if("chrome") %> ]
          },
          {
            "description": "LeftOption + openbracket_検証-コマンドパレット",
            "type": "basic",
            "from": <%= from("open_bracket", ["left_option"], ["caps_lock"]) %>,
            "to": <%= to([["p", ["right_gui", "right_shift"]]]) %>,
            "conditions": [ <%= frontmost_application_if("chrome") %> ]
          },
          {
            "description": "LeftCommand + openbracket_検証-プロジェクト内に存在するファイルを指定して開く",
            "type": "basic",
            "from": <%= from("open_bracket", ["left_control"], ["caps_lock"]) %>,
            "to": <%= to([["p", ["right_gui"]]]) %>,
            "conditions": [ <%= frontmost_application_if("chrome") %> ]
          },
          {
            "description": "RightOption + L_検証-前のパネル",
            "type": "basic",
            "from": <%= from("l", ["right_option"], ["caps_lock"]) %>,
            "to": <%= to([["open_bracket", ["left_gui"]]]) %>,
            "conditions": [ <%= frontmost_application_unless("virtual_machine") %> ]
          },
          {
            "description": "RightOption + Semicolon_検証-次のパネル",
            "type": "basic",
            "from": <%= from("semicolon", ["right_option"], ["caps_lock"]) %>,
            "to": <%= to([["close_bracket", ["left_gui"]]]) %>,
            "conditions": [ <%= frontmost_application_unless("virtual_machine") %> ]
          },
          {
            "description": "LeftAlt + W_検証-コンソールを消去",
            "type": "basic",
            "from": <%= from("w", ["left_alt"], ["caps_lock"]) %>,
            "to": <%= to([["1", ["left_control"]]]) %>,
            "conditions": [ <%= frontmost_application_if("chrome") %> ]
          },
          {
            "description": "LeftAlt + C_タブを複製",
            "type": "basic",
            "from": <%= from("c", ["left_alt"], ["caps_lock"]) %>,
            "to": <%= to([["f12", ["right_shift"]]]) %>,
            "conditions": [ <%= frontmost_application_if("chrome") %> ]
          },
          {
            "description": "A_新しいタブを開く",
            "type": "basic",
            "from": <%= from("a", ["caps_lock"]) %>,
            "to": <%= to([["n", ["right_gui"]]]) %>,
            "conditions": [ <%= frontmost_application_if("chrome") %> ]
          },
          {
            "description": "W_新しいウィンドウで開く",
            "type": "basic",
            "from": <%= from("w", ["caps_lock"]) %>,
            "to": <%= to([["a", ["left_option"]]]) %>,
            "conditions": [ <%= frontmost_application_if("chrome") %> ]
          },
          {
            "description": "C_タブを複製",
            "type": "basic",
            "from": <%= from("c", ["caps_lock"]) %>,
            "to": <%= to([["f12", ["right_shift"]]]) %>,
            "conditions": [ <%= frontmost_application_if("chrome") %> ]
          },
          {
            "description": "E_Reload",
            "type": "basic",
            "from": <%= from("e", ["caps_lock"]) %>,
            "to": <%= to([["w", ["right_control", "right_shift"]]]) %>,
            "conditions": [ <%= frontmost_application_if("chrome") %> ]
          },
          {
            "description": "X_BookMarkSearch",
            "type": "basic",
            "from": <%= from("x", ["caps_lock"]) %>,
            "to": <%= to([["period", "left_option"]]) %>,
            "conditions": [ <%= frontmost_application_if("chrome") %> ]
          },
          {
            "description": "RightCommand + quote_閉じたタブを再度開く",
            "type": "basic",
            "from": <%= from("quote", ["right_gui"], ["caps_lock"]) %>,
            "to": <%= to([["t", ["right_gui", "right_shift"]]]) %>,
            "conditions": [ <%= frontmost_application_if("chrome") %> ]
          },
        {
          "description": "RightOption + T_BookMarkSearch",
          "type": "basic",
          "from": <%= from("t", ["right_alt"], ["caps_lock"]) %>,
          "to": <%= to([["period", "left_option"]]) %>,
          "conditions": [ <%= frontmost_application_if("chrome") %> ]
        },
        {
          "description": "RightCommand + H_Reload",
          "type": "basic",
          "from": <%= from("h", ["right_gui"], ["caps_lock"]) %>,
          "to": <%= to([["w", ["right_control", "right_shift"]]]) %>,
          "conditions": [ <%= frontmost_application_if("chrome") %> ]
        },
        {
          "description": "LeftShift + RightCommand + H_ハードキャッシュ",
          "type": "basic",
          "from": <%= from("h", ["left_shift", "right_gui"], ["caps_lock"]) %>,
          "to": <%= to([["delete_or_backspace", ["right_gui", "right_shift"]]]) %>,
          "conditions": [ <%= frontmost_application_if("chrome") %> ]
        },
        {
          "description": "LeftAlt + E_NotionWebCliper起動",
          "type": "basic",
          "from": <%= from("e", ["left_alt"], ["caps_lock"]) %>,
          "to": <%= to([["1", ["right_gui"]]]) %>,
          "conditions": [ <%= frontmost_application_if("chrome") %> ]
        },
        {
          "description": "RightAlt + A_タブを新しいウィンドウに移動",
          "type": "basic",
          "from": <%= from("a", ["right_alt"], ["caps_lock"]) %>,
          "to": <%= to([["hyphen", ["right_control", "right_alt"]]]) %>,
          "conditions": [ <%= frontmost_application_if("chrome") %> ]
        },
        {
          "description": "LeftAlt + A_シークレットウィンドウを開く",
          "type": "basic",
          "from": <%= from("a", ["left_alt"], ["caps_lock"]) %>,
          "to": <%= to([["n", ["right_shift", "right_gui"]]]) %>,
          "conditions": [ <%= frontmost_application_if("chrome") %> ]
        },
        {
          "description": "LeftAlt + B_BookmarkManage",
          "type": "basic",
          "from": <%= from("b", ["left_alt"], ["caps_lock"]) %>,
          "to": <%= to([["b", ["right_option", "right_gui"]]]) %>,
          "conditions": [ <%= frontmost_application_if("chrome") %> ]
        },
        {
          "description": "LeftAlt + F_Title & URL Picker",
          "type": "basic",
          "from": <%= from("f", ["left_option"], ["caps_lock"]) %>,
          "to": <%= to([["9",["right_shift", "right_gui"]]]) %>,
          "conditions": [ <%= frontmost_application_if("chrome") %> ]
        }
        ]
      }
    ]
}