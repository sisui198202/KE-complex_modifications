{
    "title": "FrontmostIfChrome_summary",
    "rules": [
      {
        "description": "FrontmostIfChrome_summary",
        "manipulators": [
          {
            "description": "LeftAlt + C_タブを複製",
            "type": "basic",
            "from": <%= from("c", ["left_alt"], ["caps_lock"]) %>,
            "to": <%= to([["f12", ["right_shift"]]]) %>,
            "conditions": [ <%= frontmost_application_if("chrome") %> ]
          }
        ]
      }
    ]
}