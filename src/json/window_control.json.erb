{
  "title": "WindowControl97",
  "rules": [
    {
      "description": "WindowsControl97",
      "manipulators": [
    {
      "description": "LeftControl + U_WindowLeftUp",
      "type": "basic",
      "from": <%= from("u", ["fn"], ["caps_lock"]) %>,
      "to": <%= to([["6", "right_alt"]]) %>
    },
    {
        "description": "LeftControl + J_WindowLeftDown",
        "type": "basic",
        "from": <%= from("j", ["fn"], ["caps_lock"]) %>,
        "to": <%= to([["8", "right_alt"]]) %>
    },
    {
        "description": "LeftControl + I_WindowRightUp",
        "type": "basic",
        "from": <%= from("i", ["fn"], ["caps_lock"]) %>,
        "to": <%= to([["7", "right_alt"]]) %>
    },
    {
        "description": "LeftControl + K_WindowRightDown",
        "type": "basic",
        "from": <%= from("k", ["fn"], ["caps_lock"]) %>,
        "to": <%= to([["9", "right_alt"]]) %>
    },
    {
        "description": "LeftControl + G_WindowLeftOneThird",
        "type": "basic",
        "from": <%= from("g", ["fn"], ["caps_lock"]) %>,
        "to": <%= to([["d", ["right_control", "right_alt"]]]) %>
    },
    {
        "description": "LeftControl + Quote_WindowRightOneThird",
        "type": "basic",
        "from": <%= from("quote", ["fn"], ["caps_lock"]) %>,
        "to": <%= to([["g", ["right_control", "right_alt"]]]) %>
    },
    {
        "description": "LeftControl + Y_WindowLeftTwo-thirds",
        "type": "basic",
        "from": <%= from("y", ["fn"], ["caps_lock"]) %>,
        "to": <%= to([["e", ["right_control", "right_alt"]]]) %>
    },
    {
        "description": "LeftControl + O_WindowRightTwo-thirds",
        "type": "basic",
        "from": <%= from("o", ["fn"], ["caps_lock"]) %>,
        "to": <%= to([["t", ["right_control", "right_alt"]]]) %>
    },
    {
        "description": "LeftControl + Comma_WindowCenter",
        "type": "basic",
        "from": <%= from("comma", ["fn"], ["caps_lock"]) %>,
        "to": <%= to([["c", ["right_control", "right_alt"]]]) %>
    },
    {
      "description": "RightCommand + Period_MaxWindow",
      "type": "basic",
      "from": <%= from("period", ["right_gui"], ["caps_lock"]) %>,
      "to": <%= to([["f", ["right_control", "right_gui"]]]) %>,
      "conditions": [ <%= frontmost_application_unless("virtual_machine") %> ]
    },
    {
        "description": "LeftControl + N_WindowUP",
        "type": "basic",
        "from": <%= from("n", ["fn"], ["caps_lock"]) %>,
        "to": <%= to([["4", ["left_alt"]]]) %>
    },
    {
        "description": "LeftControl + M_WindowDown",
        "type": "basic",
        "from": <%= from("m", ["fn"], ["caps_lock"]) %>,
        "to": <%= to([["5", ["left_alt"]]]) %>
    },
    {
      "description": "RightCommand + P_RightHalfWindowMove",
      "type": "basic",
      "from": <%= from("p", ["right_gui"], ["caps_lock"]) %>,
      "to": <%= to([["3", ["right_alt"]]]) %>,
      "conditions": [ <%= frontmost_application_unless("virtual_machine") %> ]
    },
    {
      "description": "LeftCommand + Q_LeftHalfWindowMove",
      "type": "basic",
      "from": <%= from("q", ["left_control"], ["caps_lock"]) %>,
      "to": <%= to([["2", ["right_alt"]]]) %>,
      "conditions": [ <%= frontmost_application_unless("virtual_machine") %> ]
    },
    {
      "description": "LeftControl + L_RightHalfWindowMove",
      "type": "basic",
      "from": <%= from("l", ["fn"], ["caps_lock"]) %>,
      "to": <%= to([["3", ["right_alt"]]]) %>
    },
    {
      "description": "LeftControl + H_LeftWindowMove",
      "type": "basic",
      "from": <%= from("h", ["fn"], ["caps_lock"]) %>,
      "to": <%= to([["2", ["right_alt"]]]) %>
    },
    {
      "description": "Leftcommand + R_miActiveWindow(WindowControl)",
      "type": "basic",
      "from": <%= from("r", ["left_control"], ["caps_lock"]) %>,
      "to": <%= set_shell_command(["open -a 'mi'"]) %>
    },
    {
      "description": "LeftOption + J_ChromeActiveWindow(WindowControl)",
      "type": "basic",
      "from": <%= from("j", ["left_alt"], ["caps_lock"]) %>,
      "to": <%= set_shell_command(["open -a 'Google Chrome.app'"]) %>,
      "conditions": [ <%= frontmost_application_unless("virtual_machine") %> ]
    },
    {
      "description": "LeftOption + K_VscodeActiveWindow(WindowControl)",
      "type": "basic",
      "from": <%= from("k", ["left_alt"], ["caps_lock"]) %>,
      "to": <%= set_shell_command(["open -a 'Visual Studio Code.app'"]) %>,
      "conditions": [ <%= frontmost_application_unless("virtual_machine") %> ]
    },
    {
      "description": "LeftOption + Semicolon_PathfinderActiveWindow(WindowControl)",
      "type": "basic",
      "from": <%= from("semicolon", ["left_alt"], ["caps_lock"]) %>,
      "to": <%= set_shell_command(["open -a 'Path Finder.app'"]) %>,
      "conditions": [ <%= frontmost_application_unless("virtual_machine") %> ]
    },
    {
      "description": "LeftCommand + X_iterm2ActiveWindow(WindowControl)",
      "type": "basic",
      "from": <%= from("x", ["left_control"], ["caps_lock"]) %>,
      "to": <%= set_shell_command(["open -a 'iTerm.app'"]) %>,
      "conditions": [ <%= frontmost_application_unless("virtual_machine") %> ]
    },
    {
      "description": "LeftOption + H_ExcelActiveWindow(WindowControl)",
      "type": "basic",
      "from": <%= from("h", ["left_alt"], ["caps_lock"]) %>,
      "to": <%= set_shell_command(["open -a 'Microsoft Excel.app'"]) %>,
      "conditions": [ <%= frontmost_application_unless("virtual_machine") %> ]
    },
    {
      "description": "LeftOption + Quote_PreviewActiveWindow(WindowControl)",
      "type": "basic",
      "from": <%= from("quote", ["left_alt"], ["caps_lock"]) %>,
      "to": <%= set_shell_command(["open -a 'Preview.app'"]) %>,
      "conditions": [ <%= frontmost_application_unless("virtual_machine") %> ]
    },
    {
      "description": "RightOption + G_VivaldiActiveWindow(WindowControl)",
      "type": "basic",
      "from": <%= from("g", ["right_alt"], ["caps_lock"]) %>,
      "to": <%= set_shell_command(["open -a 'Vivaldi.app'"]) %>,
      "conditions": [ <%= frontmost_application_unless("virtual_machine") %>]
    },
    {
      "description": "RightOption + L_AppleMusicActiveWindow(WindowControl)",
      "type": "basic",
      "from": <%= from("l", ["right_alt"], ["caps_lock"]) %>,
      "to": <%= set_shell_command(["open -a 'Music.app'"]) %>,
      "conditions": [ <%= frontmost_application_unless("virtual_machine") %>]
    },
    {
      "description": "LeftOption + slash_FinderActiveWindow(WindowControl)",
      "type": "basic",
      "from": <%= from("slash", ["left_alt"], ["caps_lock"]) %>,
      "to": <%= set_shell_command(["open -a 'finder.app'"]) %>,
      "conditions": [ <%= frontmost_application_unless("virtual_machine") %>]
    },
    {
      "description": "LeftOption + L_KindleActiveWindow(WindowControl)",
      "type": "basic",
      "from": <%= from("l", ["left_alt"], ["caps_lock"]) %>,
      "to": <%= set_shell_command(["open -a 'Kindle.app'"]) %>
    },
        {
          "description": "RightOption + E_ActiveParellDeskWindowsExploer(WindowControl)",
          "type": "basic",
          "from": <%= from("e", ["right_alt"], ["caps_lock"]) %>,
          "to": <%= set_shell_command(["open -a '/Users/user/Applications (Parallels)/{b9dc48ef-d509-4ca8-a69c-0e98f2ecf717} Applications.localized/エクスプローラー.app/Contents/MacOS/WinAppHelper'"]) %>,
          "conditions": [ <%= frontmost_application_unless("virtual_machine") %>]
        },
        {
          "description": "RightOption + I_ActiveMacWindowShare/img/pdf(WindowControl)",
          "type": "basic",
          "from": <%= from("i", ["right_option"], ["caps_lock"]) %>,
          "to": <%= set_shell_command(["open -a 'Path Finder.app' '/Users/user/MacWindowShare/img/pdf'"]) %>,
          "conditions": [ <%= frontmost_application_unless("virtual_machine") %>]
        },
        {
          "description": "RightOption + Q_ActiveParellDeskWindowsExploerDesktop(WindowControl)",
          "type": "basic",
          "from": {
            "key_code": "q",
            "modifiers": {
              "mandatory": [
                "right_alt"
              ],
              "optional": [
                "caps_lock"
              ]
            }
          },
          "to": [
                 {
                  "shell_command": "open -a '/Users/user/Applications (Parallels)/{b9dc48ef-d509-4ca8-a69c-0e98f2ecf717} Applications.localized/エクスプローラー.app/Contents/MacOS/WinAppHelper'"
            },
            {
              "key_code": "c",
              "modifiers": "left_control"
            }
          ],
          "conditions": [
            {
              "type": "frontmost_application_unless",
              "bundle_identifiers": [
                "^com\\.vmware\\.fusion$",
                "^com\\.vmware\\.horizon$",
                "^com\\.vmware\\.view$",
                "^com\\.parallels\\.desktop$",
                "^com\\.parallels\\.vm$",
                "^com\\.parallels\\.desktop\\.console$",
                "^org\\.virtualbox\\.app\\.VirtualBoxVM$",
                "^com\\.vmware\\.proxyApp\\.",
                "^com\\.parallels\\.winapp\\."
              ]
            }
          ]
        },
        {
          "description": "RightOption + W_ActiveParellDeskSublime(WindowControl)",
          "type": "basic",
          "from": <%= from("w", ["right_alt"], ["caps_lock"]) %>,
          "to": <%= set_shell_command(["open -a '/Users/user/Applications (Parallels)/{b9dc48ef-d509-4ca8-a69c-0e98f2ecf717} Applications.localized/Sublime Text 3.app/Contents/MacOS/WinAppHelper'"]) %>,
          "conditions": [ <%= frontmost_application_unless("virtual_machine") %> ]
        }
      ]
    }
  ]
}