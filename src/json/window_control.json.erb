          "type": "basic",
          "from": <%= from("e", ["right_alt"], ["caps_lock"]) %>,
          "to": <%= set_shell_command(["open -a '/Users/user/Applications (Parallels)/{dc4dfc26-76c8-48d5-bb27-640140167936} Applications.localized/エクスプローラー.app/Contents/MacOS/WinAppHelper'"]) %>
