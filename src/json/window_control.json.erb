    },
    {
      "description": "Fn + H_LeftWindowMove",
      "type": "basic",
      "from": <%= from("h", ["fn"], ["caps_lock"]) %>,
      "to": <%= to([["2", ["right_alt"]]]) %>
    }
 ]
}
  ]
}