{
  "title": "WindowControl",
  "rules": [
    {
      "description": "Fn + ~_WindowsControl(WindowControl)",
      "manipulators": [
        {
          "description": "Fn + U_WindowLeftUp",
          "type": "basic",
          "from": <%= from("u", ["fn"], ["caps_lock"]) %>,
          "to": <%= to([["6", "right_alt"]]) %>
        },
    {
        "description": "Fn + J_WindowLeftDown",
        "type": "basic",
        "from": <%= from("j", ["fn"], ["caps_lock"]) %>,
        "to": <%= to([["8", "right_alt"]]) %>
    },
    {
        "description": "Fn + I_WindowRightUp",
        "type": "basic",
        "from": <%= from("i", ["fn"], ["caps_lock"]) %>,
        "to": <%= to([["7", "right_alt"]]) %>
    },
    {
        "description": "Fn + K_WindowRightDown",
        "type": "basic",
        "from": <%= from("k", ["fn"], ["caps_lock"]) %>,
        "to": <%= to([["9", "right_alt"]]) %>
    },
    {
        "description": "Fn + G_WindowLeftOneThird",
        "type": "basic",
        "from": <%= from("g", ["fn"], ["caps_lock"]) %>,
        "to": <%= to([["d", ["right_control", "right_alt"]]]) %>
    },
    {
        "description": "Fn + Quote_WindowRightOneThird",
        "type": "basic",
        "from": <%= from("quote", ["fn"], ["caps_lock"]) %>,
        "to": <%= to([["g", ["right_control", "right_alt"]]]) %>
    },
    {
        "description": "Fn + Y_WindowLeftTwo-thirds",
        "type": "basic",
        "from": <%= from("y", ["fn"], ["caps_lock"]) %>,
        "to": <%= to([["e", ["right_control", "right_alt"]]]) %>
    },
    {
        "description": "Fn + O_WindowRightTwo-thirds",
        "type": "basic",
        "from": <%= from("o", ["fn"], ["caps_lock"]) %>,
        "to": <%= to([["t", ["right_control", "right_alt"]]]) %>
    },
    {
        "description": "Fn + Comma_WindowCenter",
        "type": "basic",
        "from": <%= from("comma", ["fn"], ["caps_lock"]) %>,
        "to": <%= to([["c", ["right_control", "right_alt"]]]) %>
    },
    {
      "description": "RightCommand + Period_MaxWindow1",
      "type": "basic",
      "from": <%= from("period", ["right_gui"], ["caps_lock"]) %>,
      "to": <%= to([["f", ["right_control", "right_gui"]]]) %>,
      "conditions": [ <%= frontmost_application_unless("pararell") %> ]
    },
    {
        "description": "Fn + N_WindowUP",
        "type": "basic",
        "from": <%= from("n", ["fn"], ["caps_lock"]) %>,
        "to": <%= to([["4", ["left_alt"]]]) %>
    },
    {
        "description": "Fn + M_WindowDown",
        "type": "basic",
        "from": <%= from("m", ["fn"], ["caps_lock"]) %>,
        "to": <%= to([["5", ["left_alt"]]]) %>
    },
    {
      "description": "LeftOption + Q_WindowUP",
      "type": "basic",
      "from": <%= from("q", ["left_alt"], ["caps_lock"]) %>,
      "to": <%= to([["4", ["left_alt"]]]) %>,
      "conditions": [ <%= frontmost_application_unless("pararell") %> ]
    },
    {
      "description": "LeftOption + P_WindowDown",
      "type": "basic",
      "from": <%= from("p", ["left_alt"], ["caps_lock"]) %>,
      "to": <%= to([["5", ["left_alt"]]]) %>,
      "conditions": [ <%= frontmost_application_unless("pararell") %> ]
    },
    {
      "description": "LeftOption + W_WindowRightUp",
      "type": "basic",
      "from": <%= from("w", ["left_alt"], ["caps_lock"]) %>,
      "to": <%= to([["7", ["left_alt"]]]) %>,
      "conditions": [ <%= frontmost_application_unless("pararell") %> ]
    },
    {
      "description": "LeftOption + E_WindowRightDown",
      "type": "basic",
      "from": <%= from("e", ["left_alt"], ["caps_lock"]) %>,
      "to": <%= to([["9", ["left_alt"]]]) %>,
      "conditions": [ <%= frontmost_application_unless("pararell") %> ]
    },
    {
      "description": "RightCommand + P_RightHalfWindowMove",
      "type": "basic",
      "from": <%= from("p", ["right_gui"], ["caps_lock"]) %>,
      "to": <%= to([["3", ["right_alt"]]]) %>,
      "conditions": [ <%= frontmost_application_unless("pararell") %> ]
    },
    {
      "description": "LeftCommand + Q_LeftWindowMove",
      "type": "basic",
      "from": <%= from("q", ["left_control"], ["caps_lock"]) %>,
      "to": <%= to([["2", ["right_alt"]]]) %>,
      "conditions": [ <%= frontmost_application_unless("pararell") %> ]
    },
    {
      "description": "Fn + L_RightHalfWindowMove",
      "type": "basic",
      "from": <%= from("l", ["fn"], ["caps_lock"]) %>,
