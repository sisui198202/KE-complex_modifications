    {
      "description": "使用しない_RightOption + E_ActiveParellDeskWindowsExploer(iterm2Script)",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "e",
            "modifiers": {
              "mandatory": [
                "right_alt"
              ],
              "optional": [
                "caps_lock"
              ]
            }
          },
          "to": [
                 {
              "shell_command": "open -a 'Sublime Text 3.app'"
            },
            {
              "key_code": "e",
              "modifiers": "right_alt"
            }
          ]
        }
      ]
