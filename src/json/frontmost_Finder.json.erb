{
    "title": "FrontmostIfFinder",
    "rules": [
   {
      "description": "F2_Rename(Finder)",
      "manipulators": [
        {
          "type": "basic",
          "from": <%= from("f2") %>,
          "to": <%= to([["return_or_enter"]]) %>,
          "conditions": [ <%= frontmost_application_if("finder") %> ]
        }
      ]
    },
    {
      "description": "RightCommand + Space_FileDelete(Finder)",
      "manipulators": [
        {
          "type": "basic",
          "from": <%= from("spacebar", ["right_gui"]) %>,
          "to": <%= to([["delete_or_backspace", ["left_gui"]]]) %>,
          "conditions": [ <%= frontmost_application_if("finder") %> ]
        }
      ]
    },
    {
      "description": "LeftAlt + Space_OpenFile(Finder)",
      "manipulators": [
        {
          "type": "basic",
          "from": <%= from("spacebar", ["left_alt"]) %>,
          "to": <%= to([["button1"], ["button1"]]) %>,
          "conditions": [ <%= frontmost_application_if("finder") %> ]
        }
      ]
    },
    {
      "description": "Enter_OpenFile(Finder)",
      "manipulators": [
        {
          "type": "basic",
          "from": <%= from("return_or_enter") %>,
          "to": <%= to([["button1"], ["button1"]]) %>,
          "conditions": [ <%= frontmost_application_if("finder") %> ]
        }
      ]
    },
  {
      "description": "LeftAlt + A_FrontICollectionsFolderCreate(Finder)",
      "manipulators": [
        {
          "type": "basic",
          "from": <%= from("a", ["left_alt"]) %>,
          "to": <%= set_shell_command(["mkdir '/Users/user/Desktop/iCollections/TmpFolder/z'"]) %>,
          "conditions": [ <%= frontmost_application_if("finder") %> ]
        }
      ]
    },
  {
      "description": "RightCommand + A_FrontICollectionsFileCreate(Finder)",
      "manipulators": [
        {
          "type": "basic",
          "from": <%= from("a", ["right_gui"]) %>,
          "to": <%= set_shell_command(["touch '/Users/user/Desktop/iCollections/TmpFile/z.txt'"]) %>,
          "conditions": [ <%= frontmost_application_if("finder") %> ]
        }
      ]
    }
    ]
}