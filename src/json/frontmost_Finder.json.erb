{
    "title": "FrontmostIfFinder_ver1",
    "rules": [
        {
            "description": "LeftAlt + Space_OpenFile(Finder)_old",
            "manipulators": [
                {
                    "type": "basic",
                    "from": {
                        "key_code": "spacebar",
                        "modifiers": {
                            "mandatory": [
                                "left_alt"
                            ]
                        }
                    },
                    "to": [
                        {
                            "key_code": "o",
                            "modifiers": [
                                "right_gui"
                            ]
                        }
                    ],
                    "conditions": [
                        {
                            "type": "frontmost_application_if",
                            "bundle_identifiers": [
                                "^com\\.apple\\.finder"
                            ]
                        }
                    ]
                }
            ]
        },
    {
      "description": "LeftAlt + Space_OpenFile(Finder)",
      "manipulators": [
        {
          "type": "basic",
          "from": <%= from("spacebar", ["left_alt"]) %>,
          "to": <%= to([["o", ["right_gui"]]]) %>,
          "conditions": [ <%= frontmost_application_if("finder") %> ]
        }
      ]
    },
        {
            "description": "Enter_OpenFile(Finder)_old",
            "manipulators": [
                {
                    "type": "basic",
                    "from": {
                        "key_code": "return_or_enter"
                    },
                    "to": [
                        {
                            "key_code": "o",
                            "modifiers": [
                                "right_command"
                            ]
                        }
                    ],
                    "conditions": [
                        {
                            "type": "frontmost_application_if",
                            "bundle_identifiers": [
                                "^com\\.apple\\.finder"
                            ]
                        }
                    ]
                }
            ]
        },
    {
      "description": "Enter_OpenFile(Finder)",
      "manipulators": [
        {
          "type": "basic",
          "from": <%= from("return_or_enter") %>,
          "to": <%= to([["o", ["right_gui"]]]) %>,
          "conditions": [ <%= frontmost_application_if("finder") %> ]
        }
      ]
    },
        {
            "description": "F2_Rename(Finder)_old",
            "manipulators": [
                {
                    "type": "basic",
                    "from": {
                        "key_code": "f2"
                    },
                    "to": [
                        {
                            "key_code": "return_or_enter"
                        }
                    ],
                    "conditions": [
                        {
                            "type": "frontmost_application_if",
                            "bundle_identifiers": [
                                "^com\\.apple\\.finder"
                            ]
                        }
                    ]
                }
            ]
        },
   {
      "description": "F2_Rename(Finder)",
      "manipulators": [
        {
          "type": "basic",
          "from": <%= from("f2") %>,
          "to": <%= to([["return_or_enter"]]) %>,
          "conditions": [ <%= frontmost_application_if("finder") %> ]
        }
      ]
    },
        {
            "description": "RightCommand + space_FileDelete(Finder)_old",
            "manipulators": [
                {
                    "type": "basic",
                    "from": {
                      "key_code": "spacebar",
                      "modifiers": {
                        "mandatory": [
                          "right_gui"
                        ]
                      }
                    },
                    "to": [
                      {
                        "key_code": "delete_or_backspace",
                        "modifiers": [
                          "left_command"
                        ]
                      }
                    ],
                    "conditions": [
                        {
                            "type": "frontmost_application_if",
                            "bundle_identifiers": [
                                "^com\\.apple\\.finder"
                            ]
                        }
                    ]
                }
            ]
        },
    {
      "description": "RightCommand + space_FileDelete(Finder)",
      "manipulators": [
        {
          "type": "basic",
          "from": <%= from("spacebar", ["right_gui"]) %>,
          "to": <%= to([["delete_or_backspace", ["left_gui"]]]) %>,
          "conditions": [ <%= frontmost_application_if("finder") %> ]
        }
      ]
    }
    ]
}