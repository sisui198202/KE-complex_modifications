      "manipulators": [
        {
          "type": "basic",
          "from": <%= from("r", ["caps_lock"]) %>,
          "to": <%= set_shell_command(["open -a 'mi'"]) %>,
          "conditions": [ <%= frontmost_application_if("chrome") %> ]
        }
      ]
    },
   {
      "description": "P_RightHalfWindowMove(Chrome)",
