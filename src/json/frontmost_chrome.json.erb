{
    "title": "FrontmostIfChrome9",
    "rules": [
      {
        "description": "J_DownArrow(Chrome)",
        "manipulators": [
          {
            "type": "basic",
            "from": <%= from("j",["caps_lock"]) %>,
            "to": <%= to([["down_arrow"]]) %>,
            "conditions": [ <%= frontmost_application_if("chrome") %> ]
          }
        ]
      },
      {
        "description": "M_最終行へ(Chrome)",
        "manipulators": [
          {
            "type": "basic",
            "from": <%= from("m",["caps_lock"]) %>,
            "to": <%= to([["right_arrow", ["fn"]]]) %>,
            "conditions": [ <%= frontmost_application_if("chrome") %> ]
          }
        ]
      },
      {
        "description": "K_UpArrow(Chrome)",
        "manipulators": [
          {
            "type": "basic",
            "from": <%= from("k",["caps_lock"]) %>,
            "to": <%= to([["up_arrow"]]) %>,
            "conditions": [ <%= frontmost_application_if("chrome") %> ]

          }
        ]
      },
      {
        "description": "N_最初の行へ(Chrome)",
        "manipulators": [
          {
            "type": "basic",
            "from": <%= from("n",["caps_lock"]) %>,
            "to": <%= to([["left_arrow", ["fn"]]]) %>,
            "conditions": [ <%= frontmost_application_if("chrome") %> ]
          }
        ]
      },
    {
      "description": "S_検索(Chrome)",
      "manipulators": [
        {
          "type": "basic",
