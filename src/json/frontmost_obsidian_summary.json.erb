{
    "title": "FrontmostIfObsidian_summary",
    "rules": [
      {
        "description": "FrontmostIfObsidian_summary",
        "manipulators": [
          {
            "description": "LeftCommand + N_HeadRowCursol",
            "type": "basic",
            "from": <%= from("n", ["left_control"], ["caps_lock"]) %>,
            "to": <%= to([["up_arrow", ["right_gui"]]]) %>,
            "conditions": [ <%= frontmost_application_if("obsidian") %> ]
          },
          {
            "description": "LeftCommand + M_EndRowCursol",
            "type": "basic",
            "from": <%= from("m", ["left_control"], ["caps_lock"]) %>,
            "to": <%= to([["down_arrow", ["right_gui"]]]) %>,
            "conditions": [ <%= frontmost_application_if("obsidian") %> ]
          },
          {
            "description": "RightOption + E_編集モード/プレビューモードを切り替える",
            "type": "basic",
            "from": <%= from("e", ["right_option"], ["caps_lock"]) %>,
            "to": <%= to([["7", ["left_shift", "right_option"]]]) %>,
            "conditions": [ <%= frontmost_application_if("obsidian") %> ]
          },
          {
            "description": "RightOption + W_ライブプレビュー/ソースモードを切り替える",
            "type": "basic",
            "from": <%= from("w", ["right_option"], ["caps_lock"]) %>,
            "to": <%= to([["6", ["left_shift", "right_option"]]]) %>,
            "conditions": [ <%= frontmost_application_if("obsidian") %> ]
          },
          {
            "description": "LeftCommand + T_左サイドバーを開閉",
            "type": "basic",
            "from": <%= from("t", ["left_control"], ["caps_lock"]) %>,
            "to": <%= to([["5", ["left_shift", "right_option"]]]) %>,
            "conditions": [ <%= frontmost_application_if("obsidian") %> ]
          },
          {
            "description": "LeftOption + T_右サイドバーを開閉",
            "type": "basic",
            "from": <%= from("t", ["left_option"], ["caps_lock"]) %>,
            "to": <%= to([["4", ["left_shift", "right_option"]]]) %>,
            "conditions": [ <%= frontmost_application_if("obsidian") %> ]
          },
          {
            "description": "LeftCommand + Oオウ_右に分割",
            "type": "basic",
            "from": <%= from("o", ["left_control"], ["caps_lock"]) %>,
            "to": <%= to([["3", ["left_shift", "right_option"]]]) %>,
            "conditions": [ <%= frontmost_application_if("obsidian") %> ]
          }
        ]
      }
    ]
}