{
    "title": "FrontmostIfLine_summary",
    "rules": [
      {
        "description": "FrontmostIfLine_summary",
        "manipulators": [
          {
            "description": "J_DownArrow",
            "type": "basic",
            "from": <%= from("j",["caps_lock"]) %>,
            "to": <%= to([["down_arrow"]]) %>,
            "conditions": [ <%= frontmost_application_if("readgamen") %> ]
          },
          {
            "description": "K_UpArrow",
            "type": "basic",
            "from": <%= from("k",["caps_lock"]) %>,
            "to": <%= to([["up_arrow"]]) %>,
            "conditions": [ <%= frontmost_application_if("readgamen") %> ]
          },
          {
            "description": "M_最終行へ",
            "type": "basic",
            "from": <%= from("m",["caps_lock"]) %>,
            "to": <%= to([["right_arrow", ["fn"]]]) %>,
            "conditions": [ <%= frontmost_application_if("line") %> ]
          },
          {
            "description": "N_最初の行へ",
            "type": "basic",
            "from": <%= from("n",["caps_lock"]) %>,
            "to": <%= to([["left_arrow", ["fn"]]]) %>,
            "conditions": [ <%= frontmost_application_if("line") %> ]
          },
          {
            "description": "S_検索バーアクティブ",
            "type": "basic",
            "from": <%= from("s", ["caps_lock"]) %>,
            "to": <%= to([["l", ["right_gui"]]]) %>,
            "conditions": [ <%= frontmost_application_if("line") %> ]
          },
          {
            "description": "Slash_PageUp",
            "type": "basic",
            "from": <%= from("slash", ["caps_lock"]) %>,
            "to": <%= to([["up_arrow", ["fn"]]]) %>,
            "conditions": [ <%= frontmost_application_if("line") %> ]
          },
          {
            "description": "B_PageDown",
            "type": "basic",
            "from": <%= from("b", ["caps_lock"]) %>,
            "to": <%= to([["down_arrow", ["fn"]]]) %>,
            "conditions": [ <%= frontmost_application_if("line") %> ]
          },
          {
            "description": "X_BookMarkSearch",
            "type": "basic",
            "from": <%= from("x", ["caps_lock"]) %>,
            "to": <%= to([["period", "left_option"]]) %>,
            "conditions": [ <%= frontmost_application_if("line") %> ]
          },
          {
            "description": "RightOption + T_BookMarkSearch",
            "type": "basic",
            "from": <%= from("t", ["right_alt"]) %>,
            "to": <%= to([["period", "left_option"]]) %>,
            "conditions": [ <%= frontmost_application_if("line") %> ]
          },
          {
            "description": "O(オー)_拡大",
            "type": "basic",
            "from": <%= from("o", ["caps_lock"]) %>,
            "to": <%= to([["equal_sign", ["left_shift","right_gui"]]]) %>,
            "conditions": [ <%= frontmost_application_if("line") %> ]
          },
          {
            "description": "i_縮小",
            "type": "basic",
            "from": <%= from("i", ["caps_lock"]) %>,
            "to": <%= to([["hyphen", ["left_shift","right_gui"]]]) %>,
            "conditions": [ <%= frontmost_application_if("line") %> ]
          },
          {
            "description": "T_単語検索",
            "type": "basic",
            "from": <%= from("t", ["caps_lock"]) %>,
            "to": <%= to([["f", ["right_gui"]]]) %>,
            "conditions": [ <%= frontmost_application_if("line") %> ]
          },
          {
            "description": "Semicolon_NextTab",
            "type": "basic",
            "from": <%= from("semicolon", ["caps_lock"]) %>,
            "to": <%= to([["tab", ["left_control"]]]) %>,
            "conditions": [ <%= frontmost_application_if("line") %> ]
          },
          {
            "description": "L_PreviousTab",
            "type": "basic",
            "from": <%= from("l", ["caps_lock"]) %>,
            "to": <%= to([["tab", ["left_shift","left_control"]]]) %>,
            "conditions": [ <%= frontmost_application_if("line") %> ]
          },
          {
            "description": "G_TabClose",
            "type": "basic",
            "from": <%= from("g", ["caps_lock"]) %>,
            "to": <%= to([["w", ["right_gui"]]]) %>,
            "conditions": [ <%= frontmost_application_if("line") %> ]
          },
          {
            "description": "V_Bookmark_Add",
            "type": "basic",
            "from": <%= from("v", ["caps_lock"]) %>,
            "to": <%= to([["d", ["right_gui"]]]) %>,
            "conditions": [ <%= frontmost_application_if("line") %> ]
          },
          {
            "description": "RightCommand + B_Bookmark_Add",
            "type": "basic",
            "from": <%= from("b", ["right_gui"]) %>,
            "to": <%= to([["d", ["right_gui"]]]) %>,
            "conditions": [ <%= frontmost_application_if("line") %> ]
          },
          {
            "description": "A_新しいタブを開く",
            "type": "basic",
            "from": <%= from("a", ["caps_lock"]) %>,
            "to": <%= to([["n", ["right_gui"]]]) %>,
            "conditions": [ <%= frontmost_application_if("line") %> ]
          },
          {
            "description": "W_新しいウィンドウで開く",
            "type": "basic",
            "from": <%= from("w", ["caps_lock"]) %>,
            "to": <%= to([["a", ["left_option"]]]) %>,
            "conditions": [ <%= frontmost_application_if("line") %> ]
          },
          {
            "description": "C_タブ複製",
            "type": "basic",
            "from": <%= from("c", ["caps_lock"]) %>,
            "to": <%= to([["f12", ["right_shift"]]]) %>,
            "conditions": [ <%= frontmost_application_if("line") %> ]
          },
          {
            "description": "E_再読込",
            "type": "basic",
            "from": <%= from("e", ["caps_lock"]) %>,
            "to": <%= to([["w", ["right_control", "right_shift"]]]) %>,
            "conditions": [ <%= frontmost_application_if("line") %> ]
          },
          {
            "description": "H_LeftArrow",
            "type": "basic",
            "from": <%= from("h", ["caps_lock"]) %>,
            "to": <%= to([["left_arrow"]]) %>,
            "conditions": [ <%= frontmost_application_if("line") %> ]
          },
          {
            "description": "Period_MaxWindow",
            "type": "basic",
            "from": <%= from("period", ["caps_lock"]) %>,
            "to": <%= to([["f", ["right_control", "right_gui"]]]) %>,
            "conditions": [ <%= frontmost_application_if("line") %> ]
          },
          {
            "description": "Comma_AppsSelect-Next",
            "type": "basic",
            "from": <%= from("comma", ["caps_lock"]) %>,
            "to": <%= to([["tab", "right_option"],["caps_lock"]]) %>,
            "conditions": [ <%= frontmost_application_if("line") %> ]
          },
          {
            "description": "Quote_PreviewActiveWindow",
            "type": "basic",
            "from": <%= from("quote", ["caps_lock"]) %>,
            "to": <%= set_shell_command(["open -a 'Preview.app'"]) %>,
            "conditions": [ <%= frontmost_application_if("line") %> ]
          },
          {
            "description": "U_Undo",
            "type": "basic",
            "from": <%= from("u", ["caps_lock"]) %>,
            "to": <%= to([["z", ["left_gui"]]]) %>,
            "conditions": [ <%= frontmost_application_if("line") %> ]
          },
          {
            "description": "R_miアクテイブ",
            "type": "basic",
            "from": <%= from("r", ["caps_lock"]) %>,
            "to": <%= set_shell_command(["open -a 'mi'"]) %>,
            "conditions": [ <%= frontmost_application_if("line") %> ]
          },
          {
          "description": "P_RightHalfWindowMove",
          "type": "basic",
          "from": <%= from("p", ["caps_lock"]) %>,
          "to": <%= to([["3", ["right_alt"]]]) %>,
          "conditions": [ <%= frontmost_application_if("line") %> ]
          },
          {
            "description": "Q_LeftHalfWindowMove",
            "type": "basic",
            "from": <%= from("q", ["caps_lock"]) %>,
            "to": <%= to([["2", ["right_alt"]]]) %>,
            "conditions": [ <%= frontmost_application_if("line") %> ]
          },
          {
            "description": "D_WindowTopHalf",
            "type": "basic",
            "from": <%= from("d", ["caps_lock"]) %>,
            "to": <%= to([["4", ["left_alt"]]]) %>,
            "conditions": [ <%= frontmost_application_if("line") %> ]
          },
          {
            "description": "F_WindowBottomHalf",
            "type": "basic",
            "from": <%= from("f", ["caps_lock"]) %>,
            "to": <%= to([["5", ["left_alt"]]]) %>,
            "conditions": [ <%= frontmost_application_if("line") %> ]
          },
          {
            "description": "RightCommand + H_Reload",
            "type": "basic",
            "from": <%= from("h", ["right_gui"]) %>,
            "to": <%= to([["w", ["right_control", "right_shift"]]]) %>,
            "conditions": [ <%= frontmost_application_if("line") %> ]
          },
          {
            "description": "LeftAlt + C_CopyTab",
            "type": "basic",
            "from": <%= from("c", ["left_alt"]) %>,
            "to": <%= to([["f12", ["right_shift"]]]) %>,
            "conditions": [ <%= frontmost_application_if("line") %> ]
          },
          {
            "description": "LeftAlt + d_DeveloperTool",
            "type": "basic",
            "from": <%= from("d", ["left_alt"]) %>,
            "to": <%= to([["i", ["right_alt", "right_gui"]]]) %>,
            "conditions": [ <%= frontmost_application_if("line") %> ]
          },
          {
            "description": "RightAlt + H_PageSourchOutput",
            "type": "basic",
            "from": <%= from("h", ["right_alt"]) %>,
            "to": <%= to([["u", ["right_alt", "right_gui"]]]) %>,
            "conditions": [ <%= frontmost_application_if("line") %> ]
          },
          {
            "description": "RightAlt + A_タブを新しいウィンドウに移動",
            "type": "basic",
            "from": <%= from("a", ["right_alt"]) %>,
            "to": <%= to([["hyphen", ["right_control", "right_alt"]]]) %>,
            "conditions": [ <%= frontmost_application_if("line") %> ]
          },
          {
            "description": "LeftAlt + A_シークレットウィンドウを開く",
            "type": "basic",
            "from": <%= from("a", ["left_alt"]) %>,
            "to": <%= to([["n", ["right_shift", "right_gui"]]]) %>,
            "conditions": [ <%= frontmost_application_if("line") %> ]
          },
          {
            "description": "LeftAlt + U_検索バーアクティヴ",
            "type": "basic",
            "from": <%= from("u", ["left_alt"]) %>,
            "to": <%= to([["l", ["right_gui"]]]) %>,
            "conditions": [ <%= frontmost_application_if("line") %> ]
          }
        ]
      }
    ]
}