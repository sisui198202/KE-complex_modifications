{
    "title": "FrontmostIfmi90",
    "rules": [
    {
      "description": "RightCommand + J_1RowSelection_LeftArrow(mi)",
      "manipulators": [
        {
          "type": "basic",
          "from": <%= from("j", ["right_gui"]) %>,
          "to": <%= to([["a", ["left_control"]], ["down_arrow", "left_shift"], ["left_arrow", "left_shift"]]) %>,
          "conditions": [ <%= frontmost_application_if("mi") %> ]
        }
      ]
    },
    {
      "description": "RightCommand + J_1RowSelection優先(mi)",
      "manipulators": [
        {
          "type": "basic",
          "from": <%= from("j", ["right_gui"]) %>,
          "to": <%= to([["a", ["left_control"]], ["down_arrow", "left_shift"], ["c", ["right_gui"]]]) %>,
          "conditions": [ <%= frontmost_application_if("mi") %> ]
        }
      ]
    },
    {
      "description": "RightCommand + K_CopyCurrentRowCut(mi)",
      "manipulators": [
        {
          "type": "basic",
          "from": <%= from("k", ["right_gui"]) %>,
          "to": <%= to([["a", ["left_control"]], ["down_arrow", "left_shift"], ["c", ["right_gui"]], ["delete_or_backspace"]]) %>,
          "conditions": [ <%= frontmost_application_if("mi") %> ]
        }
      ]
    },
    {
      "description": "LeftCommand + Z_RowCursolNewline(mi)",
      "manipulators": [
        {
          "type": "basic",
          "from": <%= from("z", ["left_control"], ["caps_lock"]) %>,
          "to": <%= to([["e", ["left_control"]], ["return_or_enter"]]) %>,
          "conditions": [ <%= frontmost_application_if("mi") %> ]
        }
      ]
    },
    {
      "description": "LeftAlt + R_Replace(mi)",
      "manipulators": [
        {
          "type": "basic",
          "from": <%= from("r", ["left_alt"], ["caps_lock"]) %>,
          "to": <%= to([["f", ["right_gui"]]]) %>,
          "conditions": [ <%= frontmost_application_if("mi") %> ]
        }
      ]
    },
    {
      "description": "RightCommand + L_NextTab(mi)",
      "manipulators": [
        {
          "type": "basic",
          "from": <%= from("l", ["right_gui"], ["caps_lock"]) %>,
          "to": <%= to([["open_bracket", ["left_shift", "right_gui"]]]) %>,
          "conditions": [ <%= frontmost_application_if("mi") %> ]
        }
      ]
    },
    {
      "description": "RightCommand + Semicolon_LatestTabToggle(mi)",
      "manipulators": [
        {
          "type": "basic",
          "from": <%= from("semicolon", ["right_gui"], ["caps_lock"]) %>,
          "to": <%= to([["close_bracket", ["left_shift", "right_gui"]]]) %>,
          "conditions": [ <%= frontmost_application_if("mi") %> ]
        }
      ]
    },
    {
      "description": "LeftAlt + M_マーク設定(mi)",
      "manipulators": [
        {
          "type": "basic",
          "from": <%= from("m", ["left_alt"], ["caps_lock"]) %>,
          "to": <%= to([["q", ["left_control", "right_shift"]]]) %>,
          "conditions": [ <%= frontmost_application_if("mi") %> ]
        }
      ]
    },
    {
      "description": "LeftCommand + B_PageDown(mi)",
      "manipulators": [
        {
          "type": "basic",
          "from": <%= from("b", ["left_control"], ["caps_lock"]) %>,
          "to": <%= to([["b", ["left_control", "right_option"]]]) %>,
          "conditions": [ <%= frontmost_application_if("mi") %> ]
        }
      ]
    },
    {
      "description": "LeftCommand + /(バックスラッシュ)_PageUp(mi)",
      "manipulators": [
        {
          "type": "basic",
          "from": <%= from("slash", ["left_control"], ["caps_lock"]) %>,
          "to": <%= to([["r", ["left_control", "right_option"]]]) %>,
          "conditions": [ <%= frontmost_application_if("mi") %> ]
        }
      ]
    },
    {
      "description": "LeftAlt + Comma_マークと選択範囲とスワップ(mi)",
      "manipulators": [
        {
          "type": "basic",
          "from": <%= from("comma", ["left_alt"], ["caps_lock"]) %>,
          "to": <%= to([["w", ["left_control", "right_shift"]]]) %>,
          "conditions": [ <%= frontmost_application_if("mi") %> ]
        }
      ]
    },
    {
      "description": "LeftCommand + W_CurrentCursol-EndRowSelect(mi)",
      "manipulators": [
        {
          "type": "basic",
          "from": <%= from("w", ["left_control"], ["caps_lock"]) %>,
          "to": <%= to([["down_arrow", ["left_shift"]]]) %>,
          "conditions": [ <%= frontmost_application_if("mi") %> ]
        }
      ]
    },
    {
      "description": "LeftCommand + N_HeadRowCursol(mi)",
      "manipulators": [
        {
          "type": "basic",
          "from": <%= from("n", ["left_control"], ["caps_lock"]) %>,
          "to": <%= to([["up_arrow", ["right_gui"]]]) %>,
          "conditions": [ <%= frontmost_application_if("mi") %> ]
        }
      ]
    },
    {
      "description": "LeftCommand + M_EndRowCursol(mi)",
      "manipulators": [
        {
          "type": "basic",
          "from": <%= from("m", ["left_control"], ["caps_lock"]) %>,
          "to": <%= to([["down_arrow", ["right_gui"]]]) %>,
          "conditions": [ <%= frontmost_application_if("mi") %> ]
        }
      ]
    },
    {
      "description": "Marukako_OpenCloseInput(mi)",
      "manipulators": [
        {
          "type": "basic",
          "from": <%= from("9", ["left_shift"], ["caps_lock"]) %>,
          "to": <%= to([["japanese_eisuu"], ["9", ["left_shift"]], ["0", ["left_shift"]], ["left_arrow"]]) %>,
          "conditions": [ <%= frontmost_application_if("mi") %> ]
        }
      ]
    },
   {
      "description": "Namikako_OpenCloseInput(mi)",
      "manipulators": [
        {
          "type": "basic",
          "from": <%= from("open_bracket", ["left_shift"], ["caps_lock"]) %>,
          "to": <%= to([["japanese_eisuu"], ["open_bracket", ["left_shift"]], ["close_bracket", ["left_shift"]], ["left_arrow"]]) %>,
          "conditions": [ <%= frontmost_application_if("mi") %> ]
        }
      ]
    },
    {
      "description": "Kakukako_OpenCloseInput(mi)",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "open_bracket",
            "modifiers": {
              "optional": [
                "caps_lock"
              ]
            }
          },
          "to": [
            {
              "key_code": "open_bracket"
            },
            {
              "key_code": "close_bracket"
            },
            {
              "key_code": "left_arrow"
            }
          ],
          "conditions": [
            {
              "type": "input_source_if",
              "input_sources": [{ "language": "en" }]
            },
            {
              "type": "frontmost_application_if",
              "bundle_identifiers": [
                "^net.mimikaki.mi$"
              ]
            }
          ]
        }
      ]
    },
    {
      "description": "Kagikako_OpenCloseInput(mi)",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "open_bracket",
