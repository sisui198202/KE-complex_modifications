{
    "title": "FrontmostIfmi",
    "rules": [
    {
      "description": "RightCommand + J_1RowSelection(mi)",
      "manipulators": [
        {
          "type": "basic",
          "from": <%= from("j", ["right_gui"]) %>,
          "to": <%= to([["a", ["left_control"]],["down_arrow","left_shift"]]) %>,
          "conditions": [ <%= frontmost_application_if("mi") %> ]
        }
      ]
    },
    {
      "description": "RightCommand + K_1RowCut(mi)",
      "manipulators": [
        {
          "type": "basic",
          "from": <%= from("k", ["right_gui"]) %>,
          "to": <%= to([["a", ["left_control"]],["down_arrow","left_shift"],["delete_or_backspace"]]) %>,
          "conditions": [ <%= frontmost_application_if("mi") %> ]
        }
      ]
    },
    {
      "description": "LeftCommand + Z_RowCursolNewline(mi)",
      "manipulators": [
        {
          "type": "basic",
          "from": <%= from("z", ["left_control"]) %>,
          "to": <%= to([["e",["left_control"]],["return_or_enter"]]) %>,
          "conditions": [ <%= frontmost_application_if("mi") %> ]
        }
      ]
    },
    {
      "description": "RightCommand + NextTab(mi)",
      "manipulators": [
        {
          "type": "basic",
          "from": <%= from("l", ["right_gui"]) %>,
          "to": <%= to([["close_bracket",["left_shift","right_gui"]]]) %>,
          "conditions": [ <%= frontmost_application_if("mi") %> ]
        }
      ]
    },
    {
      "description": "RightCommand + semicolon_LatestTabToggle(mi)",
      "manipulators": [
        {
          "type": "basic",
          "from": <%= from("semicolon", ["right_gui"]) %>,
          "to": <%= to([["open_bracket",["left_shift","right_gui"]]]) %>,
          "conditions": [ <%= frontmost_application_if("mi") %> ]
        }
      ]
    }
    ]
}