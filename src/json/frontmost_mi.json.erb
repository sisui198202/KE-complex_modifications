            {
              "key_code": "open_bracket"
            },
            {
              "key_code": "close_bracket"
            },
            {
              "key_code": "left_arrow"
            }
          ],
          "conditions": [
            {
              "type": "input_source_if",
              "input_sources": [{ "language": "ja" }]
            },
            {
              "type": "frontmost_application_if",
              "bundle_identifiers": [
                "^net.mimikaki.mi$"
              ]
            }
          ]
        }
      ]
    },
   {
      "description": "DoubleQuote_OpenCloseInput(mi)",
      "manipulators": [
        {
          "type": "basic",
          "from": <%= from("quote", ["left_shift"], ["caps_lock"]) %>,
