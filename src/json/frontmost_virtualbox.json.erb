    {
      "description": "コマンドキー単体で押す（左コマンドキー英数、右コマンドキーかな）(Vitualbox)",
      "manipulators": [
          {
              "from": {
                  "key_code": "left_control",
                  "modifiers": {
                    "optional": [
                      "caps_lock"
                    ]
                  }
              },
              "to": [
                  {
                      "key_code": "left_control",
                      "lazy": true
                  }
              ],
              "conditions": [
                {
                  "type": "frontmost_application_if",
                  "bundle_identifiers": [
                    "^org.virtualbox.app.VirtualBoxVM$"
                  ]
                }
              ],
              "to_if_alone": [
                  {
                      "key_code": "spacebar",
                      "modifiers": [
                        "left_command"
                    ]
                  }
              ],
              "type": "basic"
          },
          {
              "from": {
                  "key_code": "right_command",
                  "modifiers": {
                    "optional": [
                      "caps_lock"
                    ]
                  }
              },
              "to": [
                  {
                      "key_code": "right_command",
                      "lazy": true
                  }
              ],
              "conditions": [
                {
