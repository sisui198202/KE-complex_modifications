        {
          "type": "basic",
          "from": {
            "key_code": "left_alt",
            "modifiers": {
              "optional": [
                "caps_lock"
              ]
            }
          },
          "to": [
            {
              "key_code": "left_alt"
            }
          ],
          "to_if_alone": [
            {
              "key_code": "left_alt",
              "modifiers": "left_gui"
            }
          ],
	  "conditions": [
		{
		  "type": "frontmost_application_if",
		  "bundle_identifiers": [
		    "^com\\.microsoft\\.Excel$"
		  ]
		}
	      ]
        }
      ]
