{
    "title": "FrontmostIfExcel103",
    "rules": [
   {
      "description": "Marukako_OpenCloseInput(Excel)",
      "manipulators": [
        {
          "type": "basic",
          "from": <%= from("9", ["left_shift"]) %>,
          "to": <%= to([["9", ["left_shift"]], ["0", ["left_shift"]]]) %>,
          "conditions": [ <%= frontmost_application_if("excel") %> ]
        }
      ]
    },
