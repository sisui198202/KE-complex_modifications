        }
      ]
    },
    {
      "description": "J_DownArrow(eagle)",
      "manipulators": [
        {
          "type": "basic",
          "from": <%= from("j",["caps_lock"]) %>,
          "to": <%= to([["down_arrow"]]) %>,
          "conditions": [ <%= frontmost_application_if("eagle") %> ]
        }
      ]
    },
    {
    "description": "k_UpArrow(eagle)",
