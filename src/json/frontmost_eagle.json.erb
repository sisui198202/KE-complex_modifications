{
    "title": "FrontmostIfEagle1",
    "rules": [
    {
      "description": "RightCommand + L_サイズを調整(preview)",
      "manipulators": [
        {
          "type": "basic",
          "from": <%= from("l", ["right_gui"], ["caps_lock"]) %>,
          "to": <%= to([["0", ["right_gui", "right_option"]]]) %>,
          "conditions": [ <%= frontmost_application_if("preview") %> ]
        }
      ]
    },
    {
      "description": "LeftOption + O_Expansion(preview)",
      "manipulators": [
        {
          "type": "basic",
          "from": <%= from("o", ["left_option"]) %>,
          "to": <%= to([["equal_sign", ["left_shift", "right_gui"]]]) %>,
          "conditions": [ <%= frontmost_application_if("preview") %> ]
        }
      ]
    },
    {
      "description": "LeftOption + i_Shrink(preview)",
      "manipulators": [
        {
          "type": "basic",
          "from": <%= from("i", ["left_option"]) %>,
          "to": <%= to([["hyphen", ["left_shift", "right_gui"]]]) %>,
          "conditions": [ <%= frontmost_application_if("preview") %> ]
        }
      ]
    }
    ]
}