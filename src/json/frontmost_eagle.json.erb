{
    "title": "FrontmostIfEagle",
    "rules": [
    {
      "description": "LeftOption + O_Expansion(eagle)",
      "manipulators": [
        {
          "type": "basic",
          "from": <%= from("o", ["left_option"]) %>,
          "to": <%= to([["equal_sign", ["right_gui"]]]) %>,
          "conditions": [ <%= frontmost_application_if("eagle") %> ]
        }
      ]
    },
    {
      "description": "LeftOption + i_Shrink(eagle)",
      "manipulators": [
        {
          "type": "basic",
          "from": <%= from("i", ["left_option"]) %>,
          "to": <%= to([["hyphen", ["right_gui"]]]) %>,
          "conditions": [ <%= frontmost_application_if("eagle") %> ]
        }
      ]
    },
    {
      "description": "LeftOption + .[ピリオド]_pathFinderから開く(eagle)",
      "manipulators": [
        {
          "type": "basic",
          "from": <%= from("period", ["left_option"]) %>,
          "to": <%= to([["return_or_enter", ["right_gui"]]]) %>,
          "conditions": [ <%= frontmost_application_if("eagle") %> ]
        }
      ]
    },
    {
      "description": "O[オー]_Expansion(eagle)",
      "manipulators": [
        {
          "type": "basic",
            "from": <%= from("o",["caps_lock"]) %>,
          "to": <%= to([["equal_sign", ["right_gui"]]]) %>,
          "conditions": [ <%= frontmost_application_if("eagle") %> ]
        }
      ]
    },
    {
