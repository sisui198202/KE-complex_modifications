{
    "title": "FrontmostIfSkitch",
    "rules": [
    {
      "description": "LeftOption + O(オー)_Export(skitch)",
      "manipulators": [
        {
          "type": "basic",
          "from": <%= from("o", ["left_option"]) %>,
          "to": <%= to([["e", ["right_gui"]]]) %>,
          "conditions": [ <%= frontmost_application_if("skitch") %> ]
        }
      ]
    },
    {
      "description": "RightCommand + L_長方形(skitch)",
      "manipulators": [
        {
          "type": "basic",
          "from": <%= from("l", ["right_gui"]) %>,
          "to": <%= to([["r", ["right_gui","right_option"]]]) %>,
          "conditions": [ <%= frontmost_application_if("skitch") %> ]
        }
      ]
    }
    ]
}