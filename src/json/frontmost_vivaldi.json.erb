{
    "title": "FrontmostIfVivaldi_ver1",
    "rules": [
    {
      "description": "LeftCommand + U_戻る(Vivaldi)",
      "manipulators": [
        {
          "type": "basic",
          "from": <%= from("u", ["left_control"]) %>,
          "to": <%= to([["8", ["right_alt","right_gui"]]]) %>,
          "conditions": [ <%= frontmost_application_if("vivaldi") %> ]
        }
      ]
    },
    {
      "description": "RightCommand + H_Reload(Vivaldi)",
      "manipulators": [
        {
          "type": "basic",
          "from": <%= from("h", ["right_gui"]) %>,
          "to": <%= to([["f5"]]) %>,
          "conditions": [ <%= frontmost_application_if("vivaldi") %> ]
        }
      ]
    },
    {
      "description": "LeftAlt + H_SuperReload(Vivaldi)",
      "manipulators": [
        {
          "type": "basic",
          "from": <%= from("h", ["left_alt"]) %>,
          "to": <%= to([["r", ["left_control","right_shift"]]]) %>,
          "conditions": [ <%= frontmost_application_if("vivaldi") %> ]
        }
      ]
    }
    ]
}