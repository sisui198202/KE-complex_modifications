          "type": "basic",
          "from": <%= from("u", ["caps_lock"]) %>,
          "to": <%= to([["z", ["left_gui"]]]) %>,
          "conditions": [ <%= frontmost_application_if("browser") %> ]
        },
        {
          "description": "LeftAlt + U_検索バーアクティヴ-Copy(browser)",
          "type": "basic",
          "from": <%= from("u", ["left_alt"], ["caps_lock"]) %>,
          "to": <%= to([["l","right_gui"], ["c", "right_gui"]]) %>,
          "conditions": [ <%= frontmost_application_if("browser") %> ]
        },
        {
          "description": "RightCommand + H_Reload(browser)",
          "type": "basic",
          "from": <%= from("h", ["right_gui"], ["caps_lock"]) %>,
          "to": <%= to([["w", ["right_control", "right_shift"]]]) %>,
          "conditions": [ <%= frontmost_application_if("browser") %> ]
        },
        {
          "description": "LeftShift + RightCommand + H_ハードキャッシュ(browser)",
          "type": "basic",
          "from": <%= from("h", ["left_shift", "right_gui"], ["caps_lock"]) %>,
          "to": <%= to([["delete_or_backspace", ["right_gui", "right_shift"]]]) %>,
