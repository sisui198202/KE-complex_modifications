{
  "title": "FrontmostIfBrowser99",
  "rules": [
    {
      "description": "Basic Navigation Commands (browser)",
      "manipulators": [
        {
          "description": "J_DownArrow",
          "type": "basic",
          "from": <%= from("j", ["caps_lock"]) %>,
          "to": <%= to([["down_arrow"]]) %>,
          "conditions": [<%= frontmost_application_if("browser") %>]
        },
        {
          "description": "K_UpArrow",
          "type": "basic",
          "from": <%= from("k",["caps_lock"]) %>,
          "to": <%= to([["up_arrow"]]) %>,
          "conditions": [ <%= frontmost_application_if("browser") %> ]
        },
        {
          "description": "L_RightArrow",
          "type": "basic",
          "from": <%= from("l", ["caps_lock"]) %>,
          "to": <%= to([["right_arrow"]]) %>,
          "conditions": [<%= frontmost_application_if("browser") %>]
        },
        {
          "description": "H_LeftArrow",
          "type": "basic",
          "from": <%= from("h", ["caps_lock"]) %>,
          "to": <%= to([["left_arrow"]]) %>,
          "conditions": [<%= frontmost_application_if("browser") %>]
        },
        {
          "description": "i_PageDown",
          "type": "basic",
          "from": <%= from("i", ["caps_lock"]) %>,
          "to": <%= to([["down_arrow", ["fn"]]]) %>,
          "conditions": [ <%= frontmost_application_if("browser") %> ]
        },
        {
          "description": "O(オー)_PageUp",
          "type": "basic",
          "from": <%= from("o", ["caps_lock"]) %>,
          "to": <%= to([["up_arrow", ["fn"]]]) %>,
          "conditions": [ <%= frontmost_application_if("browser") %> ]
        },
        {
          "description": "M_最終行へ",
          "type": "basic",
          "from": <%= from("m",["caps_lock"]) %>,
          "to": <%= to([["right_arrow", ["fn"]]]) %>,
          "conditions": [ <%= frontmost_application_if("browser") %> ]
        },
        {
          "description": "N_最初の行へ",
          "type": "basic",
          "from": <%= from("n",["caps_lock"]) %>,
          "to": <%= to([["left_arrow", ["fn"]]]) %>,
          "conditions": [ <%= frontmost_application_if("browser") %> ]
        }
      ]
    },
    {
      "description": "Window Control Commands(browser)",
      "manipulators": [
        {
          "description": "W_新規ウインドウ",
          "type": "basic",
          "from": <%= from("w", ["caps_lock"]) %>,
          "to": <%= to([["a", ["left_option"]]]) %>,
          "conditions": [ <%= frontmost_application_if("browser") %> ]
        },
        {
          "description": "Period_MaxWindow",
          "type": "basic",
          "from": <%= from("period", ["caps_lock"]) %>,
          "to": <%= to([["f", ["right_control", "right_gui"]]]) %>,
          "conditions": [ <%= frontmost_application_if("browser") %> ]
        },
        {
          "description": "Comma_AppsSelect-Next",
          "type": "basic",
          "from": <%= from("comma", ["caps_lock"]) %>,
          "to": <%= to([["tab", "right_gui"]]) %>,
          "conditions": [ <%= frontmost_application_if("browser") %> ]
        },
        {
          "description": "Quote_PreviewActiveWindow",
          "type": "basic",
          "from": <%= from("quote", ["caps_lock"]) %>,
          "to": <%= set_shell_command(["open -a 'Preview.app'"]) %>,
          "conditions": [ <%= frontmost_application_if("browser") %> ]
        },
        {
          "description": "R_miアクテイブ",
          "type": "basic",
          "from": <%= from("r", ["caps_lock"]) %>,
          "to": <%= set_shell_command(["open -a 'mi'"]) %>,
          "conditions": [ <%= frontmost_application_if("browser") %> ]
        },
        {
          "description": "P_RightHalfWindowMove",
          "type": "basic",
          "from": <%= from("p", ["caps_lock"]) %>,
          "to": <%= to([["3", ["right_alt"]]]) %>,
          "conditions": [ <%= frontmost_application_if("browser") %> ]
        },
        {
          "description": "Q_LeftHalfWindowMove",
          "type": "basic",
          "from": <%= from("q", ["caps_lock"]) %>,
          "to": <%= to([["2", ["right_alt"]]]) %>,
          "conditions": [ <%= frontmost_application_if("browser") %> ]
        },
        {
          "description": "D_WindowTopHalf",
          "type": "basic",
          "from": <%= from("d", ["caps_lock"]) %>,
          "to": <%= to([["4", ["left_alt"]]]) %>,
          "conditions": [ <%= frontmost_application_if("browser") %> ]
        },
        {
          "description": "F_WindowBottomHalf",
          "type": "basic",
          "from": <%= from("f", ["caps_lock"]) %>,
          "to": <%= to([["5", ["left_alt"]]]) %>,
          "conditions": [ <%= frontmost_application_if("browser") %> ]
        },
        {
          "description": "RightCommand + Period_MaxWindow",
          "type": "basic",
          "from": <%= from("period", ["right_gui"], ["caps_lock"]) %>,
          "to": <%= to([["f", ["right_control", "right_gui"]]]) %>,
          "conditions": [ <%= frontmost_application_if("browser") %> ]
        },
        {
          "description": "Fn + J_WindowLeftDown",
          "type": "basic",
          "from": <%= from("j", ["right_control"], ["caps_lock"]) %>,
          "to": <%= to([["8", "right_alt"]]) %>,
          "conditions": [ <%= frontmost_application_if("browser") %> ]
        },
        {
          "description": "Fn + U_WindowLeftUp",
          "type": "basic",
          "from": <%= from("u", ["right_control"], ["caps_lock"]) %>,
          "to": <%= to([["6", "right_alt"]]) %>,
          "conditions": [<%= frontmost_application_if("browser") %>]
        },
        {
          "description": "Fn + I_WindowRightUp",
          "type": "basic",
          "from": <%= from("i", ["right_control"], ["caps_lock"]) %>,
          "to": <%= to([["7", "right_alt"]]) %>,
          "conditions": [<%= frontmost_application_if("browser") %>]
        },
        {
          "description": "Fn + K_WindowRightDown",
          "type": "basic",
          "from": <%= from("k", ["right_control"], ["caps_lock"]) %>,
          "to": <%= to([["9", "right_alt"]]) %>,
          "conditions": [<%= frontmost_application_if("browser") %>]
        },
        {
          "description": "Fn + L_RightHalfWindowMove",
          "type": "basic",
          "from": <%= from("l", ["right_control"], ["caps_lock"]) %>,
          "to": <%= to([["3", ["right_alt"]]]) %>,
          "conditions": [<%= frontmost_application_if("browser") %>]
        },
        {
            "description": "Fn + G_WindowLeftOneThird",
            "type": "basic",
            "from": <%= from("g", ["right_control"], ["caps_lock"]) %>,
            "to": <%= to([["d", ["right_control", "right_alt"]]]) %>,
            "conditions": [ <%= frontmost_application_if("browser") %> ]
        },
        {
          "description": "Fn + Quote_WindowRightOneThird",
          "type": "basic",
          "from": <%= from("quote", ["right_control"], ["caps_lock"]) %>,
          "to": <%= to([["g", ["right_control", "right_alt"]]]) %>,
          "conditions": [ <%= frontmost_application_if("browser") %> ]
        },
        {
            "description": "Fn + Y_WindowLeftTwo-thirds",
            "type": "basic",
            "from": <%= from("y", ["right_control"], ["caps_lock"]) %>,
            "to": <%= to([["e", ["right_control", "right_alt"]]]) %>,
            "conditions": [ <%= frontmost_application_if("browser") %> ]
        },
        {
          "description": "Fn + O_WindowRightTwo-thirds",
          "type": "basic",
          "from": <%= from("o", ["right_control"], ["caps_lock"]) %>,
          "to": <%= to([["t", ["right_control", "right_alt"]]]) %>,
          "conditions": [ <%= frontmost_application_if("browser") %> ]
        },
        {
            "description": "Fn + Comma_WindowCenter",
            "type": "basic",
            "from": <%= from("comma", ["right_control"], ["caps_lock"]) %>,
            "to": <%= to([["c", ["right_control", "right_alt"]]]) %>,
            "conditions": [ <%= frontmost_application_if("browser") %> ]
        },
        {
            "description": "Fn + N_WindowUP",
            "type": "basic",
            "from": <%= from("n", ["right_control"], ["caps_lock"]) %>,
            "to": <%= to([["4", ["left_alt"]]]) %>,
            "conditions": [ <%= frontmost_application_if("browser") %> ]
        },
        {
          "description": "Fn + M_WindowDown",
          "type": "basic",
          "from": <%= from("m", ["right_control"], ["caps_lock"]) %>,
          "to": <%= to([["5", ["left_alt"]]]) %>,
          "conditions": [ <%= frontmost_application_if("browser") %> ]
        },
        {
          "description": "Fn + H_LeftWindowMove",
          "type": "basic",
          "from": <%= from("h", ["right_control"], ["caps_lock"]) %>,
          "to": <%= to([["2", ["right_alt"]]]) %>,
          "conditions": [ <%= frontmost_application_if("browser") %> ]
        }
      ]
    },
    {
      "description": "Tab Management Commands(browser)",
      "manipulators": [
        {
          "description": "Semicolon_NextTab",
          "type": "basic",
          "from": <%= from("semicolon", ["caps_lock"]) %>,
          "to": <%= to([["tab", ["left_control"]]]) %>,
          "conditions": [<%= frontmost_application_if("browser") %>]
        },
        {
          "description": "Z_PreviousTab",
          "type": "basic",
          "from": <%= from("z", ["caps_lock"]) %>,
          "to": <%= to([["tab", ["left_shift", "left_control"]]]) %>,
          "conditions": [<%= frontmost_application_if("browser") %>]
        },
        {
          "description": "G_TabClose",
          "type": "basic",
          "from": <%= from("g", ["caps_lock"]) %>,
          "to": <%= to([["w", ["right_gui"]]]) %>,
          "conditions": [<%= frontmost_application_if("browser") %>]
        }
      ]
    },
    {
      "description": "Zoom Commands(browser)",
      "manipulators": [
        {
          "description": "Slash_拡大",
          "type": "basic",
          "from": <%= from("slash", ["caps_lock"]) %>,
          "to": <%= to([["equal_sign", ["left_shift", "right_gui"]]]) %>,
          "conditions": [<%= frontmost_application_if("browser") %>]
        },
        {
          "description": "B_縮小",
          "type": "basic",
          "from": <%= from("b", ["caps_lock"]) %>,
          "to": <%= to([["hyphen", ["left_shift", "right_gui"]]]) %>,
          "conditions": [<%= frontmost_application_if("browser") %>]
        },
        {
          "description": "RightCommand + Slash_拡大",
          "type": "basic",
          "from": <%= from("slash", ["right_gui"], ["caps_lock"]) %>,
          "to": <%= to([["equal_sign", ["left_shift","right_gui"]]]) %>,
          "conditions": [ <%= frontmost_application_if("browser") %> ]
        },
        {
          "description": "RightOption + B_縮小",
          "type": "basic",
          "from": <%= from("b", ["right_option"], ["caps_lock"]) %>,
          "to": <%= to([["hyphen", ["left_shift","right_gui"]]]) %>,
          "conditions": [ <%= frontmost_application_if("browser") %> ]
        }
      ]
    },
    {
      "description": "Custom browser Commands(browser)",
      "manipulators": [
        {
          "description": "T_単語検索",
          "type": "basic",
          "from": <%= from("t", ["caps_lock"]) %>,
          "to": <%= to([["f", ["right_gui"]]]) %>,
          "conditions": [<%= frontmost_application_if("browser") %>]
        },
        {
          "description": "S_検索バーアクティブ",
          "type": "basic",
          "from": <%= from("s", ["caps_lock"]) %>,
          "to": <%= to([["l", ["right_gui"]]]) %>,
          "conditions": [ <%= frontmost_application_if("browser") %> ]
        },
        {
          "description": "検証要-U_Undo",
          "type": "basic",
          "from": <%= from("u", ["caps_lock"]) %>,
          "to": <%= to([["z", ["left_gui"]]]) %>,
          "conditions": [ <%= frontmost_application_if("browser") %> ]
        },
        {
          "description": "LeftAlt + U_検索バーアクティヴ-Copy",
          "type": "basic",
          "from": <%= from("u", ["left_alt"], ["caps_lock"]) %>,
          "to": <%= to([["l","right_gui"], ["c", "right_gui"]]) %>,
          "conditions": [ <%= frontmost_application_if("browser") %> ]
        },
        {
          "description": "RightAlt + H_PageSourchOutput",
          "type": "basic",
          "from": <%= from("h", ["right_alt"], ["caps_lock"]) %>,
          "to": <%= to([["u", ["right_alt", "right_gui"]]]) %>,
          "conditions": [ <%= frontmost_application_if("browser") %> ]
        }
      ]
    },
    {
      "description": "BookMarkManagement(browser)",
      "manipulators": [
        {
          "description": "V_Bookmark_Add",
          "type": "basic",
          "from": <%= from("v", ["caps_lock"]) %>,
          "to": <%= to([["d", ["right_gui"]]]) %>,
          "conditions": [<%= frontmost_application_if("browser") %>]
        },
        {
          "description": "RightCommand + B_Bookmark_Add",
          "type": "basic",
          "from": <%= from("b", ["right_gui"], ["caps_lock"]) %>,
          "to": <%= to([["d", ["right_gui"]]]) %>,
          "conditions": [ <%= frontmost_application_if("browser") %> ]
        }
      ]
    },
    {
      "description": "verification(browser)",
      "manipulators": [
        {
          "description": "RightAlt + S_検証-Select an Element in the Page to inspect it",
          "type": "basic",
          "from": <%= from("s", ["right_alt"], ["caps_lock"]) %>,
          "to": <%= to([["c", ["right_gui", "right_shift"]]]) %>,
          "conditions": [ <%= frontmost_application_if("browser") %> ]
        },
        {
          "description": "LeftCommand + T_検証-ドックの位置変更",
          "type": "basic",
          "from": <%= from("t", ["left_control"], ["caps_lock"]) %>,
          "to": <%= to([["d", ["right_gui", "right_shift"]]]) %>,
          "conditions": [ <%= frontmost_application_if("browser") %> ]
        },
        {
          "description": "LeftAlt + d_DeveloperTool起動",
          "type": "basic",
          "from": <%= from("d", ["left_alt"], ["caps_lock"]) %>,
          "to": <%= to([["i", ["right_alt", "right_gui"]]]) %>,
          "conditions": [ <%= frontmost_application_if("browser") %> ]
        },
        {
          "description": "RightAlt + C_検証-ConsoleClear",
          "type": "basic",
          "from": <%= from("c", ["right_alt"], ["caps_lock"]) %>,
          "to": <%= to([["l","right_control"]]) %>,
          "conditions": [ <%= frontmost_application_if("browser") %> ]
        }
      ]
    }
  ]
}
