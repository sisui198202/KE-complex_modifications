{
  "title": "FrontmostIfBrowser99",
  "rules": [
    {
      "description": "Basic Navigation Commands (browser)",
      "manipulators": [
        {
          "description": "J_DownArrow(browser)",
          "type": "basic",
          "from": <%= from("j", ["caps_lock"]) %>,
          "to": <%= to([["down_arrow"]]) %>,
          "conditions": [<%= frontmost_application_if("browser") %>]
        },
        {
          "description": "K_UpArrow(browser)",
          "type": "basic",
          "from": <%= from("k",["caps_lock"]) %>,
          "to": <%= to([["up_arrow"]]) %>,
          "conditions": [ <%= frontmost_application_if("browser") %> ]
        },
        {
          "description": "L_RightArrow(browser)",
          "type": "basic",
          "from": <%= from("l", ["caps_lock"]) %>,
          "to": <%= to([["right_arrow"]]) %>,
          "conditions": [<%= frontmost_application_if("browser") %>]
        },
        {
          "description": "H_LeftArrow(browser)",
          "type": "basic",
          "from": <%= from("h", ["caps_lock"]) %>,
          "to": <%= to([["left_arrow"]]) %>,
          "conditions": [<%= frontmost_application_if("browser") %>]
        },
        {
          "description": "i_PageDown(browser)",
          "type": "basic",
          "from": <%= from("i", ["caps_lock"]) %>,
          "to": <%= to([["down_arrow", ["fn"]]]) %>,
          "conditions": [ <%= frontmost_application_if("browser") %> ]
