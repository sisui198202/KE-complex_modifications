        {
          "description": "O(オー)_PageUp(browser)",
          "type": "basic",
          "from": <%= from("o", ["caps_lock"]) %>,
          "to": <%= to([["up_arrow", ["fn"]]]) %>,
          "conditions": [ <%= frontmost_application_if("browser") %> ]
        },
        {
          "description": "M_最終行へ(browser)",
          "type": "basic",
          "from": <%= from("m",["caps_lock"]) %>,
          "to": <%= to([["right_arrow", ["fn"]]]) %>,
          "conditions": [ <%= frontmost_application_if("browser") %> ]
        },
        {
          "description": "N_最初の行へ(browser)",
          "type": "basic",
          "from": <%= from("n",["caps_lock"]) %>,
          "to": <%= to([["left_arrow", ["fn"]]]) %>,
          "conditions": [ <%= frontmost_application_if("browser") %> ]
        }
      ]
    },
