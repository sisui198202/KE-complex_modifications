{
  "title": "add_Personal rules1 (@sisui)_ver14",
  "rules": [
    {
      "description": "LeftAlt + o_Save_ver7",
      "manipulators":
      <%=
               each_key(
                 source_keys_list: ["o"],
                 from_mandatory_modifiers: ["left_option"],
                 dest_keys_list: ["s"],
                 to_modifiers: ["left_gui"],
                 as_json: true
               )
       %>        
    },
   {
      "description": "RightCommand + Slash_SelectAllSameWord2(vscode)",
      "manipulators":
      <%=
               each_key(
                 source_keys_list: ["slash"],
                 from_mandatory_modifiers: ["right_gui"],
                 dest_keys_list: ["i"],
                 to_modifiers: ["right_control","right-shift"],
                 as_json: true
               )
       %>
   },
   {
      "description": "LeftCommand + CloseBracket_unindent",
      "manipulators":
      <%=
               each_key(
                 source_keys_list: ["close_bracket"],
                 from_mandatory_modifiers: ["left_control"],
                 dest_keys_list: ["1"],
                 to_modifiers: ["right_control","right-shift"],
                 as_json: true
               )
       %>
    }
    ]
}