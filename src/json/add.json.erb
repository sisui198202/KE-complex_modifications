{
  "title": "Add_3",
  "rules": [
  {
      "description": "None",
      "manipulators": [
        {
          "type": "basic",
          "from": <%= from("k", ["right_gui"]) %>,
          "to": <%= to([["l", "right_gui"], ["delete_or_backspace"]]) %>
        }
      ]
    },
  {
      "description": "tesuto",
      "manipulators": [
        {
          "type": "basic",
          "from": <%= from("f1") %>,
          "to": <%= to(["a"]) %>
        }
      ]
    },
    {
      "description": "Fn + period_ChromeUp&VscodeDown(WindowControl)",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "period",
            "modifiers": {
              "mandatory": [
                "fn"
              ]
            }
          },
          "to": [
            {
              "shell_command": "open -a 'Google Chrome.app'"
            },
            {
              "key_code": "5",
              "modifiers": [
                "left_alt"
              ]
            },
            {
              "shell_command": "open -a 'Visual Studio Code.app'"
            },
            {
              "key_code": "4",
              "modifiers": [
                "left_alt"
              ]
            }            
          ]
        }
      ]
    }
    ]
}