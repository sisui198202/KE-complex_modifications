{
  "title": "FrontmostIfPathFinder98",
  "rules": [
    {
      "description": "LeftAlt + U_PathSelection(pathfinder)",
      "manipulators": [
        {
          "type": "basic",
          "from": <%= from("u", ["left_alt"], ["caps_lock"]) %>,
          "to": <%= to([["l", ["left_alt", "left_gui"]]]) %>,
          "conditions": [ <%= frontmost_application_if("pathfinder") %> ]
        }
      ]
    },
    {
      "description": "RightCommand + Space_FileDelete(pathfinder)",
      "manipulators": [
        {
          "type": "basic",
          "from": <%= from("spacebar", ["right_gui"], ["caps_lock"]) %>,
          "to": <%= to([["delete_or_backspace", ["right_gui"]]]) %>,
          "conditions": [ <%= frontmost_application_if("pathfinder") %> ]
        }
      ]
    },
    {
      "description": "RightCommand + Z_エイリアスを作る(pathfinder)",
      "manipulators": [
        {
          "type": "basic",
          "from": <%= from("z", ["right_gui"], ["caps_lock"]) %>,
          "to": <%= to([["l", ["right_gui"]]]) %>,
          "conditions": [ <%= frontmost_application_if("pathfinder") %> ]
        }
      ]
    },
    {
      "description": "LeftAlt + F_NewFileCreate(pathfinder)",
      "manipulators": [
        {
          "type": "basic",
          "from": <%= from("f", ["left_alt"], ["caps_lock"]) %>,
          "to": <%= to([["n", ["right_alt", "right_gui"]]]) %>,
          "conditions": [ <%= frontmost_application_if("pathfinder") %> ]
        }
      ]
    },
   {
      "description": "LeftAlt + A_FolderCreate(pathfinder)",
      "manipulators": [
        {
          "type": "basic",
          "from": <%= from("a", ["left_alt"], ["caps_lock"]) %>,
          "to": <%= to([["n", ["right_shift", "right_gui"]]]) %>,
          "conditions": [ <%= frontmost_application_if("pathfinder") %> ]
        }
      ]
    },
   {
      "description": "LeftAlt + Period_CurrentFolderTerminalOpen(pathfinder)",
      "manipulators": [
        {
          "type": "basic",
          "from": <%= from("period", ["left_alt"], ["caps_lock"]) %>,
          "to": <%= to([["period", ["left_control", "right_alt"]]]) %>,
          "conditions": [ <%= frontmost_application_if("pathfinder") %> ]
        }
      ]
    },
    {
      "description": "LeftAlt + Comma_SelectFilePathCopy(pathfinder)",
      "manipulators": [
        {
          "type": "basic",
          "from": <%= from("comma", ["left_alt"], ["caps_lock"]) %>,
          "to": <%= to([["backslash", ["left_control", "left_alt"]]]) %>,
          "conditions": [ <%= frontmost_application_if("pathfinder") %> ]
        }
      ]
    },
    {
      "description": "LeftOption + SpaceBar_FileOpen(pathfinder)",
      "manipulators": [
        {
          "type": "basic",
          "from": <%= from("spacebar", ["left_option"], ["caps_lock"]) %>,
          "to": <%= to([["o", ["right_gui"]]]) %>,
          "conditions": [ <%= frontmost_application_if("pathfinder") %> ]
        }
      ]
    },
    {
      "description": "LeftOption + I_親ディレクトリへ(pathfinder)",
      "manipulators": [
        {
          "type": "basic",
          "from": <%= from("i", ["left_option"], ["caps_lock"]) %>,
          "to": <%= to([["up_arrow", ["right_gui"]]]) %>,
          "conditions": [ <%= frontmost_application_if("pathfinder") %> ]
        }
      ]
    },
    {
      "description": "LeftOption + O(オー)_FileOpen(pathfinder)",
      "manipulators": [
        {
          "type": "basic",
          "from": <%= from("o", ["left_option"], ["caps_lock"]) %>,
          "to": <%= to([["o", ["right_gui"]]]) %>,
          "conditions": [ <%= frontmost_application_if("pathfinder") %> ]
        }
      ]
    },
    {
      "description": "LeftCommand + O(オー)_デュアルウィンドウトグル(pathfinder)",
      "manipulators": [
        {
          "type": "basic",
          "from": <%= from("o", ["left_control"], ["caps_lock"]) %>,
          "to": <%= to([["f1"]]) %>,
          "conditions": [ <%= frontmost_application_if("pathfinder") %> ]
        }
      ]
    },
    {
      "description": "LeftAlt + D_~/Desktop/(pathfinder)",
      "manipulators": [
        {
          "type": "basic",
          "from": <%= from("d", ["left_alt"], ["caps_lock"]) %>,
          "to": <%= to([["d", ["right_shift", "right_gui"]]]) %>,
          "conditions": [ <%= frontmost_application_if("pathfinder") %> ]
        }
      ]
    },
    {
      "description": "LeftCommand + N_Active_~/Dropbox/(pathfinder)",
      "manipulators": [
        {
      "type": "basic",
      "from": <%= from("n", ["left_control"], ["caps_lock"]) %>,
      "to": <%= set_shell_command(["open -a 'Path Finder.app' '/Users/user/Dropbox'"]) %>,
      "conditions": [ <%= frontmost_application_if("pathfinder") %>]
        }
      ]
    },
    {
      "description": "LeftCommand + M_Active_~/Google ドライブ/(pathfinder)",
      "manipulators": [
        {
      "type": "basic",
      "from": <%= from("m", ["left_control"], ["caps_lock"]) %>,
      "to": <%= set_shell_command(["open -a 'Path Finder.app' '/Users/user/Library/CloudStorage/GoogleDrive-aruke570209@gmail.com/マイドライブ/'"]) %>,
      "conditions": [ <%= frontmost_application_if("pathfinder") %>]
        }
      ]
    },
    {
      "description": "LeftControl_Rename(pathfinder)",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "apple_vendor_top_case_key_code":"keyboard_fn",
          "modifiers": {
              "optional": [
                "caps_lock"
              ]
            }
          },
          "to": [
            {
              "key_code": "return_or_enter"
            }
          ],
          "conditions": [
            {
              "type": "frontmost_application_if",
              "bundle_identifiers": [
                "^com\\.cocoatech\\.PathFinder$"
              ]
            }
          ]
        }
      ]
    }
]
}