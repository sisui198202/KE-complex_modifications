{
  "title": "FrontmostIfPathFinder1",
  "rules": [
    {
      "description": "LeftAlt + U_PathSelection(pathfinder)",
      "manipulators": [
        {
          "type": "basic",
          "from": <%= from("u", ["left_alt"]) %>,
          "to": <%= to([["l", ["left_alt", "left_gui"]]]) %>,
          "conditions": [ <%= frontmost_application_if("pathfinder") %> ]
        }
      ]
    },
    {
      "description": "RightCommand + Space_FileDelete(pathfinder)",
      "manipulators": [
        {
          "type": "basic",
          "from": <%= from("spacebar", ["right_gui"]) %>,
          "to": <%= to([["delete_or_backspace", ["right_gui"]]]) %>,
          "conditions": [ <%= frontmost_application_if("pathfinder") %> ]
        }
      ]
    },
    {
      "description": "LeftAlt + F_NewFileCreate(pathfinder)",
      "manipulators": [
        {
          "type": "basic",
          "from": <%= from("f", ["left_alt"], ["caps_lock"]) %>,
          "to": <%= to([["n", ["right_alt", "right_gui"]]]) %>,
          "conditions": [ <%= frontmost_application_if("pathfinder") %> ]
        }
      ]
    },
    {
      "description": "LeftAlt + Space_OpenFile(pathfinder)",
      "manipulators": [
        {
          "type": "basic",
          "from": <%= from("spacebar", ["left_alt"]) %>,
          "to": <%= to([["button1"], ["button1"]]) %>,
          "conditions": [ <%= frontmost_application_if("pathfinder") %> ]
        }
      ]
    },
    {
      "description": "Enter_OpenFile(pathfinder)",
      "manipulators": [
        {
          "type": "basic",
          "from": <%= from("return_or_enter") %>,
          "to": <%= to([["button1"], ["button1"]]) %>,
          "conditions": [ <%= frontmost_application_if("pathfinder") %> ]
        }
      ]
    },
   {
      "description": "LeftAlt + A_FolderCreate(pathfinder)",
      "manipulators": [
        {
          "type": "basic",
          "from": <%= from("a", ["left_alt"]) %>,
          "to": <%= to([["n", ["right_shift", "right_gui"]]]) %>,
          "conditions": [ <%= frontmost_application_if("pathfinder") %> ]
        }
      ]
    },
   {
      "description": "LeftAlt + Period_CurrentFolderTerminalOpen(pathfinder)",
      "manipulators": [
        {
          "type": "basic",
          "from": <%= from("period", ["left_alt"]) %>,
          "to": <%= to([["period", ["left_control", "right_alt"]]]) %>,
          "conditions": [ <%= frontmost_application_if("pathfinder") %> ]
        }
      ]
    },
    {
      "description": "LeftAlt + Comma_SelectFilePathCopy(pathfinder)",
      "manipulators": [
        {
          "type": "basic",
          "from": <%= from("comma", ["left_alt"]) %>,
          "to": <%= to([["backslash", ["left_control", "left_alt"]]]) %>,
          "conditions": [ <%= frontmost_application_if("pathfinder") %> ]
        }
      ]
    },
    {
      "description": "LeftAlt + SpaceBar_FileOpen(pathfinder)",
      "manipulators": [
        {
          "type": "basic",
          "from": <%= from("spacebar", ["left_alt"]) %>,
          "to": <%= to([["o", ["right_gui"]]]) %>,
          "conditions": [ <%= frontmost_application_if("pathfinder") %> ]
        }
      ]
    },
    {
      "description": "使用しない_delete_FileDelete(pathfinder)",
      "manipulators": [
        {
          "type": "basic",
          "from": <%= from("delete_or_backspace") %>,
          "to": <%= to([["delete_or_backspace", ["right_gui"]]]) %>,
          "conditions": [ <%= frontmost_application_if("pathfinder") %> ]
        }
      ]
    }
]
}