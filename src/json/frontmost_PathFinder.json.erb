{
  "title": "FrontmostIfPathFinder",
  "rules": [
    {
      "description": "LeftAlt + U_PathSelection(pathfinder)",
      "manipulators": [
        {
          "type": "basic",
          "from": <%= from("u", ["left_alt"], ["caps_lock"]) %>,
          "to": <%= to([["l", ["left_alt", "left_gui"]]]) %>,
          "conditions": [ <%= frontmost_application_if("pathfinder") %> ]
        }
      ]
    },
    {
      "description": "LeftAlt + O(オー)_一括名称変更(pathfinder)",
      "manipulators": [
        {
          "type": "basic",
          "from": <%= from("o", ["left_alt"], ["caps_lock"]) %>,
          "to": <%= to([["r", ["left_gui", "left_shift"]]]) %>,
          "conditions": [ <%= frontmost_application_if("pathfinder") %> ]
        }
      ]
    },
    {
      "description": "RightCommand + Space_FileDelete(pathfinder)",
      "manipulators": [
        {
          "type": "basic",
          "from": <%= from("spacebar", ["right_gui"], ["caps_lock"]) %>,
          "to": <%= to([["delete_or_backspace", ["right_gui"]]]) %>,
          "conditions": [ <%= frontmost_application_if("pathfinder") %> ]
        }
      ]
    },
    {
      "description": "RightOption + Z_エイリアスを作る(pathfinder)",
      "manipulators": [
        {
          "type": "basic",
          "from": <%= from("z", ["right_alt"], ["caps_lock"]) %>,
          "to": <%= to([["l", ["right_gui"]]]) %>,
          "conditions": [ <%= frontmost_application_if("pathfinder") %> ]
        }
      ]
    },
    {
      "description": "LeftCommand + Z_オリジナルを表示(pathfinder)",
      "manipulators": [
        {
          "type": "basic",
          "from": <%= from("z", ["left_control"], ["caps_lock"]) %>,
          "to": <%= to([["r", ["right_gui"]]]) %>,
          "conditions": [ <%= frontmost_application_if("pathfinder") %> ]
        }
      ]
    },
    {
      "description": "LeftCommand + T_フォルダーへ移動(pathfinder)",
      "manipulators": [
        {
          "type": "basic",
          "from": <%= from("t", ["left_control"], ["caps_lock"]) %>,
          "to": <%= to([["g", ["left_shift", "right_gui"]]]) %>,
          "conditions": [ <%= frontmost_application_if("pathfinder") %> ]
        }
      ]
    },
    {
      "description": "LeftAlt + F_NewFileCreate(pathfinder)",
      "manipulators": [
        {
          "type": "basic",
          "from": <%= from("f", ["left_alt"], ["caps_lock"]) %>,
          "to": <%= to([["n", ["right_alt", "right_gui"]]]) %>,
          "conditions": [ <%= frontmost_application_if("pathfinder") %> ]
        }
      ]
    },
   {
      "description": "LeftAlt + A_FolderCreate(pathfinder)",
      "manipulators": [
        {
          "type": "basic",
          "from": <%= from("a", ["left_alt"], ["caps_lock"]) %>,
          "to": <%= to([["n", ["right_shift", "right_gui"]]]) %>,
          "conditions": [ <%= frontmost_application_if("pathfinder") %> ]
        }
      ]
    },
   {
      "description": "LeftAlt + Period_CurrentFolderTerminalOpen(pathfinder)",
      "manipulators": [
        {
          "type": "basic",
          "from": <%= from("period", ["left_alt"], ["caps_lock"]) %>,
          "to": <%= to([["period", ["left_control", "right_alt"]]]) %>,
          "conditions": [ <%= frontmost_application_if("pathfinder") %> ]
        }
      ]
    },
    {
      "description": "LeftAlt + Comma_SelectFilePathCopy(pathfinder)",
      "manipulators": [
        {
          "type": "basic",
          "from": <%= from("comma", ["left_alt"], ["caps_lock"]) %>,
          "to": <%= to([["backslash", ["left_control", "left_alt"]]]) %>,
          "conditions": [ <%= frontmost_application_if("pathfinder") %> ]
        }
      ]
    },
    {
      "description": "LeftOption + SpaceBar_FileOpen(pathfinder)",
      "manipulators": [
        {
          "type": "basic",
          "from": <%= from("spacebar", ["left_option"], ["caps_lock"]) %>,
          "to": <%= to([["o", ["right_gui"]]]) %>,
          "conditions": [ <%= frontmost_application_if("pathfinder") %> ]
        }
      ]
    },
    {
      "description": "LeftOption + I_親ディレクトリへ(pathfinder)",
      "manipulators": [
        {
          "type": "basic",
          "from": <%= from("i", ["left_option"], ["caps_lock"]) %>,
          "to": <%= to([["up_arrow", ["right_gui"]]]) %>,
          "conditions": [ <%= frontmost_application_if("pathfinder") %> ]
        }
      ]
    },
    {
      "description": "使用しない_LeftOption + O(オー)_FileOpen(pathfinder)",
      "manipulators": [
        {
          "type": "basic",
          "from": <%= from("o", ["left_option"], ["caps_lock"]) %>,
          "to": <%= to([["o", ["right_gui"]]]) %>,
          "conditions": [ <%= frontmost_application_if("pathfinder") %> ]
        }
      ]
    },
    {
      "description": "LeftCommand + O(オー)_デュアルウィンドウトグル(pathfinder)",
      "manipulators": [
        {
          "type": "basic",
          "from": <%= from("o", ["left_control"], ["caps_lock"]) %>,
          "to": <%= to([["f1"]]) %>,
          "conditions": [ <%= frontmost_application_if("pathfinder") %> ]
        }
      ]
    },
    {
      "description": "LeftAlt + D_~/Desktop/(pathfinder)",
      "manipulators": [
        {
          "type": "basic",
          "from": <%= from("d", ["left_alt"], ["caps_lock"]) %>,
          "to": <%= to([["d", ["right_shift", "right_gui"]]]) %>,
          "conditions": [ <%= frontmost_application_if("pathfinder") %> ]
        }
      ]
    },
    {
      "description": "LeftOption + N_Active_~/Dropbox/(pathfinder)",
      "manipulators": [
        {
      "type": "basic",
      "from": <%= from("n", ["left_option"], ["caps_lock"]) %>,
      "to": <%= set_shell_command(["open -a 'Path Finder.app' '/Users/user/Dropbox'"]) %>,
      "conditions": [ <%= frontmost_application_if("pathfinder") %>]
        }
      ]
    },
    {
      "description": "LeftOption + M_Active_~/Google ドライブ/(pathfinder)",
      "manipulators": [
        {
      "type": "basic",
      "from": <%= from("m", ["left_option"], ["caps_lock"]) %>,
      "to": <%= set_shell_command(["open -a 'Path Finder.app' '/Users/user/Library/CloudStorage/GoogleDrive-aruke570209@gmail.com/マイドライブ/'"]) %>,
      "conditions": [ <%= frontmost_application_if("pathfinder") %>]
        }
      ]
    },
    {
      "description": "LeftCommand + N_HeadRowCursol(pathfinder)",
      "manipulators": [
        {
      "type": "basic",
      "from": <%= from("n", ["left_control"], ["caps_lock"]) %>,
      "to": <%= to([["left_arrow", ["fn"]]]) %>,
      "conditions": [ <%= frontmost_application_if("pathfinder") %>]
        }
      ]
    },
    {
      "description": "LeftCommand + M_EndRowCursol(pathfinder)",
      "manipulators": [
        {
      "type": "basic",
      "from": <%= from("m", ["left_control"], ["caps_lock"]) %>,
      "to": <%= to([["right_arrow", ["fn"]]]) %>,
      "conditions": [ <%= frontmost_application_if("pathfinder") %>]
        }
      ]
    },
    {
      "description": "RightCommand + L_Active_~/マイドライブ/Summary(pathfinder)",
      "manipulators": [
        {
      "type": "basic",
      "from": <%= from("l", ["right_gui"], ["caps_lock"]) %>,
      "to": <%= set_shell_command(["open -a 'Path Finder.app' '/Users/user/Library/CloudStorage/GoogleDrive-aruke570209@gmail.com/マイドライブ/Summary'"]) %>,
      "conditions": [ <%= frontmost_application_if("pathfinder") %>]
        }
      ]
    },
    {
      "description": "RightOption + S_Active_~/MacWindowShare/(pathfinder)",
      "manipulators": [
        {
      "type": "basic",
      "from": <%= from("s", ["right_option"], ["caps_lock"]) %>,
      "to": <%= set_shell_command(["open -a 'Path Finder.app' '/Users/user/MacWindowShare'"]) %>,
      "conditions": [ <%= frontmost_application_if("pathfinder") %>]
        }
      ]
    },
    {
      "description": "RightCommand + Semicolon_次のタブを選択(pathfinder)",
      "manipulators": [
        {
      "type": "basic",
      "from": <%= from("semicolon", ["right_gui"], ["caps_lock"]) %>,
      "to": <%= to([["close_bracket", ["right_gui","right_shift"]]]) %>,
      "conditions": [ <%= frontmost_application_if("pathfinder") %>]
        }
      ]
    },
    {
      "description": "RightOption + T_タブセット1(pathfinder)",
      "manipulators": [
        {
      "type": "basic",
      "from": <%= from("t", ["right_option"], ["caps_lock"]) %>,
      "to": <%= to([["p", ["right_gui","left_control"]]]) %>,
      "conditions": [ <%= frontmost_application_if("pathfinder") %>]
        }
      ]
    },
    {
      "description": "LeftControl_Rename(pathfinder)",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "apple_vendor_top_case_key_code":"keyboard_fn",
          "modifiers": {
              "optional": [
                "caps_lock"
              ]
            }
          },
          "to": [
            {
              "key_code": "return_or_enter"
            }
          ],
          "conditions": [
            {
              "type": "frontmost_application_if",
              "bundle_identifiers": [
                "^com\\.cocoatech\\.PathFinder$"
              ]
            }
          ]
        }
      ]
    },
 {
      "description": "Fn + ~_WindowsControl(pathfinder)",
      "manipulators": [
        {
          "description": "Fn + U_WindowLeftUp",
          "type": "basic",
          "from": <%= from("u", ["right_control"], ["caps_lock"]) %>,
          "to": <%= to([["6", "right_alt"]]) %>,
          "conditions": [ <%= frontmost_application_if("pathfinder") %> ]
        },
    {
        "description": "Fn + J_WindowLeftDown",
