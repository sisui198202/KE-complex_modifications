{
    "title": "FrontmostIfMysqlworkbench_summary",
    "rules": [
      {
        "description": "FrontmostIfMysqlworkbench_summary",
        "manipulators": [
          {
            "description": "LeftCommand + A_RowHeadCursolMove",
            "type": "basic",
            "from": <%= from("a", ["left_control"]) %>,
            "to": <%= to([["left_arrow",["fn"]]]) %>,
            "conditions": [ <%= frontmost_application_if("mysqlworkbench") %> ]
          },
          {
            "description": "LeftCommand + Semicolon_RowEndCursolMove",
            "type": "basic",
            "from": <%= from("semicolon", ["left_control"]) %>,
            "to": <%= to([["right_arrow",["fn"]]]) %>,
            "conditions": [ <%= frontmost_application_if("mysqlworkbench") %> ]
          },
          {
            "description": "LeftCommand + N_HeadRowCursol",
            "type": "basic",
            "from": <%= from("n", ["left_control"]) %>,
            "to": <%= to([["up_arrow", ["right_gui"]]]) %>,
            "conditions": [ <%= frontmost_application_if("mysqlworkbench") %> ]
          },
          {
            "description": "LeftCommand + M_EndRowCursol",
            "type": "basic",
            "from": <%= from("m", ["left_control"]) %>,
            "to": <%= to([["down_arrow", ["right_gui"]]]) %>,
            "conditions": [ <%= frontmost_application_if("mysqlworkbench") %> ]
          }
        ]
      }
    ]
}