{
  "title": "Replacekey_vim1",
  "rules": [
   {
      "description": "Leftoption + Space to Enter_!Finder(Replacekey)",
      "manipulators": [
        {
          "type": "basic",
          "from": <%= from("spacebar", ["left_alt"], ["any"]) %>,
          "to": <%= to([["return_or_enter"]]) %>,
          "conditions": [ <%= frontmost_application_unless("finder") %> ]
        }
      ]
    },
    {
      "description": "LeftCommand + space_BackSpace_!Finder(Replacekey)",
      "manipulators": [
        {
          "type": "basic",
          "from": <%= from("spacebar", ["left_control"], ["caps_lock"]) %>,
          "to": <%= to([["japanese_eisuu"], ["delete_or_backspace"]]) %>,
          "conditions": [ <%= frontmost_application_unless("finder") %> ]
        }
      ]
    },
    {
      "description": "LeftCommand + Rightoption_equal(Replacekey)",
      "manipulators": [
        {
          "type": "basic",
          "from": <%= from("right_alt", ["left_control"], ["caps_lock"]) %>,
          "to": <%= to([["japanese_eisuu"], ["equal_sign"]]) %>,
          "conditions": [ <%= frontmost_application_unless("finder") %> ]
        }
      ]
    },
    {
      "description": "RightCommand + Space_delete_!Finder(Replacekey)",
      "manipulators": [
        {
          "type": "basic",
          "from": <%= from("spacebar", ["right_gui"], ["any"]) %>,
          "to": <%= to([["delete_forward"]]) %>,
          "conditions": [ <%= frontmost_application_unless("finder") %> ]
        }
      ]
    },
    {
      "description": "RightCommand + Leftoption_backslash(Replacekey)",
      "manipulators": [
        {
          "type": "basic",
          "from": <%= from("left_alt", ["right_gui"], ["caps_lock"]) %>,
          "to": <%= to([["japanese_eisuu"], ["backslash"]]) %>
        }
      ]
    },
      {
      "description": "LeftCommand + G_escape(Replacekey)",
      "manipulators": [
        {
          "type": "basic",
          "from": <%= from("g", ["left_control"], ["caps_lock"]) %>,
          "to": <%= to([["escape"]]) %>
        }
      ]
    },
    {
      "description": "LeftCommand + F_Tab(Replacekey)",
      "manipulators": [
        {
          "type": "basic",
          "from": <%= from("f", ["left_control"], ["caps_lock"]) %>,
          "to": <%= to([["tab"]]) %>
        }
      ]
    },
    {
      "description": "LeftAlt_Copy(Replacekey)",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "left_alt",
            "modifiers": {
              "optional": [
                "caps_lock"
              ]
            }
          },
          "to": [
            {
              "key_code": "left_alt"
            }
          ],
          "to_if_alone": [
            {
              "key_code": "japanese_eisuu"
            },
            {
              "key_code": "c",
              "modifiers": "right_gui"
            }
          ]
        }
      ]
    },
    {
      "description": "RightOption_Paste(Replacekey)",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "right_alt",
            "modifiers": {
              "optional": [
                "caps_lock"
              ]
            }
          },
          "to": [
            {
              "key_code": "right_alt"
            }
          ],
          "to_if_alone": [
            {
              "key_code": "v",
              "modifiers": "left_gui"
            }
          ]
        }
      ]
    },
    {
      "description": "Leftoption + Rightoption_parsent(Replacekey)",
      "manipulators": [
        {
          "type": "basic",
          "from": <%= from("right_alt", ["left_alt"], ["caps_lock"]) %>,
          "to": <%= to([["japanese_eisuu"], ["5", "right_shift"]]) %>
        }
      ]
    },
    {
      "description": "Rightoption + Leftoption_anbasado(Replacekey)",
      "manipulators": [
        {
          "type": "basic",
          "from": <%= from("left_alt", ["right_alt"], ["any"]) %>,
          "to": <%= to([["japanese_eisuu"], ["7", "right_shift"]]) %>
        }
      ]
    },
{
                        "description": "コマンドキーを単体で押したときに、英数・かなキーを送信する。（左コマンドキーは英数、右コマンドキーはかな）(Replacekey)",
                        "manipulators": [
                            {
                                "from": {
                                    "key_code": "left_control",
                                    "modifiers": {
                                      "optional": [
                                        "caps_lock"
                                      ]
                                    }
                                },
                                "to": [
                                    {
                                        "key_code": "left_control",
                                        "lazy": true
                                    }
                                ],
                                "to_if_alone": [
                                    {
                                        "key_code": "japanese_eisuu"
                                    }
                                ],
                                "type": "basic"
                            },
                            {
                                "from": {
                                    "key_code": "right_command",
                                    "modifiers": {
                                      "optional": [
                                        "caps_lock"
                                      ]
                                    }
                                },
                                "to": [
                                    {
                                        "key_code": "right_command",
                                        "lazy": true
                                    }
                                ],
                                "to_if_alone": [
                                    {
                                        "key_code": "japanese_kana"
                                    }
                                ],
                                "type": "basic"
                            }
                        ]
                    },
    {
      "description": "LeftCommand + RightCommand_plus(Replacekey)",
      "manipulators": [
        {
          "type": "basic",
          "from": <%= from("right_gui", ["left_control"], ["caps_lock"]) %>,
          "to": <%= to([["japanese_eisuu"], ["equal_sign", "right_shift"]]) %>
        }
      ]
    },
    {
      "description": "RightCommand + LeftCommand_underbar(Replacekey)",
      "manipulators": [
        {
          "type": "basic",
          "from": <%= from("left_control", ["right_gui"], ["caps_lock"]) %>,
          "to": <%= to([["japanese_eisuu"], ["hyphen", "right_shift"]]) %>
        }
      ]
    },
    {
      "description": "Leftoption + Rightcommand_hypen(Replacekey)",
      "manipulators": [
        {
          "type": "basic",
          "from": <%= from("right_gui", ["left_alt"], ["caps_lock"]) %>,
          "to": <%= to([["japanese_eisuu"], ["hyphen"]]) %>
        }
      ]
    },
    {
      "description": "RightCommand + Z,X,C,V,A,S,D,W,E,R_Number0~9(Replacekey)",
      "manipulators":
      <%= each_key(
            source_keys_list: ["z", "x", "c", "v", "s", "d", "f", "w", "e", "r"],
            from_mandatory_modifiers: ["right_gui"],
            from_optional_modifiers: ["caps_lock"],
            dest_keys_list: [["japanese_eisuu", "0"], ["japanese_eisuu", "1"], ["japanese_eisuu", "2"], ["japanese_eisuu", "3"], ["japanese_eisuu", "4"], ["japanese_eisuu", "5"], ["japanese_eisuu", "6"], ["japanese_eisuu", "7"], ["japanese_eisuu", "8"], ["japanese_eisuu", "9"]],
            as_json: true,
          ) %>
    },
        {
      "description": "LeftCommand + H/J/K/L_Arrows(Replacekey)",
      "manipulators":
      <%= each_key(
            source_keys_list: ["h", "j", "k", "l"],
            from_mandatory_modifiers: ["left_control"],
            from_optional_modifiers: ["any"],
            dest_keys_list: ["left_arrow", "down_arrow", "up_arrow", "right_arrow"],
            as_json: true,
          ) %>
        }
  ]
}