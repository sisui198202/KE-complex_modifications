      "manipulators": [
        {
          "type": "basic",
          "from": <%= from("t", ["right_option"], ["caps_lock"]) %>,
          "to": <%= to([["8", ["right_shift","left_gui"]]]) %>,
          "conditions": [ <%= frontmost_application_if("intellij") %> ]
        }
      ]
    },
    {
      "description": "LeftOption + openbracket_アクションの検索(intellij)",
      "manipulators": [
        {
          "type": "basic",
          "from": <%= from("open_bracket", ["left_option"], ["caps_lock"]) %>,
          "to": <%= to([["9", ["right_shift","left_gui"]]]) %>,
          "conditions": [ <%= frontmost_application_if("intellij") %> ]
        }
      ]
    },
    {
      "description": "LeftOption + comma_ルーラー・コンテキスト・メニューの表示(intellij)",
      "manipulators": [
        {
          "type": "basic",
          "from": <%= from("comma", ["left_option"], ["caps_lock"]) %>,
          "to": <%= to([["0", ["right_shift","left_gui"]]]) %>,
          "conditions": [ <%= frontmost_application_if("intellij") %> ]
        }
      ]
    }
 ]
}