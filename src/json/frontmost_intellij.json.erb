{
    "title": "FrontmostIfintellij97",
    "rules": [
      {
        "description": "RightCommand + A_新規Javaクラス(intellij)",
        "manipulators": [
          {
            "type": "basic",
            "from": <%= from("a", ["right_gui"], ["caps_lock"]) %>,
            "to": <%= to([["hyphen", ["left_control","left_option"]]]) %>,
            "conditions": [ <%= frontmost_application_if("intellij") %> ]
          }
        ]
      },
      {
        "description": "RightCommand + T_検査(intellij)",
        "manipulators": [
          {
            "type": "basic",
            "from": <%= from("t", ["right_gui"], ["caps_lock"]) %>,
            "to": <%= to([["1", ["left_control","left_option"]]]) %>,
            "conditions": [ <%= frontmost_application_if("intellij") %> ]
          }
        ]
      },
      {
        "description": "LeftCommand + P_選択範囲の拡大(intellij)",
        "manipulators": [
          {
            "type": "basic",
            "from": <%= from("p", ["left_control"], ["caps_lock"]) %>,
            "to": <%= to([["0", ["right_option","left_shift"]]]) %>,
            "conditions": [ <%= frontmost_application_if("intellij") %> ]
          }
        ]
      }
 ]
}