{
    "title": "FrontmostIfintellij",
    "rules": [
      {
      "description": "LeftCommand + semicolon_CurrentRowCursolEnd(intellij)",
      "manipulators": [
          {
            "type": "basic",
            "from": <%= from("semicolon", ["left_control"], ["caps_lock"]) %>,
            "to": <%= to([["right_arrow", ["right_gui"]]]) %>,
            "conditions": [ <%= frontmost_application_if("intellij") %>]
          }
        ]
      },
      {
      "description": "LeftCommand + A_CurrentRowCursolHead(intellij)",
      "manipulators": [
        {
          "type": "basic",
          "from": <%= from("a", ["left_control"], ["caps_lock"]) %>,
          "to": <%= to([["left_arrow", ["right_gui"]]]) %>,
          "conditions": [ <%= frontmost_application_if("intellij") %>]
        }
      ]
      },
      {
        "description": "LeftCommand + E_次の単語を選択(intellij)",
        "manipulators": [
          {
            "type": "basic",
            "from": <%= from("e", ["left_control"], ["caps_lock"]) %>,
            "to": <%= to([["3", ["right_option", "right_gui"]]]) %>,
            "conditions": [ <%= frontmost_application_if("intellij") %> ]
          }
        ]
      },
      {
        "description": "LeftCommand + P_次の要素を選択(intellij)",
        "manipulators": [
          {
            "type": "basic",
            "from": <%= from("p", ["left_control"], ["caps_lock"]) %>,
            "to": <%= to([["3", ["right_option","left_shift"]]]) %>,
            "conditions": [ <%= frontmost_application_if("intellij") %> ]
          }
        ]
      },
      {
        "description": "RightCommand + H_ビューの表示[パッケージ・エクスプローラー](intellij)",
        "manipulators": [
          {
            "type": "basic",
            "from": <%= from("h", ["right_gui"], ["caps_lock"]) %>,
            "to": <%= to([["1", ["right_option","left_gui"]]]) %>,
            "conditions": [ <%= frontmost_application_if("intellij") %> ]
          }
        ]
      },
      {
      "description": "LeftCommand + O[オウ]_エディター分割切替[垂直](intellij)",
      "manipulators": [
        {
          "type": "basic",
          "from": <%= from("o", ["left_control"], ["caps_lock"]) %>,
          "to": <%= to([["backslash", ["left_control"]]]) %>,
          "conditions": [ <%= frontmost_application_if("intellij") %>]
        }
      ]
      },
      {
      "description": "LeftCommand + 0[ゼロ]_エディター分割切替[水平](intellij)",
      "manipulators": [
        {
          "type": "basic",
          "from": <%= from("0", ["left_control"], ["caps_lock"]) %>,
          "to": <%= to([["open_bracket", ["left_control"]]]) %>,
          "conditions": [ <%= frontmost_application_if("intellij") %>]
        }
      ]
      },
      {
      "description": "RightOption + G_コードアシスト(intellij)",
      "manipulators": [
        {
          "type": "basic",
          "from": <%= from("g", ["right_option"], ["caps_lock"]) %>,
          "to": <%= to([["spacebar", ["left_control"]]]) %>,
          "conditions": [ <%= frontmost_application_if("intellij") %>]
        }
      ]
      },
    {
      "description": "LeftCommand + Z_現在行の下に行を挿入(intellij)",
      "manipulators": [
        {
          "type": "basic",
          "from": <%= from("z", ["left_control"], ["caps_lock"]) %>,
          "to": <%= to([["return_or_enter", ["right_gui"]]]) %>,
          "conditions": [ <%= frontmost_application_if("intellij") %> ]
        }
      ]
    },
    {
      "description": "RightOption + Z_現在行の上に行を挿入(intellij)",
      "manipulators": [
        {
          "type": "basic",
          "from": <%= from("z", ["right_option"], ["caps_lock"]) %>,
          "to": <%= to([["7", ["right_option","left_gui"]]]) %>,
          "conditions": [ <%= frontmost_application_if("intellij") %> ]
        }
      ]
    },
    {
      "description": "LeftOption + backslash_ブロック選択の切り替え(intellij)",
      "manipulators": [
        {
          "type": "basic",
          "from": <%= from("backslash", ["left_option"], ["caps_lock"]) %>,
          "to": <%= to([["2", ["right_option","left_gui"]]]) %>,
          "conditions": [ <%= frontmost_application_if("intellij") %> ]
        }
      ]
    },
    {
      "description": "LeftOption + period_行をコピ(intellij)",
      "manipulators": [
        {
          "type": "basic",
