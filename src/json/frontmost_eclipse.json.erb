{
    "title": "FrontmostIfEclipse90",
    "rules": [
      {
      "description": "LeftCommand + semicolon_CurrentRowCursolEnd(eclipse)",
      "manipulators": [
          {
            "type": "basic",
            "from": <%= from("semicolon", ["left_control"], ["caps_lock"]) %>,
            "to": <%= to([["right_arrow", ["right_gui"]]]) %>,
            "conditions": [ <%= frontmost_application_if("eclipse") %>]
          }
        ]
      },
      {
      "description": "LeftCommand + A_CurrentRowCursolHead(eclipse)",
      "manipulators": [
        {
          "type": "basic",
          "from": <%= from("a", ["left_control"], ["caps_lock"]) %>,
          "to": <%= to([["left_arrow", ["right_gui"]]]) %>,
          "conditions": [ <%= frontmost_application_if("eclipse") %>]
        }
      ]
      },
      {
        "description": "LeftCommand + E_Expand Selection Word(eclipse)",
        "manipulators": [
          {
            "type": "basic",
            "from": <%= from("e", ["left_control"], ["caps_lock"]) %>,
            "to": <%= to([["3", ["left_option","left_gui"]]]) %>,
            "conditions": [ <%= frontmost_application_if("eclipse") %> ]
          }
        ]
      },
      {
        "description": "LeftCommand + P_次の要素を選択(eclipse)",
        "manipulators": [
          {
            "type": "basic",
            "from": <%= from("p", ["left_control"], ["caps_lock"]) %>,
            "to": <%= to([["3", ["right_option","left_shift"]]]) %>,
            "conditions": [ <%= frontmost_application_if("eclipse") %> ]
          }
        ]
      },
      {
      "description": "LeftCommand + O[オウ]_エディター分割切替[水平](eclipse)",
      "manipulators": [
        {
          "type": "basic",
          "from": <%= from("o", ["left_control"], ["caps_lock"]) %>,
          "to": <%= to([["backslash", ["left_control"]]]) %>,
          "conditions": [ <%= frontmost_application_if("eclipse") %>]
        }
      ]
      },
      {
      "description": "LeftCommand + 0[ゼロ]_エディター分割切替[垂直](eclipse)",
      "manipulators": [
        {
          "type": "basic",
          "from": <%= from("0", ["left_control"], ["caps_lock"]) %>,
          "to": <%= to([["open_bracket", ["left_control"]]]) %>,
          "conditions": [ <%= frontmost_application_if("eclipse") %>]
        }
      ]
      },
    {
      "description": "LeftCommand + P_選択範囲拡張(eclipse)",
      "manipulators": [
        {
          "type": "basic",
          "from": <%= from("p", ["left_control"], ["caps_lock"]) %>,
          "to": <%= to([["3", ["right_shift", "right_alt"]]]) %>,
          "conditions": [ <%= frontmost_application_if("eclipse") %> ]
        }
      ]
    },
    {
      "description": "RightCommand + O_SameWordExpandSelection(eclipse)",
      "manipulators": [
        {
          "type": "basic",
          "from": <%= from("o", ["right_gui"], ["caps_lock"]) %>,
          "to": <%= to([["0", ["right_gui", "right_alt"]]]) %>,
          "conditions": [ <%= frontmost_application_if("eclipse") %> ]
        }
      ]
    },
    {
      "description": "LeftCommand + Z_現在行の下に行を挿入(eclipse)",
      "manipulators": [
        {
          "type": "basic",
          "from": <%= from("z", ["left_control"], ["caps_lock"]) %>,
          "to": <%= to([["return_or_enter", ["right_gui"]]]) %>,
          "conditions": [ <%= frontmost_application_if("eclipse") %> ]
        }
      ]
    },
    {
      "description": "RightCommand + J_行をコピー(eclipse)",
      "manipulators": [
        {
          "type": "basic",
          "from": <%= from("j", ["right_gui"], ["caps_lock"]) %>,
          "to": <%= to([["w", ["right_shift", "right_gui"]]]) %>,
          "conditions": [ <%= frontmost_application_if("eclipse") %> ]
        }
      ]
    }
 ]
}