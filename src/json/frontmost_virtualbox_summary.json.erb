{
    "title": "FrontmostIfVirtualbox_summary",
    "rules": [
      {
        "description": "FrontmostIfVirtualbox_summary",
        "manipulators": [
          {
            "description": "RightCommand + Z_0",
            "type": "basic",
            "from": <%= from("z", ["right_gui"], ["caps_lock"]) %>,
            "to": <%= to([["0"]]) %>,
            "conditions": [ <%= frontmost_application_if("centos_ubuntu") %> ]
          },
          {
            "description": "RightCommand + X_1",
            "type": "basic",
            "from": <%= from("x", ["right_gui"], ["caps_lock"]) %>,
            "to": <%= to([["1"]]) %>,
            "conditions": [ <%= frontmost_application_if("centos_ubuntu") %> ]
          },
          {
            "description": "RightCommand + C_2",
            "type": "basic",
