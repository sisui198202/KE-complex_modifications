            "conditions": [ <%= frontmost_application_if("centos_ubuntu") %> ]
          },
          {
            "description": "RightCommand + D_5",
            "type": "basic",
            "from": <%= from("d", ["right_gui"], ["caps_lock"]) %>,
            "to": <%= to([["5"]]) %>,
            "conditions": [ <%= frontmost_application_if("centos_ubuntu") %> ]
          },
          {
            "description": "RightCommand + F_6",
            "type": "basic",
            "from": <%= from("f", ["right_gui"], ["caps_lock"]) %>,
            "to": <%= to([["6"]]) %>,
            "conditions": [ <%= frontmost_application_if("centos_ubuntu") %> ]
          },
          {
            "description": "RightCommand + W_7",
            "type": "basic",
            "from": <%= from("w", ["right_gui"], ["caps_lock"]) %>,
            "to": <%= to([["7"]]) %>,
            "conditions": [ <%= frontmost_application_if("centos_ubuntu") %> ]
          },
          {
            "description": "RightCommand + E_8",
            "type": "basic",
            "from": <%= from("e", ["right_gui"], ["caps_lock"]) %>,
            "to": <%= to([["8"]]) %>,
            "conditions": [ <%= frontmost_application_if("centos_ubuntu") %> ]
          },
          {
            "description": "RightCommand + R_9",
            "type": "basic",
            "from": <%= from("r", ["right_gui"], ["caps_lock"]) %>,
            "to": <%= to([["9"]]) %>,
            "conditions": [ <%= frontmost_application_if("centos_ubuntu") %> ]
          },
          {
            "description": "Leftoption + Rightoption_parsent",
            "type": "basic",
            "from": <%= from("right_alt", ["left_alt"], ["caps_lock"]) %>,
            "to": <%= to([["5", "right_shift"]]) %>,
            "conditions": [ <%= frontmost_application_if("centos_ubuntu") %> ]
          },
          {
            "description": "Rightoption + Leftoption_anbasado",
            "type": "basic",
            "from": <%= from("left_alt", ["right_alt"], ["any"]) %>,
            "to": <%= to([["7", "right_shift"]]) %>,
            "conditions": [ <%= frontmost_application_if("centos_ubuntu") %> ]
          },
          {
            "description": "LeftCommand + RightCommand_plus",
            "type": "basic",
            "from": <%= from("right_gui", ["left_control"], ["caps_lock"]) %>,
            "to": <%= to([["equal_sign", "right_shift"]]) %>,
            "conditions": [ <%= frontmost_application_if("centos_ubuntu") %> ]
          },
          {
            "description": "RightCommand + LeftCommand_underbar",
            "type": "basic",
            "from": <%= from("left_control", ["right_gui"], ["caps_lock"]) %>,
            "to": <%= to([["hyphen", "right_shift"]]) %>,
            "conditions": [ <%= frontmost_application_if("centos_ubuntu") %> ]
          },
          {
            "description": "LeftCommand + space_BackSpace",
            "type": "basic",
            "from": <%= from("spacebar", ["left_control"], ["caps_lock"]) %>,
            "to": <%= to([["delete_or_backspace"]]) %>,
            "conditions": [ <%= frontmost_application_if("centos_ubuntu") %> ]
          },
          {
            "description": "LeftCommand + Rightoption_equal",
            "type": "basic",
            "from": <%= from("right_alt", ["left_control"], ["caps_lock"]) %>,
            "to": <%= to([["equal_sign"]]) %>,
            "conditions": [ <%= frontmost_application_if("centos_ubuntu") %> ]
          },
          {
            "description": "RightCommand + Leftoption_backslash",
            "type": "basic",
            "from": <%= from("left_alt", ["right_gui"], ["caps_lock"]) %>,
            "to": <%= to([["backslash"]]) %>,
            "conditions": [ <%= frontmost_application_if("centos_ubuntu") %> ]
          },
          {
            "description": "LeftCommand + N_HeadRowCursol",
            "type": "basic",
            "from": <%= from("n", ["left_control"], ["caps_lock"]) %>,
            "to": <%= to([["left_arrow", ["fn", "left_shift"]]]) %>,
            "conditions": [ <%= frontmost_application_if("centos_ubuntu") %> ]
          },
          {
            "description": "RightCommand + T_Search",
            "type": "basic",
            "from": <%= from("t", ["right_gui"], ["caps_lock"]) %>,
            "to": <%= to([["f", ["left_control", "left_shift"]]]) %>,
            "conditions": [ <%= frontmost_application_if("centos_ubuntu") %> ]
          },
          {
            "description": "LeftAlt + C_Clear",
            "type": "basic",
            "from": <%= from("c", ["left_alt"], ["caps_lock"]) %>,
            "to": <%= to([["l", ["right_control"]]]) %>,
            "conditions": [ <%= frontmost_application_if("centos_ubuntu") %> ]
          },
          {
            "description": "RightCommand + A_NewFileCreate",
            "type": "basic",
            "from": <%= from("a", ["right_gui"], ["caps_lock"]) %>,
            "to": <%= to([["t", ["left_control", "left_shift"]]]) %>,
            "conditions": [ <%= frontmost_application_if("centos_ubuntu") %> ]
          },
          {
            "description": "RightCommand + G_FileTabClose",
            "type": "basic",
            "from": <%= from("g", ["right_gui"], ["caps_lock"]) %>,
            "to": <%= to([["w", ["left_control", "left_shift"]]]) %>,
            "conditions": [ <%= frontmost_application_if("centos_ubuntu") %> ]
          },
          {
            "description": "LeftCommand + B_PageDown",
            "type": "basic",
            "from": <%= from("b", ["left_control"], ["caps_lock"]) %>,
            "to": <%= to([["page_down", ["left_shift"]]]) %>,
            "conditions": [ <%= frontmost_application_if("centos_ubuntu") %> ]
          },
          {
                  "description": "LeftCommand + Slash_PageUp",
                  "type": "basic",
                  "from": <%= from("slash", ["left_control"], ["caps_lock"]) %>,
                  "to": <%= to([["page_up", ["left_shift"]]]) %>,
                  "conditions": [ <%= frontmost_application_if("centos_ubuntu") %> ]
          },
          {
            "description": "RightOption_Paste(Vitualbox)",
            "type": "basic",
            "from": {
              "key_code": "right_alt",
              "modifiers": {
                "optional": [
                  "caps_lock"
                ]
              }
            },
            "to": [
              {
                "key_code": "right_alt"
              }
            ],
            "to_if_alone": [
              {
                "key_code": "v",
                "modifiers": [
                  "left_shift",
                  "left_control"
                ]
              }
            ],
            "conditions": [
              {
                "type": "frontmost_application_if",
                "bundle_identifiers": [
                  "^org.virtualbox.app.VirtualBoxVM$"
                ]
              }
            ]
          },
          {
            "description": "LeftAlt_Copy(Vitualbox)",
            "type": "basic",
            "from": {
              "key_code": "left_alt",
              "modifiers": {
                "optional": [
                  "caps_lock"
                ]
              }
            },
            "to": [
              {
                "key_code": "left_alt"
              }
            ],
            "to_if_alone": [
              {
                "key_code": "c",
                "modifiers": [
                  "left_shift",
                  "left_control"
                ]
              }
            ],
            "conditions": [
              {
                "type": "frontmost_application_if",
                "bundle_identifiers": [
                  "^org.virtualbox.app.VirtualBoxVM$"
                ]
              }
            ]
          },
          {
            "description": "コマンドキー単体で押す（左コマンドキー英数、右コマンドキーかな）(Vitualbox)",
            "from": {
                "key_code": "left_control",
                "modifiers": {
                  "optional": [
                    "caps_lock"
                  ]
                }
            },
            "to": [
                {
                    "key_code": "left_control",
                    "lazy": true
                }
            ],
            "conditions": [
              {
                "type": "frontmost_application_if",
                "bundle_identifiers": [
                  "^org.virtualbox.app.VirtualBoxVM$"
                ]
              }
            ],
            "to_if_alone": [
                {
                    "key_code": "spacebar",
                    "modifiers": [
                      "left_command"
                  ]
                }
            ],
            "type": "basic"
          },
          {
            "from": {
                "key_code": "right_command",
                "modifiers": {
                  "optional": [
                    "caps_lock"
                  ]
                }
            },
            "to": [
                {
                    "key_code": "right_command",
                    "lazy": true
                }
            ],
            "conditions": [
              {
                "type": "frontmost_application_if",
                "bundle_identifiers": [
                  "^org.virtualbox.app.VirtualBoxVM$"
                ]
              }
            ],
            "to_if_alone": [
              {
                "key_code": "spacebar",
                "modifiers": [
                  "left_command"
              ]
            }
            ],
            "type": "basic"
          }
        ]
      }
    ]
}