{
  "title": "Frontmost-if-sublime",
  "rules": [
    {
      "description": "LeftCommand + Z_RowCursolNewline",
      "manipulators":
      <%=
               each_key(
                 source_keys_list: ["z"],
                 from_mandatory_modifiers: ["left_control"],
                 dest_keys_list: ["return_or_enter"],
                 conditions: [frontmost_application_if("sublime", false)],
                 to_modifiers: ["right_command"],
                 as_json: true
               )
       %>
    }
]
}