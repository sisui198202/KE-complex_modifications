{
    "title": "FrontmostIfpreview3",
    "rules": [
    {
      "description": "J_DownArrow(preview)",
      "manipulators": [
        {
          "type": "basic",
          "from": <%= from("j") %>,
          "to": <%= to([["down_arrow"]]) %>,
          "conditions": [ <%= frontmost_application_if("preview") %> ]
        }
      ]
    },
    {
      "description": "C_ChromeOpen(preview)",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "c"
          },
          "to": [
            {
              "shell_command": "open -a 'Google Chrome.app'"
            }
          ],
          "conditions": [
            {
              "type": "frontmost_application_if",
              "bundle_identifiers": [
                "^com\\.apple\\.Preview$"
              ]
            }
          ]
        }
      ]
    },
   {
      "description": "k_UpArrow(preview)",
      "manipulators": [
        {
          "type": "basic",
          "from": <%= from("k") %>,
          "to": <%= to([["up_arrow"]]) %>,
          "conditions": [ <%= frontmost_application_if("preview") %> ]
        }
      ]
    },
   {
      "description": "h_LeftArrow(preview)",
      "manipulators": [
        {
          "type": "basic",
          "from": <%= from("h") %>,
          "to": <%= to([["left_arrow"]]) %>,
          "conditions": [ <%= frontmost_application_if("preview") %> ]
        }
      ]
    },
   {
      "description": "L_RigthArrow(preview)",
      "manipulators": [
        {
          "type": "basic",
          "from": <%= from("l") %>,
          "to": <%= to([["right_arrow"]]) %>,
          "conditions": [ <%= frontmost_application_if("preview") %> ]
        }
      ]
    },
  {
      "description": "M_EndRowCursol(preview)",
      "manipulators": [
        {
          "type": "basic",
          "from": <%= from("m") %>,
          "to": <%= to([["right_arrow", ["fn"]]]) %>,
          "conditions": [ <%= frontmost_application_if("preview") %> ]
        }
      ]
    },
  {
      "description": "N_HeadRowCursol(preview)",
      "manipulators": [
        {
          "type": "basic",
          "from": <%= from("n") %>,
          "to": <%= to([["left_arrow", ["fn"]]]) %>,
          "conditions": [ <%= frontmost_application_if("preview") %> ]
        }
      ]
    },
    {
      "description": "LeftCommand + O_Expansion(preview)",
      "manipulators": [
        {
          "type": "basic",
          "from": <%= from("o", ["left_control"]) %>,
          "to": <%= to([["equal_sign", ["left_shift", "right_gui"]]]) %>,
          "conditions": [ <%= frontmost_application_if("preview") %> ]
        }
      ]
    },
    {
      "description": "LeftCommand + i_Shrink(preview)",
      "manipulators": [
        {
          "type": "basic",
          "from": <%= from("i", ["left_control"]) %>,
          "to": <%= to([["hyphen", ["left_shift", "right_gui"]]]) %>,
          "conditions": [ <%= frontmost_application_if("preview") %> ]
        }
      ]
    },
    {
      "description": "LeftAlt + R_RowNumberSelectMove(preview)",
      "manipulators": [
        {
          "type": "basic",
          "from": <%= from("r", ["left_alt"]) %>,
          "to": <%= to([["8", ["left_control", "left_alt"]]]) %>,
          "conditions": [ <%= frontmost_application_if("preview") %> ]
        }
      ]
    },
    {
      "description": "LeftOption + T_サイドバーを非表示(preview)",
      "manipulators": [
        {
          "type": "basic",
          "from": <%= from("t", ["left_option"]) %>,
          "to": <%= to([["1", ["left_control", "right_gui"]]]) %>,
          "conditions": [ <%= frontmost_application_if("preview") %> ]
        }
      ]
    },
    {
      "description": "RightCommand + B_BookMark(preview)",
      "manipulators": [
        {
          "type": "basic",
          "from": <%= from("b", ["right_gui"]) %>,
          "to": <%= to([["d", ["right_gui"]]]) %>,
          "conditions": [ <%= frontmost_application_if("preview") %> ]
        }
      ]
    }
    ]
}