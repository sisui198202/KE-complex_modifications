{
  "title": "FrontmostIfNotion2",
  "rules": [
    {
      "description": "RightOption + A_新しいNotionウィンドウを開く(notion)",
      "manipulators": [
        {
          "type": "basic",
          "from": <%= from("a", ["right_option"], ["caps_lock"]) %>,
          "to": <%= to([["n", ["left_shift", "right_gui"]]]) %>,
          "conditions": [ <%= frontmost_application_if("notion") %> ]
        }
      ]
    }
  ]
}