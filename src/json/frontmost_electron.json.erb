{
    "title": "FrontmostIfElectron",
    "rules": [
      {
        "description": "RightCommand + H_Reload(electron)",
        "manipulators": [
          {
            "type": "basic",
            "from": <%= from("h", ["right_gui"], ["caps_lock"]) %>,
            "to": <%= to([["r", ["right_gui"]]]) %>,
            "conditions": [ <%= frontmost_application_if("electron") %> ]
          }
        ]
      },
    {
      "description": "LeftAlt + d_ToggleDeveloperTools(electron)",
      "manipulators": [
        {
          "type": "basic",
          "from": <%= from("d", ["left_alt"], ["caps_lock"]) %>,
          "to": <%= to([["i", ["right_alt", "right_gui"]]]) %>,
          "conditions": [ <%= frontmost_application_if("electron") %> ]
        }
      ]
    }
    ]
}