{
  "title": "FrontmostIfReadgamen_summary",
  "rules": [
    {
      "description": "FrontmostIfReadgamen_summary",
      "manipulators": [
        {
          "description": "J_DownArrow",
          "type": "basic",
          "from": <%= from("j",["caps_lock"]) %>,
          "to": <%= to([["down_arrow"]]) %>,
          "conditions": [ <%= frontmost_application_if("readgamen") %> ]
        },
        {
          "description": "K_UpArrow",
          "type": "basic",
          "from": <%= from("k",["caps_lock"]) %>,
          "to": <%= to([["up_arrow"]]) %>,
          "conditions": [ <%= frontmost_application_if("readgamen") %> ]
        }
      ]
    }
  ]
}