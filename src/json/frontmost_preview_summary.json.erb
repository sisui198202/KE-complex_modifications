{
    "title": "FrontmostIfPreview_summary1",
    "rules": [
      {
        "description": "FrontmostIfPreview_summary1",
        "manipulators": [
          {
            "description": "J_DownArrow",
            "type": "basic",
            "from": <%= from("j") %>,
            "to": <%= to([["down_arrow"]]) %>,
            "conditions": [ <%= frontmost_application_if("preview") %> ]
          },
          {
            "description": "K_UpArrow",
            "type": "basic",
            "from": <%= from("k") %>,
            "to": <%= to([["up_arrow"]]) %>,
            "conditions": [ <%= frontmost_application_if("preview") %> ]
          },
          {
            "description": "h_LeftArrow",
            "type": "basic",
            "from": <%= from("h") %>,
            "to": <%= to([["left_arrow"]]) %>,
            "conditions": [ <%= frontmost_application_if("preview") %> ]
          },
          {
            "description": "L_RigthArrow",
            "type": "basic",
            "from": <%= from("l") %>,
            "to": <%= to([["right_arrow"]]) %>,
            "conditions": [ <%= frontmost_application_if("preview") %> ]
          },
          {
            "description": "/(スラッシュ)_PageUp",
            "type": "basic",
            "from": <%= from("slash") %>,
            "to": <%= to([["up_arrow", ["fn"]]]) %>,
            "conditions": [ <%= frontmost_application_if("preview") %> ]
          },
          {
            "description": "B_PageDown",
            "type": "basic",
            "from": <%= from("b") %>,
            "to": <%= to([["down_arrow", ["fn"]]]) %>,
            "conditions": [ <%= frontmost_application_if("preview") %> ]
          },
          {
            "description": "Z_保存",
            "type": "basic",
            "from": <%= from("z") %>,
            "to": <%= to([["s", ["right_gui"]]]) %>,
            "conditions": [ <%= frontmost_application_if("preview") %> ]
          },
          {
            "description": "M_WindowBottomHalf",
            "type": "basic",
            "from": <%= from("m") %>,
            "to": <%= to([["5", ["left_alt"]]]) %>,
            "conditions": [ <%= frontmost_application_if("preview") %> ]
          },
          {
            "description": "N_WindowTopHalf",
            "type": "basic",
            "from": <%= from("n") %>,
            "to": <%= to([["4", ["left_alt"]]]) %>,
            "conditions": [ <%= frontmost_application_if("preview") %> ]
          },
          {
            "description": "P_RightHalfWindowMove",
            "type": "basic",
            "from": <%= from("p") %>,
            "to": <%= to([["3", ["right_alt"]]]) %>,
            "conditions": [ <%= frontmost_application_if("preview") %> ]
          },
          {
            "description": "Q_LeftHalfWindowMove",
            "type": "basic",
            "from": <%= from("q") %>,
            "to": <%= to([["2", ["right_alt"]]]) %>,
            "conditions": [ <%= frontmost_application_if("preview") %> ]
          },
          {
            "description": "U_Undo",
            "type": "basic",
            "from": <%= from("u") %>,
            "to": <%= to([["z", ["right_gui"]]]) %>,
            "conditions": [ <%= frontmost_application_if("preview") %> ]
          },
          {
            "description": "T_ブックマークリスト表示",
            "type": "basic",
            "from": <%= from("t") %>,
            "to": <%= to([["2", ["right_gui","right_control"]]]) %>,
            "conditions": [ <%= frontmost_application_if("preview") %> ]
          },
          {
            "description": "Y_サイドバーを非表示",
            "type": "basic",
            "from": <%= from("y") %>,
            "to": <%= to([["1", ["right_gui","right_control"]]]) %>,
            "conditions": [ <%= frontmost_application_if("preview") %> ]
          },
          {
            "description": "D_SummaryDirectory",
            "type": "basic",
            "from": <%= from("d") %>,
            "to": <%= to([["7", ["left_shift", "right_option","right_gui"]],["right_arrow"]]) %>,
            "conditions": [ <%= frontmost_application_if("preview") %> ]
          },
          {
            "description": "オウ_Expansion",
            "type": "basic",
            "from": <%= from("o") %>,
            "to": <%= to([["equal_sign", ["fn", "right_gui"]]]) %>,
            "conditions": [<%= frontmost_application_if("preview") %>]
          },
          {
            "description": "i_Shrink",
            "type": "basic",
            "from": <%= from("i") %>,
            "to": <%= to([["hyphen", ["right_gui"]]]) %>,
            "conditions": [<%= frontmost_application_if("preview") %>]
          },
          {
            "description": "CloseBracket_実際のサイズに合わせる",
            "type": "basic",
            "from": <%= from("close_bracket") %>,
            "to": <%= to([["9", ["right_gui"]]]) %>,
            "conditions": [ <%= frontmost_application_if("preview") %> ]
          },
          {
            "description": "Equalsign_ウィンドウに合わせる",
            "type": "basic",
            "from": <%= from("equal_sign") %>,
            "to": <%= to([["0", ["right_gui"]]]) %>,
            "conditions": [ <%= frontmost_application_if("preview") %> ]
          },
          {
            "description": "R_miアクテイブ",
            "type": "basic",
            "from": <%= from("r") %>,
            "to": <%= set_shell_command(["open -a 'mi'"]) %>,
            "conditions": [ <%= frontmost_application_if("preview") %> ]
          },
          {
            "description": "Semicolon_星マーク新",
            "type": "basic",
            "from": <%= from("semicolon") %>,
            "to": <%= to([["1", ["left_control","left_option"]]]) %>,
            "conditions": [ <%= frontmost_application_if("preview") %> ]
          },
          {
            "description": "Quote_注釈",
            "type": "basic",
            "from": <%= from("quote") %>,
            "to": <%= to([["i", ["right_gui"]]]) %>,
            "conditions": [ <%= frontmost_application_if("preview") %> ]
          },
          {
            "description": "OpenBracket[オープン角括弧]_ページ数指定",
            "type": "basic",
            "from": <%= from("open_bracket") %>,
            "to": <%= to([["7", ["right_gui","right_option"]]]) %>,
            "conditions": [ <%= frontmost_application_if("preview") %> ]
          },
          {
            "description": "G_WindowClose",
            "type": "basic",
            "from": <%= from("g") %>,
            "to": <%= to([["w", ["right_gui"]]]) %>,
            "conditions": [ <%= frontmost_application_if("preview") %> ]
          },
          {
            "description": "V_テキスト",
            "type": "basic",
            "from": <%= from("v") %>,
            "to": <%= to([["t", ["right_gui","right_control"]]]) %>,
            "conditions": [ <%= frontmost_application_if("preview") %> ]
          },
          {
            "description": "F_メモ_[n, [right_gui,right_control],[caps_lock]]は適用不可_検証済",
            "type": "basic",
            "from": <%= from("f") %>,
            "to": <%= to([["9", ["right_option","right_control"]]]) %>,
            "conditions": [ <%= frontmost_application_if("preview") %> ]
          },
          {
            "description": "S_サムネイル",
            "type": "basic",
            "from": <%= from("s") %>,
            "to": <%= to([["2", ["right_option", "right_gui"]]]) %>,
            "conditions": [ <%= frontmost_application_if("preview") %> ]
          },
          {
            "description": "X_ハイライトとメモ",
            "type": "basic",
            "from": <%= from("x") %>,
            "to": <%= to([["4", ["right_option", "right_gui"]]]) %>,
            "conditions": [ <%= frontmost_application_if("preview") %> ]
          },
          {
            "description": "Oオー_マークアップツールバーを非表示",
            "type": "basic",
            "from": <%= from("0") %>,
            "to": <%= to([["a", ["right_shift", "right_gui"]]]) %>,
            "conditions": [ <%= frontmost_application_if("preview") %> ]
          },
          {
            "description": "Hyphen_拡大鏡切替",
            "type": "basic",
            "from": <%= from("hyphen") %>,
            "to": <%= to([["2", ["right_option", "left_control"]]]) %>,
            "conditions": [ <%= frontmost_application_if("preview") %> ]
          },
          {
            "description": "Period_WindowMax",
            "type": "basic",
            "from": <%= from("period") %>,
            "to": <%= to([["f", ["right_control", "right_gui"]]]) %>,
            "conditions": [ <%= frontmost_application_if("preview") %> ]
          },
          {
            "description": "Comma_AppsSelect-Next",
            "type": "basic",
            "from": <%= from("comma") %>,
            "to": <%= to([["tab", "right_gui"]]) %>,
            "conditions": [ <%= frontmost_application_if("preview") %> ]
          },
          {
            "description": "A_現在ウィンドウ複製",
            "type": "basic",
            "from": <%= from("a") %>,
            "to": <%= to([["s", ["right_shift", "right_gui"]]]) %>,
            "conditions": [ <%= frontmost_application_if("preview") %>]
          },
          {
            "description": "E_2ページ",
            "type": "basic",
            "from": <%= from("e") %>,
            "to": <%= to([["3", ["right_gui"]]]) %>,
            "conditions": [ <%= frontmost_application_if("preview") %> ]
          },
          {
            "description": "W_単一ページ",
            "type": "basic",
            "from": <%= from("w") %>,
            "to": <%= to([["2", ["right_gui"]]]) %>,
            "conditions": [ <%= frontmost_application_if("preview") %> ]
          },
          {
            "description": "1(イチ)_RightTitleBarMouseClick(preview)",
            "type": "basic",
            "from": <%= from("1") %>,
            "to": [
              {
                "software_function": {
                  "set_mouse_cursor_position": {
                  "x": "950",
                  "y": "35",
                  "screen": 0
                  }
                }
              },
              { "pointing_button": "button1" }
            ],
            "conditions": [
            {
              "type": "frontmost_application_if",
              "bundle_identifiers": [
              "^com\\.apple\\.Preview$"
              ]
            }
          ]
        },
        {
          "description": "C_ChromeOpen(preview)",
          "type": "basic",
          "from": {
            "key_code": "c"
          },
          "to": [
            {
              "shell_command": "open -a 'Google Chrome.app'"
            }
          ],
          "conditions": [
            {
              "type": "frontmost_application_if",
              "bundle_identifiers": [
                "^com\\.apple\\.Preview$"
              ]
            }
          ]
        },
        {
          "description": "BackSlash_PreviewRight-VscodeLeft(preview)",
          "type": "basic",
          "from": {
            "key_code": "backslash"
          },
          "to": [
            {
              "key_code": "3",
              "modifiers": [
                "right_option"
              ]
            },
          {
            "shell_command": "open -a 'Visual Studio Code.app'"
          }
          ],
          "conditions": [
            {
              "type": "frontmost_application_if",
              "bundle_identifiers": [
                "^com\\.apple\\.Preview$"
              ]
            }
            ]
          },
          {
            "description": "LeftCommand + B_PageDown",
            "type": "basic",
            "from": <%= from("b", ["left_control"], ["caps_lock"]) %>,
            "to": <%= to([["down_arrow", ["fn"]]]) %>,
            "conditions": [ <%= frontmost_application_if("preview") %> ]
          },
          {
            "description": "LeftCommand + Slash_PageUp",
            "type": "basic",
            "from": <%= from("slash", ["left_control"], ["caps_lock"]) %>,
            "to": <%= to([["up_arrow", ["fn"]]]) %>,
            "conditions": [ <%= frontmost_application_if("preview") %> ]
          },
          {
            "description": "LeftCommand + OpenBracket[オープン角括弧]_ページ数指定",
            "type": "basic",
            "from": <%= from("open_bracket",["left_control"],["caps_lock"]) %>,
            "to": <%= to([["7", ["right_gui","right_option"]]]) %>,
            "conditions": [ <%= frontmost_application_if("preview") %> ]
          },
          {
            "description": "LeftCommand + O_2ページ",
            "type": "basic",
            "from": <%= from("o", ["left_control"]) %>,
            "to": <%= to([["3", ["right_gui"]]]) %>,
            "conditions": [ <%= frontmost_application_if("preview") %> ]
          },
          {
            "description": "RightCommand + L_サイズを調整",
            "type": "basic",
            "from": <%= from("l", ["right_gui"], ["caps_lock"]) %>,
            "to": <%= to([["0", ["right_gui", "right_option"]]]) %>,
            "conditions": [ <%= frontmost_application_if("preview") %> ]
          },
          {
            "description": "LeftCommand + W_単一ページ",
            "type": "basic",
            "from": <%= from("w", ["left_control"]) %>,
            "to": <%= to([["2", ["right_gui"]]]) %>,
            "conditions": [ <%= frontmost_application_if("preview") %> ]
          },
          {
            "description": "RightCommand + Slash_Expansion",
            "type": "basic",
            "from": <%= from("slash", ["right_gui"], ["caps_lock"]) %>,
            "to": <%= to([["equal_sign", ["left_shift","right_gui"]]]) %>,
            "conditions": [ <%= frontmost_application_if("preview") %> ]
          },
          {
            "description": "RightOption + B_Shrink",
            "type": "basic",
            "from": <%= from("b", ["right_option"], ["caps_lock"]) %>,
            "to": <%= to([["hyphen", ["left_shift","right_gui"]]]) %>,
            "conditions": [ <%= frontmost_application_if("preview") %> ]
          },
          {
            "description": "LeftAlt + R_RowNumberSelectMove",
            "type": "basic",
            "from": <%= from("r", ["left_alt"]) %>,
            "to": <%= to([["8", ["left_control", "left_alt"]]]) %>,
            "conditions": [ <%= frontmost_application_if("preview") %> ]
          },
          {
            "description": "LeftOption + T_サイドバーを非表示",
            "type": "basic",
            "from": <%= from("t", ["left_option"]) %>,
            "to": <%= to([["1", ["left_control", "right_gui"]]]) %>,
            "conditions": [ <%= frontmost_application_if("preview") %> ]
          },
          {
            "description": "LeftOption + オウ_上書き保存",
            "type": "basic",
            "from": <%= from("o", ["left_option"]) %>,
            "to": <%= to([["s", ["left_gui"]]]) %>,
            "conditions": [ <%= frontmost_application_if("preview") %> ]
          },
          {
            "description": "RightCommand + B_BookMark",
            "type": "basic",
            "from": <%= from("b", ["right_gui"]) %>,
            "to": <%= to([["d", ["right_gui"]]]) %>,
            "conditions": [ <%= frontmost_application_if("preview") %> ]
          },
          {
            "description": "RightCommand + Space_delete",
            "type": "basic",
            "from": <%= from("spacebar", ["right_gui"], ["any"]) %>,
            "to": <%= to([["delete_or_backspace"]]) %>,
            "conditions": [ <%= frontmost_application_if("preview") %> ]
          }
      ]
    }
  ]
}