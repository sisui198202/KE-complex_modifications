{
    "title": "FrontmostIfVirtual_Pararelldesk_Summary",
    "rules": [
    {
      "description": "PararellWindowsKeyBindList(VirtualMachine)",
      "manipulators": [
          {
              "description": "RightCommand + ._MaxWindow(VirtualMachine)",
              "type": "basic",
              "from": <%= from("period", ["right_gui"], ["caps_lock"]) %>,
              "to": <%= to([["7", "left_option"]]) %>,
              "conditions": [ <%= frontmost_application_if("virtual_machine") %>]
          },
          {
            "description": "LeftCommand + W_CurrentCursol-EndRowSelect(VirtualMachine)",
            "type": "basic",
            "from": <%= from("w", ["left_control"], ["caps_lock"]) %>,
            "to": <%= to([["equal_sign", "left_option"]]) %>,
            "conditions": [ <%= frontmost_application_if("virtual_machine") %>]
          },
        {
          "description": "LeftCommand、 英語モードへ",
          "type": "basic",
          "from": {
            "key_code": "left_control",
            "modifiers": {
              "optional": [
                "caps_lock"
              ]
            }
          },
          "to": [
            {
              "key_code": "left_control",
              "lazy": true
            }
          ],
          "to_if_alone": [
            {
              "key_code": "f12",
              "modifiers": [
                "left_control"
              ]
            }
          ],
          "conditions": [
            {
              "type": "frontmost_application_if",
              "bundle_identifiers": [
                "^com.vmware.fusion$",
                "^com.vmware.horizon$",
                "^com.vmware.view$",
                "^com.parallels.desktop$",
                "^com.parallels.vm$",
                "^com.parallels.desktop.console$",
                "^org.virtualbox.app.VirtualBoxVM$",
                "^com.vmware.proxyApp.",
                "^com.parallels.winapp."
              ]
            }
          ],
          "type": "basic"
        },
        {
          "description": "RightCommand、 日本語モードへ",
          "type": "basic",
          "from": {
            "key_code": "right_command",
            "modifiers": {
              "optional": [
                "caps_lock"
              ]
            }
          },
          "to": [
            {
              "key_code": "right_command",
              "lazy": true
            }
          ],
          "to_if_alone": [
            {
              "key_code": "f9",
              "modifiers": [
                "left_control"
              ]
            }
          ],
          "conditions": [
            {
              "type": "frontmost_application_if",
              "bundle_identifiers": [
                "^com.vmware.fusion$",
                "^com.vmware.horizon$",
                "^com.vmware.view$",
                "^com.parallels.desktop$",
                "^com.parallels.vm$",
                "^com.parallels.desktop.console$",
                "^org.virtualbox.app.VirtualBoxVM$",
                "^com.vmware.proxyApp.",
                "^com.parallels.winapp."
              ]
            }
          ],
          "type": "basic"
        },
        {
          "description": "LeftCommand + Comma_AppsSelect-previous(VirtualMachine)",
          "type": "basic",
          "from": {
            "modifiers": {
              "mandatory": [
                "left_control"
              ]
            },
            "key_code": "comma"
          },
          "to": [
            {
              "repeat": true,
              "key_code": "tab",
              "modifiers": [
                "left_alt",
                "right_shift"
