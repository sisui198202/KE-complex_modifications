          "from": {
            "modifiers": {
              "mandatory": [
                "left_control"
              ]
            },
            "key_code": "period"
          },
          "to": [
            {
              "repeat": true,
              "key_code": "tab",
              "modifiers": [
                "right_alt"
              ],
              "halt": true
