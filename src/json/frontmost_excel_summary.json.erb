            "description": "RightAlt_Paste",
            "type": "basic",
            "from": {
              "key_code": "right_alt",
              "modifiers": {
                "optional": [
                  "caps_lock"
                ]
              }
            },
            "to": [
              {
                "key_code": "right_alt"
              }
            ],
            "to_if_alone": [
              {
                "key_code": "v",
                "modifiers": "left_gui"
              }
            ],
            "conditions": [
            {
              "type": "frontmost_application_if",
              "bundle_identifiers": [
                "^com\\.microsoft\\.Excel$"
              ]
            }
          ]
        },
        {
          "description": "Leftoption+Openbracket_クイックアクセスツールバーの画像挿入クリック",
          "type": "basic",
          "from": {
            "key_code": "open_bracket",
            "modifiers": {
              "mandatory": [
                "left_option"
              ],
              "optional": [
                "caps_lock"
              ]
            }
          },
          "to": [
            {
              "software_function": {
                "set_mouse_cursor_position": {
                  "x": "220",
                  "y": "35",
                  "screen": 0
                }
              }
            },
            { "pointing_button": "button1" }
          ],
          "conditions": [
            {
              "type": "frontmost_application_if",
              "bundle_identifiers": [
              "^com\\.microsoft\\.Excel$"
              ]
