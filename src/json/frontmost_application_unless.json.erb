{
  "title": "frontmost-less-vscode",
  "rules": [
    {
      "description": "LeftCommand + N_Home",
      "manipulators":
      <%=
               each_key(
                 source_keys_list: ["n"],
                 from_mandatory_modifiers: ["left_control"],
                 dest_keys_list: ["up_arrow"],
                 conditions: [frontmost_application_if("vscode", false)],
                 to_modifiers: ["right_command"],
                 as_json: true
               )
       %>
    }
]
}