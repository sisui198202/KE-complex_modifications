{
    "title": "FrontmostIfFirefox_summary",
    "rules": [
      {
        "description": "FrontmostIfFirefox_summary",
        "manipulators": [
          {
            "description": "A_新しいタブを開く",
            "type": "basic",
            "from": <%= from("a", ["caps_lock"]) %>,
            "to": <%= to([["n", ["right_alt", "right_gui"]]]) %>,
            "conditions": [ <%= frontmost_application_if("firefox") %> ]
          },
          {
            "description": "E_Reload",
            "type": "basic",
            "from": <%= from("e", ["caps_lock"]) %>,
            "to": <%= to([["r", ["right_gui"]]]) %>,
            "conditions": [ <%= frontmost_application_if("firefox") %> ]
          },
          {
            "description": "W_新しいウィンドウで開く",
            "type": "basic",
            "from": <%= from("w", ["caps_lock"]) %>,
            "to": <%= to([["y", ["right_control", "right_alt"]]]) %>,
            "conditions": [ <%= frontmost_application_if("firefox") %> ]
          },
          {
            "description": "LeftShift + RightCommand + H_ハードキャッシュ",
            "type": "basic",
            "from": <%= from("h", ["left_shift", "right_gui"], ["caps_lock"]) %>,
            "to": <%= to([["r", ["right_shift", "right_gui"]]]) %>,
            "conditions": [ <%= frontmost_application_if("firefox") %> ]
          },
          {
            "description": "LeftAlt + B_ブックマークを管理",
            "type": "basic",
            "from": <%= from("b", ["left_alt"], ["caps_lock"]) %>,
            "to": <%= to([["o", ["right_shift", "right_gui"]]]) %>,
            "conditions": [ <%= frontmost_application_if("firefox") %> ]
          },
          {
            "description": "RightCommand + H_Reload",
            "type": "basic",
            "from": <%= from("h", ["right_gui"], ["caps_lock"]) %>,
            "to": <%= to([["r", ["right_gui"]]]) %>,
            "conditions": [ <%= frontmost_application_if("firefox") %> ]
          },
          {
            "description": "LeftAlt + A_シークレットウィンドウを開く",
            "type": "basic",
            "from": <%= from("a", ["left_alt"], ["caps_lock"]) %>,
            "to": <%= to([["2", ["right_shift", "right_gui"]]]) %>,
            "conditions": [ <%= frontmost_application_if("firefox") %> ]
          },
          {
            "description": "RightAlt + A_新規ウインドウ",
            "type": "basic",
            "from": <%= from("a", ["right_alt"], ["caps_lock"]) %>,
            "to": <%= to([["y", ["right_control", "right_alt"]]]) %>,
            "conditions": [ <%= frontmost_application_if("firefox") %> ]
          },
          {
            "description": "LeftCommand + U_戻る",
            "type": "basic",
            "from": <%= from("u", ["left_control"]) %>,
            "to": <%= to([["left_arrow",["right_gui"]]]) %>,
            "conditions": [ <%= frontmost_application_if("firefox") %> ]
          },
          {
            "description": "RightCommand + A_NewTabCreate",
            "type": "basic",
            "from": <%= from("a", ["right_gui"]) %>,
            "to": <%= to([["n",["right_alt","right_gui"]]]) %>,
            "conditions": [ <%= frontmost_application_if("firefox") %> ]
          },
          {
            "description": "RightCommand + L_ActiTabToggleOnoff",
            "type": "basic",
            "from": <%= from("l", ["right_gui"]) %>,
            "to": <%= to([["left_arrow",["right_gui","right_alt"]]]) %>,
            "conditions": [ <%= frontmost_application_if("firefox") %> ]
          }
        ]
      }
    ]
}