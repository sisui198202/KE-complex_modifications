   {
      "description": "Leftoption + Space_Enter(Replacekey)",
      "manipulators": [
        {
          "type": "basic",
          "from": <%= from("spacebar", ["left_alt"], ["any"]) %>,
          "to": <%= to([["return_or_enter"]]) %>
        }
      ]
    },
    {
      "description": "LeftCommand + space_BackSpace_!Finder(Replacekey)",
      "manipulators": [
        {
          "type": "basic",
          "from": <%= from("spacebar", ["left_control"], ["caps_lock"]) %>,
          "to": <%= to([["japanese_eisuu"], ["delete_or_backspace"]]) %>,
          "conditions": [ <%= frontmost_application_unless("finder") %> ]
        }
      ]
    },
    {
      "description": "LeftCommand + Rightoption_equal(Replacekey)",
