{
  "title": "Replacekey_ver5",
  "rules": [
   {
      "description": "Leftoption + Space to Enter(!Finder)",
      "manipulators": [
        {
          "type": "basic",
          "from": <%= from("spacebar", ["left_alt"], ["any"]) %>,
          "to": <%= to([["return_or_enter"]]) %>,
          "conditions": [ <%= frontmost_application_unless("finder") %> ]
        }
      ]
    },
    {
      "description": "LeftCommand + space_BackSpace(!Finder)",
      "manipulators": [
        {
          "type": "basic",
          "from": <%= from("spacebar", ["left_control"]) %>,
          "to": <%= to([["japanese_eisuu"],["delete_or_backspace"]]) %>,
          "conditions": [ <%= frontmost_application_unless("finder") %> ]
        }
      ]
    },
    {
      "description": "LeftCommand + Rightoption_equal",
      "manipulators": [
        {
          "type": "basic",
          "from": <%= from("right_alt", ["left_control"]) %>,
          "to": <%= to([["japanese_eisuu"],["equal_sign"]]) %>,
          "conditions": [ <%= frontmost_application_unless("finder") %> ]
        }
      ]
    },
    <%# deleteを機能させるには、anyが必要%>
    {
      "description": "RightCommand + Space_delete(!Finder)",
      "manipulators": [
        {
          "type": "basic",
          "from": <%= from("spacebar", ["right_gui"],["any"]) %>,
          "to": <%= to([["delete_forward"]]) %>,
          "conditions": [ <%= frontmost_application_unless("finder") %> ]
        }
      ]
    },
    {
      "description": "RightCommand + Leftoption_backslash",
      "manipulators": [
        {
          "type": "basic",
          "from": <%= from("left_alt", ["right_gui"]) %>,
          "to": <%= to([["japanese_eisuu"],["backslash"]]) %>
        }
      ]
    },
    {
      "description": "LeftCommand + g_escape",
      "manipulators": [
        {
          "type": "basic",
          "from": <%= from("g", ["left_control"]) %>,
          "to": <%= to([["escape"]]) %>
        }
      ]
    },
    <%# shift + 矢印キーを機能させるには、anyが必要%>
    {
      "description": "LeftCommand + I/J/K/L _Arrows",
      "manipulators":
      <%=
               each_key(
                 source_keys_list: ["i","j","k","l"],
                 from_mandatory_modifiers: ["left_control"],
                 from_optional_modifiers: ["any"],
                 dest_keys_list: ["up_arrow","left_arrow","down_arrow","right_arrow"],
                 as_json: true
               )
       %>
    },
    {
      "description": "LeftCommand + f_Tab",
      "manipulators": [
        {
          "type": "basic",
          "from": <%= from("f", ["left_control"]) %>,
          "to": <%= to([["tab"]]) %>
        }
      ]
    },
    {
      "description": "LeftAlt_Copy",
      "manipulators": [
        {
          "type": "basic",
          "from": <%= from("left_alt") %>,
          "to": <%= to([["left_alt"]]) %>,
          "to_if_alone": <%= to([["c","right_gui"]]) %>
        }
      ]
    },
    {
      "description": "RightOption_Paste",
      "manipulators": [
        {
          "type": "basic",
          "from": <%= from("right_alt") %>,
          "to": <%= to([["right_alt"]]) %>,
          "to_if_alone": <%= to([["v","left_gui"]]) %>
        }
      ]
    },
    {
      "description": "Leftoption + Rightoption_parsent_old",
      "manipulators":
      <%=
               each_key(
                 source_keys_list: ["right_alt"],
                 from_mandatory_modifiers: ["left_alt"],
                 dest_keys_list: ["5"],
                 to_modifiers: ["right_shift"],
                 as_json: true
               )
       %>
    },
    {
      "description": "Leftoption + Rightoption_parsent",
      "manipulators": [
        {
          "type": "basic",
          "from": <%= from("right_alt", ["left_alt"]) %>,
          "to": <%= to([["5","right_shift"]]) %>
        }
      ]
    },
    {
      "description": "Rightoption + Leftoption_anbasado_old",
      "manipulators":
      <%=
               each_key(
                 source_keys_list: ["left_alt"],
                 from_mandatory_modifiers: ["right_alt"],
                 from_optional_modifiers: ["any"],
                 dest_keys_list: ["7"],
                 to_modifiers: ["right_shift"],
                 as_json: true
               )
       %>
    },
    {
      "description": "Rightoption + Leftoption_anbasado",
      "manipulators": [
        {
          "type": "basic",
          "from": <%= from("left_alt", ["right_alt"],["any"]) %>,
          "to": <%= to([["7","right_shift"]]) %>
        }
      ]
    },
{
                        "description": "コマンドキーを単体で押したときに、英数・かなキーを送信する。（左コマンドキーは英数、右コマンドキーはかな）",
                        "manipulators": [
                            {
                                "from": {
                                    "key_code": "left_control"
                                },
                                "to": [
                                    {
                                        "key_code": "left_control",
                                        "lazy": true
                                    }
                                ],
                                "to_if_alone": [
                                    {
                                        "key_code": "japanese_eisuu"
                                    }
                                ],
                                "type": "basic"
                            },
                            {
                                "from": {
                                    "key_code": "right_command"
                                },
                                "to": [
                                    {
                                        "key_code": "right_command",
                                        "lazy": true
                                    }
                                ],
                                "to_if_alone": [
                                    {
                                        "key_code": "japanese_kana"
                                    }
                                ],
                                "type": "basic"
                            }
                        ]
                    },
    {
      "description": "LeftCommand + RightCommand_plus",
      "manipulators":
      <%=
               each_key(
                 source_keys_list: ["right_gui"],
                 from_mandatory_modifiers: ["left_control"],
                 dest_keys_list: ["equal_sign"],
                 to_modifiers: ["right_shift"],
                 as_json: true
               )
       %>
    },
   {
      "description": "RightCommand + LeftCommand_underbar",
      "manipulators":
      <%=
               each_key(
                 source_keys_list: ["left_control"],
                 from_mandatory_modifiers: ["right_gui"],
                 dest_keys_list: ["hyphen"],
                 to_modifiers: ["right_shift"],
                 as_json: true
               )
       %>
    },
    {
      "description": "Leftoption + Rightcommand_hypen_old",
      "manipulators":
      <%=
               each_key(
                 source_keys_list: ["right_gui"],
                 from_mandatory_modifiers: ["left_alt"],
                 dest_keys_list: [["japanese_eisuu","hyphen"]],
                 as_json: true
               )
       %>
    },
    {
      "description": "RightCommand + z,x,c,v,a,s,d,w,e,r_Number(0~9)",
      "manipulators":
      <%=
              each_key(
                source_keys_list: ["z", "x", "c", "v","s","d","f","w","e","r"],
                from_mandatory_modifiers: ["right_gui"],
                dest_keys_list: [["japanese_eisuu","0"],["japanese_eisuu","1"], ["japanese_eisuu","2"], ["japanese_eisuu","3"],["japanese_eisuu","4"],["japanese_eisuu","5"],["japanese_eisuu","6"],["japanese_eisuu","7"],["japanese_eisuu","8"],["japanese_eisuu","9"]],
                as_json: true
              )
      %>
    }
  ]
}