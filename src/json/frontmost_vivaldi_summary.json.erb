{
    "title": "FrontmostIfVivaldi_summary96",
    "rules": [
      {
        "description": "FrontmostIfVivaldi_summary96",
        "manipulators": [
          {
            "description": "X_LeftCommand + U_undo(Vivaldi)",
            "type": "basic",
            "from": <%= from("u", ["left_control"]) %>,
            "to": <%= to([["delete_or_backspace"]]) %>,
            "conditions": [ <%= frontmost_application_if("vivaldi") %> ]
          },
          {
            "description": "RightCommand + H_Reload(Vivaldi)",
            "type": "basic",
            "from": <%= from("h", ["right_gui"]) %>,
            "to": <%= to([["r", ["right_gui"]]]) %>,
            "conditions": [ <%= frontmost_application_if("vivaldi") %> ]
          },
          {
            "description": "LeftAlt + C_タブを複製",
            "type": "basic",
            "from": <%= from("c", ["left_alt"], ["caps_lock"]) %>,
            "to": <%= to([["f12", ["right_shift"]]]) %>,
            "conditions": [ <%= frontmost_application_if("vivaldi") %> ]
          },
          {
            "description": "A_新しいタブを開く",
            "type": "basic",
            "from": <%= from("a", ["caps_lock"]) %>,
            "to": <%= to([["n", ["right_gui"]]]) %>,
            "conditions": [ <%= frontmost_application_if("vivaldi") %> ]
          },
          {
            "description": "X:W_新しいウィンドウで開く",
            "type": "basic",
            "from": <%= from("w", ["caps_lock"]) %>,
            "to": <%= to([["a", ["left_option","right_gui"]]]) %>,
            "conditions": [ <%= frontmost_application_if("vivaldi") %> ]
          },
          {
            "description": "C_タブを複製",
            "type": "basic",
            "from": <%= from("c", ["caps_lock"]) %>,
            "to": <%= to([["f12", ["right_shift"]]]) %>,
            "conditions": [ <%= frontmost_application_if("vivaldi") %> ]
          },
          {
            "description": "E_Reload",
            "type": "basic",
            "from": <%= from("e", ["caps_lock"]) %>,
            "to": <%= to([["r", ["right_gui"]]]) %>,
            "conditions": [ <%= frontmost_application_if("vivaldi") %> ]
          },
          {
            "description": "RightCommand + quote_閉じたタブを再度開く",
            "type": "basic",
            "from": <%= from("quote", ["right_gui"], ["caps_lock"]) %>,
            "to": <%= to([["t", ["right_shift", "right_gui"]]]) %>,
            "conditions": [ <%= frontmost_application_if("vivaldi") %> ]
          },
        {
          "description": "LeftShift + RightCommand + H_ハードキャッシュ",
          "type": "basic",
          "from": <%= from("h", ["left_shift", "right_gui"], ["caps_lock"]) %>,
          "to": <%= to([["delete_or_backspace", ["right_gui", "right_shift"]]]) %>,
          "conditions": [ <%= frontmost_application_if("vivaldi") %> ]
        },
        {
          "description": "LeftCommand + T_ドックの位置変更",
          "type": "basic",
          "from": <%= from("t", ["left_control"], ["caps_lock"]) %>,
          "to": <%= to([["d", ["right_gui", "right_shift"]]]) %>,
          "conditions": [ <%= frontmost_application_if("vivaldi") %> ]
        },
        {
          "description": "LeftOption + openbracket_コマンドパレット",
          "type": "basic",
          "from": <%= from("open_bracket", ["left_option"], ["caps_lock"]) %>,
          "to": <%= to([["p", ["right_gui", "right_shift"]]]) %>,
          "conditions": [ <%= frontmost_application_if("vivaldi") %> ]
        },
        {
          "description": "LeftCommand + openbracket_プロジェクト内に存在するファイルを指定して開く",
          "type": "basic",
          "from": <%= from("open_bracket", ["left_control"], ["caps_lock"]) %>,
          "to": <%= to([["p", ["right_gui"]]]) %>,
          "conditions": [ <%= frontmost_application_if("vivaldi") %> ]
        },
        {
          "description": "LeftAlt + E_NotionWebCliper起動",
          "type": "basic",
          "from": <%= from("e", ["left_alt"], ["caps_lock"]) %>,
          "to": <%= to([["1", ["right_gui"]]]) %>,
          "conditions": [ <%= frontmost_application_if("vivaldi") %> ]
        },
        {
          "description": "LeftAlt + N_ノードのスクリーンショットをキャプチャ",
          "type": "basic",
          "from": <%= from("n", ["left_alt"], ["caps_lock"]) %>,
          "to": <%= to([["7", ["right_shift","right_gui"]]]) %>,
          "conditions": [ <%= frontmost_application_if("vivaldi") %> ]
        },
        {
          "description": "RightAlt + A_タブを新しいウィンドウに移動",
          "type": "basic",
          "from": <%= from("a", ["right_alt"], ["caps_lock"]) %>,
          "to": <%= to([["hyphen", ["right_control", "right_alt"]]]) %>,
          "conditions": [ <%= frontmost_application_if("vivaldi") %> ]
        },
        {
          "description": "LeftAlt + A_シークレットウィンドウを開く",
          "type": "basic",
          "from": <%= from("a", ["left_alt"], ["caps_lock"]) %>,
          "to": <%= to([["n", ["right_shift", "right_gui"]]]) %>,
          "conditions": [ <%= frontmost_application_if("vivaldi") %> ]
        },
        {
          "description": "LeftAlt + B_BookmarkManage",
          "type": "basic",
          "from": <%= from("b", ["left_alt"], ["caps_lock"]) %>,
          "to": <%= to([["b", ["right_option", "right_gui"]]]) %>,
          "conditions": [ <%= frontmost_application_if("vivaldi") %> ]
        },
        {
          "description": "LeftAlt + U_検索バーアクティヴ-Copy",
          "type": "basic",
          "from": <%= from("u", ["left_alt"], ["caps_lock"]) %>,
          "to": <%= to([["l","right_gui"], ["c", "right_gui"]]) %>,
          "conditions": [ <%= frontmost_application_if("vivaldi") %> ]
        },
        {
          "description": "LeftAlt + F_Title & URL Picker",
          "type": "basic",
          "from": <%= from("f", ["left_option"], ["caps_lock"]) %>,
          "to": <%= to([["9",["right_shift", "right_gui"]]]) %>,
          "conditions": [ <%= frontmost_application_if("vivaldi") %> ]
        }
        ]
      }
    ]
}