{
    "title": "FunctionkeySummary99",
    "rules": [
        {
            "description": "FunctionkeySummary99",
            "manipulators": [
                {
                    "description": "Double F3 -> Ctrl+Shift+0",
                    "type": "basic",
                    "from": { "key_code": "f3" },
                    "to": [
                        { "set_variable": { "name": "f3_pressed", "value": 0 } },
                        { "key_code": "0", "modifiers": ["control", "shift"] }
                    ],
                    "conditions": [
                        { "type": "variable_if", "name": "f3_pressed", "value": 1 },
                        {
                        "type": "frontmost_application_unless",
                        "bundle_identifiers": [
                            "^com.vmware.fusion$",
                            "^com.vmware.horizon$",
                            "^com.vmware.view$",
                            "^com.parallels.desktop$",
                            "^com.parallels.vm$",
                            "^com.parallels.desktop.console$",
                            "^org.virtualbox.app.VirtualBoxVM$",
                            "^com.vmware.proxyApp.",
                            "^com.parallels.winapp."
                        ]
                        }
                    ]
                },
                {
                    "description": "Single F3 -> Open Excel (delayed, canceled by double-press)",
                    "type": "basic",
                    "from": { "key_code": "f3" },
                    "to": [
                        { "set_variable": { "name": "f3_pressed", "value": 1 } }
                    ],
                    "to_delayed_action": {
                        "to_if_invoked": [
                        { "shell_command": "open '/Users/user/Dropbox/KeyCustomization/Mac/KeyManageList_Mac.xlsm'" },
                        { "set_variable": { "name": "f3_pressed", "value": 0 } }
                        ],
                        "to_if_canceled": [
                        { "set_variable": { "name": "f3_pressed", "value": 0 } }
                        ]
                    },
                    "parameters": {
                        "basic.to_delayed_action_delay_milliseconds": 300
