                "description": "F1_よく使う-md",
                "type": "basic",
                "from": <%= from("f1") %>,
                "to": <%= set_shell_command(["open '/Users/user/Desktop/test.md'"]) %>,
                "conditions": [ <%= frontmost_application_unless("virtual_machine") %>]
            },
            {
                "description": "F2_Rename",
                "type": "basic",
                "from": <%= from("f2") %>,
