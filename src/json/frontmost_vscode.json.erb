{
    "title": "FrontmostIfVscode2",
    "rules": [
        {
            "description": "LeftCommand + N_Home(vscode)_old",
            "manipulators": [
                {
                    "type": "basic",
                    "from": {
                        "key_code": "n",
                        "modifiers": {
                            "mandatory": [
                                "left_control"
                            ]
                        }
                    },
                    "to": [
                        {
                            "key_code": "up_arrow",
                            "modifiers": [
                                "right_gui"
                            ]
                        }
                    ],
                    "conditions": [
                        {
                            "type": "frontmost_application_if",
                            "bundle_identifiers": [
                                "^com\\.microsoft\\.VSCode"
                            ]
                        }
                    ]
                }
            ]
        },
    {
      "description": "LeftCommand + N_Home(vscode)",
      "manipulators": [
        {
          "type": "basic",
          "from": <%= from("n", ["left_control"]) %>,
          "to": <%= to([["up_arrow", ["right_gui"]]]) %>,
          "conditions": [ <%= frontmost_application_if("vscode") %> ]
        }
      ]
    },
        {
            "description": "LeftCommand + M_End(vscode)_old",
            "manipulators": [
                {
                    "type": "basic",
                    "from": {
                        "key_code": "m",
                        "modifiers": {
                            "mandatory": [
                                "left_control"
                            ]
                        }
                    },
                    "to": [
                        {
                            "key_code": "down_arrow",
                            "modifiers": [
                                "right_gui"
                            ]
                        }
                    ],
                    "conditions": [
                        {
                            "type": "frontmost_application_if",
                            "bundle_identifiers": [
                                "^com\\.microsoft\\.VSCode"
                            ]
                        }
                    ]
                }
            ]
        },
    {
      "description": "LeftCommand + M_End(vscode)",
      "manipulators": [
        {
          "type": "basic",
          "from": <%= from("m", ["left_control"]) %>,
          "to": <%= to([["down_arrow", ["right_gui"]]]) %>,
          "conditions": [ <%= frontmost_application_if("vscode") %> ]
        }
      ]
    },
  {
      "description": "LeftCommand + CloseBracket_unindent(vscode)_old",
      "manipulators":
      <%=
               each_key(
                 source_keys_list: ["close_bracket"],
                 from_mandatory_modifiers: ["left_control"],
                 dest_keys_list: ["1"],
                 to_modifiers: ["left_control"],
                 as_json: true
               )
       %>
    },
    {
      "description": "LeftCommand + CloseBracket_unindent(vscode)",
      "manipulators": [
        {
          "type": "basic",
          "from": <%= from("close_bracket", ["left_control"]) %>,
          "to": <%= to([["1", ["left_control"]]]) %>,
          "conditions": [ <%= frontmost_application_if("vscode") %> ]
        }
      ]
    },
   {
      "description": "RightCommand + Slash_SelectAllSameWord2(vscode)_old",
