{
  "title": "frontmost-if-vscode",
  "rules": [
    {
      "description": "LeftCommand + N_Home-vscode",
      "manipulators":
      <%=
               each_key(
                 source_keys_list: ["n"],
                 from_mandatory_modifiers: ["left_control"],
                 dest_keys_list: ["up_arrow"],
                 conditions: [frontmost_application_if("vscode", false)],
                 to_modifiers: ["right_command"],
                 as_json: true
               )
       %>
    },
   {
      "description": "LeftCommand + M_End-vscode",
      "manipulators":
      <%=
               each_key(
                 source_keys_list: ["m"],
                 from_mandatory_modifiers: ["left_control"],
                 dest_keys_list: ["down_arrow"],
                 conditions: [frontmost_application_if("vscode", false)],
                 to_modifiers: ["right_command"],
                 as_json: true
               )
       %>
    }    
]
}