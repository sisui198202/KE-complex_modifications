{
    "title": "FrontmostIfVscode",
    "rules": [
    {
      "description": "LeftCommand + N_HeadRowCursol(vscode)",
      "manipulators": [
        {
          "type": "basic",
          "from": <%= from("n", ["left_control"]) %>,
          "to": <%= to([["up_arrow", ["right_gui"]]]) %>,
          "conditions": [ <%= frontmost_application_if("vscode") %> ]
        }
      ]
    },
    {
      "description": "LeftCommand + M_EndRowCursol(vscode)",
