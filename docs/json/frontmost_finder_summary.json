{
  "title": "FrontmostIfFinder_summary",
  "rules": [
    {
      "description": "FrontmostIfFinder_summary",
      "manipulators": [
        {
          "description": "LeftOption + Y_リスト(Finder)",
          "type": "basic",
          "from": {
            "key_code": "y",
            "modifiers": {
              "mandatory": [
                "left_option"
              ]
            }
          },
          "to": [
            {
              "key_code": "2",
              "modifiers": [
                "left_gui"
              ]
            }
          ],
          "conditions": [
            {
              "type": "frontmost_application_if",
              "bundle_identifiers": [
                "^com\\.apple\\.finder$"
              ]
            }
          ]
        }
      ]
    }
  ]
}
