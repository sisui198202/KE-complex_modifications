{
  "title": "FrontmostIfVscodeSublime2",
  "rules": [
    {
      "description": "LeftCommand + Z_RowCursolNewline(vscode_sublime)",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "z",
            "modifiers": {
              "mandatory": [
                "left_control"
              ]
            }
          },
          "to": [
            {
              "key_code": "return_or_enter",
              "modifiers": [
                "right_gui"
              ]
            }
          ],
          "conditions": [
            {
              "type": "frontmost_application_if",
              "bundle_identifiers": [
                "^com.microsoft.VSCode$",
                "^com.sublimetext.3$"
              ]
            }
          ]
        }
      ]
    },
    {
      "description": "RightCommand + O_SameWordExpandSelection(vscode_sublime)",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "o",
            "modifiers": {
              "mandatory": [
                "right_gui"
              ]
            }
          },
          "to": [
            {
              "key_code": "0",
              "modifiers": [
                "right_gui",
                "right_alt"
              ]
            }
          ]
        }
      ]
    },
    {
      "description": "LeftCommand + O_2SplitEditerScreen(vscode_sublime)",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "o",
            "modifiers": {
              "mandatory": [
                "left_control"
              ],
              "optional": [
                "any"
              ]
            }
          },
          "to": [
            {
              "key_code": "o",
              "modifiers": [
                "left_control",
                "left_shift"
              ]
            },
            {
              "key_code": "i",
              "modifiers": [
                "right_gui",
                "right_alt"
              ]
            },
            {
              "key_code": "n",
              "modifiers": [
                "right_gui"
              ]
            }
          ]
        }
      ]
    },
    {
      "description": "RightCommand + I_FocusNeighboringGroup(vscode_sublime)",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "i",
            "modifiers": {
              "mandatory": [
                "right_gui"
              ],
              "optional": [
                "any"
              ]
            }
          },
          "to": [
            {
              "key_code": "i",
              "modifiers": [
                "right_gui",
                "right_alt"
              ]
            }
          ]
        }
      ]
    },
    {
      "description": "LeftCommand + Semicolon_1EditerScreen(vscode_sublime)",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "semicolon",
            "modifiers": {
              "mandatory": [
                "left_control"
              ]
            }
          },
          "to": [
            {
              "key_code": "h",
              "modifiers": [
                "right_control",
                "left_shift"
              ]
            }
          ]
        }
      ]
    },
    {
      "description": "X_LeftAlt + N_prettifyコード整形(vscode_sublime)",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "n",
            "modifiers": {
              "mandatory": [
                "left_alt"
              ]
            }
          },
          "to": [
            {
              "key_code": "h",
              "modifiers": [
                "right_gui",
                "right_shift"
              ]
            }
          ],
          "conditions": [
            {
              "type": "frontmost_application_if",
              "bundle_identifiers": [
                "^com.sublimetext.3$"
              ]
            }
          ]
        }
      ]
    }
  ]
}
