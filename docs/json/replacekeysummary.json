{
  "title": "ReplaceSummary4",
  "rules": [
    {
      "description": "ReplaceSummary",
      "manipulators": [
        {
          "description": "Leftoption + Space_Enter(Replacekey)",
          "type": "basic",
          "from": {
            "key_code": "spacebar",
            "modifiers": {
              "mandatory": [
                "left_alt"
              ],
              "optional": [
                "any"
              ]
            }
          },
          "to": [
            {
              "key_code": "return_or_enter"
            }
          ]
        },
        {
          "description": "LeftCommand + space_BackSpace_!Finder(Replacekey)",
          "type": "basic",
          "from": {
            "key_code": "spacebar",
            "modifiers": {
              "mandatory": [
                "left_control"
              ],
              "optional": [
                "caps_lock"
              ]
            }
          },
          "to": [
            {
              "key_code": "japanese_eisuu"
            },
            {
              "key_code": "delete_or_backspace"
            }
          ],
          "conditions": [
            {
              "type": "frontmost_application_unless",
              "bundle_identifiers": [
                "^com\\.apple\\.finder$"
              ]
            }
          ]
        },
        {
          "description": "LeftCommand + Rightoption_equal(Replacekey)",
          "type": "basic",
          "from": {
            "key_code": "right_alt",
            "modifiers": {
              "mandatory": [
                "left_control"
              ],
              "optional": [
                "caps_lock"
              ]
            }
          },
          "to": [
            {
              "key_code": "japanese_eisuu"
            },
            {
              "key_code": "equal_sign"
            }
          ],
          "conditions": [
            {
              "type": "frontmost_application_unless",
              "bundle_identifiers": [
                "^org.virtualbox.app.VirtualBoxVM$"
              ]
            }
          ]
        },
        {
          "description": "RightCommand + Space_delete_!Finder(Replacekey)",
          "type": "basic",
          "from": {
            "key_code": "spacebar",
            "modifiers": {
              "mandatory": [
                "right_gui"
              ],
              "optional": [
                "any"
              ]
            }
          },
          "to": [
            {
              "key_code": "delete_forward"
            }
          ],
          "conditions": [
            {
              "type": "frontmost_application_unless",
              "bundle_identifiers": [
                "^com\\.apple\\.finder$"
              ]
            }
          ]
        },
        {
          "description": "RightCommand + Leftoption_backslash(Replacekey)",
          "type": "basic",
          "from": {
            "key_code": "left_alt",
            "modifiers": {
              "mandatory": [
                "right_gui"
              ],
              "optional": [
                "caps_lock"
              ]
            }
          },
          "to": [
            {
              "key_code": "japanese_eisuu"
            },
            {
              "key_code": "backslash"
            }
          ],
          "conditions": [
            {
              "type": "frontmost_application_unless",
              "bundle_identifiers": [
                "^org.virtualbox.app.VirtualBoxVM$"
              ]
            }
          ]
        },
        {
          "description": "LeftCommand + G_escape(Replacekey)",
          "type": "basic",
          "from": {
            "key_code": "g",
            "modifiers": {
              "mandatory": [
                "left_control"
              ],
              "optional": [
                "caps_lock"
              ]
            }
          },
          "to": [
            {
              "key_code": "escape"
            }
          ]
        }
      ]
    }
  ]
}
