{
  "title": "FrontmostIfFirefox",
  "rules": [
    {
      "description": "LeftCommand + U_Un Do(Firefox)",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "u",
            "modifiers": {
              "mandatory": [
                "left_control"
              ]
            }
          },
          "to": [
            {
              "key_code": "open_bracket",
              "modifiers": [
                "right_gui"
              ]
            }
          ],
          "conditions": [
            {
              "type": "frontmost_application_if",
              "bundle_identifiers": [
                "^org\\.mozilla\\.firefox$"
              ]
            }
          ]
        }
      ]
    }
  ]
}
