{
  "title": "FrontmostIfEvernote2",
  "rules": [
    {
      "description": "適用しない_Delete_NoteDelete(Evernote)",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "delete_or_backspace"
          },
          "to": [
            {
              "key_code": "delete_or_backspace",
              "modifiers": [
                "right_gui"
              ]
            }
          ],
          "conditions": [
            {
              "type": "frontmost_application_if",
              "bundle_identifiers": [
                "^com.evernote.Evernote$"
              ]
            }
          ]
        }
      ]
    },
    {
      "description": "x_RightCommand + Spacebar_Delete(Evernote)",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "spacebar",
            "modifiers": {
              "mandatory": [
                "right_gui"
              ],
              "optional": [
                "any"
              ]
            }
          },
          "to": [
            {
              "key_code": "7",
              "modifiers": [
                "left_control",
                "left_shift"
              ]
            }
          ],
          "conditions": [
            {
              "type": "frontmost_application_if",
              "bundle_identifiers": [
                "^com.evernote.Evernote$"
              ]
            }
          ]
        }
      ]
    }
  ]
}
