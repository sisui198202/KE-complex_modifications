{
  "title": "FrontmostIfVscode999",
  "rules": [
    {
      "description": "LeftOption + M_BookmarkSet(vscode)",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "m",
            "modifiers": {
              "mandatory": [
                "left_option"
              ],
              "optional": [
                "caps_lock"
              ]
            }
          },
          "to": [
            {
              "key_code": "k",
              "modifiers": [
                "right_option",
                "right_gui"
              ]
            }
          ],
          "conditions": [
            {
              "type": "frontmost_application_if",
              "bundle_identifiers": [
                "^com.microsoft.VSCode$"
              ]
            }
          ]
        }
      ]
    }
  ]
}
