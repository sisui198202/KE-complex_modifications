{
  "title": "FrontmostIfEclipse97",
  "rules": [
    {
      "description": "LeftCommand + E_Expand Selection Word(eclipse)",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "e",
            "modifiers": {
              "mandatory": [
                "left_control"
              ],
              "optional": [
                "caps_lock"
              ]
            }
          },
          "to": [
            {
              "key_code": "3",
              "modifiers": [
                "left_option",
                "left_gui"
              ]
            }
          ],
          "conditions": [
            {
              "type": "frontmost_application_if",
              "bundle_identifiers": [
                "^epp.package.java$"
              ]
            }
          ]
        }
      ]
    },
    {
