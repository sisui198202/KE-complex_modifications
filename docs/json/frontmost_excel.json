{
  "title": "FrontmostIfExcel_1",
  "rules": [
    {
      "description": "None_RightCommand + H_TagCreate(Excel)",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "h",
            "modifiers": {
              "mandatory": [
                "right_gui"
              ]
            }
          },
          "to": [
            {
              "key_code": "w",
              "modifiers": [
                "left_control",
                "right_shift"
              ]
            }
          ],
          "conditions": [
            {
              "type": "frontmost_application_if",
              "bundle_identifiers": [
                "^com\\.microsoft\\.Excel$"
              ]
            }
          ]
        }
      ]
    }
  ]
}
