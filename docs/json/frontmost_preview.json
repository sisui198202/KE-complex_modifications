{
  "title": "FrontmostIfpreview3",
  "rules": [
    {
      "description": "J_DownArrow(preview)",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "j"
          },
          "to": [
            {
              "key_code": "down_arrow"
            }
          ],
          "conditions": [
            {
              "type": "frontmost_application_if",
              "bundle_identifiers": [
                "^com\\.apple\\.Preview$"
              ]
            }
          ]
        }
      ]
    },
    {
      "description": "C_ChromeOpen(preview)",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "c"
          },
          "to": [
            {
              "shell_command": "open -a 'Google Chrome.app'"
            }
          ],
          "conditions": [
            {
              "type": "frontmost_application_if",
              "bundle_identifiers": [
                "^com\\.apple\\.Preview$"
              ]
            }
          ]
        }
      ]
    },
