{
  "title": "MouseControl",
  "rules": [
    {
      "description": "fnキーを押しながらIキーを押すとmousecursolでUp移動",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "fn",
            "modifiers": {
              "optional": [
                "caps_lock"
              ]
            }
          },
          "to": [
            {
              "set_variable": {
                "name": "vi_mode",
                "value": 1
              }
            }
          ],
          "to_after_key_up": [
            {
              "set_variable": {
                "name": "vi_mode",
                "value": 0
              }
            }
          ]
        },
        {
          "type": "basic",
          "from": {
            "key_code": "i",
            "modifiers": {
              "optional": [
                "caps_lock"
              ]
            }
          },
          "to": [
            {
              "mouse_key": {
                "y": -1800
              }
            }
          ],
          "conditions": [
            {
              "type": "variable_if",
              "name": "vi_mode",
              "value": 1
            }
          ]
        }
      ]
    },
