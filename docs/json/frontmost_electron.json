{
  "title": "FrontmostIfElectron3",
  "rules": [
    {
      "description": "RightCommand + H_Reload(electron)",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "h",
            "modifiers": {
              "mandatory": [
                "right_gui"
              ],
              "optional": [
                "caps_lock"
              ]
            }
          },
          "to": [
            {
              "key_code": "w",
              "modifiers": [
                "right_control",
                "right_shift"
              ]
            }
          ],
          "conditions": [
            {
              "type": "frontmost_application_if",
              "bundle_identifiers": [
                "^10_desktop_mobile_app/010_electron-quick-start/node_modules/electron/dist/Electron.app/Contents/MacOS/Electron$"
              ]
            }
          ]
        }
      ]
    }
  ]
}
