{
  "title": "FrontmostIfElectron",
  "rules": [
    {
      "description": "RightCommand + H_Reload(electron)",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "h",
            "modifiers": {
              "mandatory": [
                "right_gui"
              ],
              "optional": [
                "caps_lock"
              ]
            }
          },
          "to": [
            {
              "key_code": "r",
              "modifiers": [
                "right_gui"
              ]
            }
          ],
          "conditions": [
            {
              "type": "frontmost_application_if",
              "bundle_identifiers": [
                "^com.github.Electron$"
              ]
            }
          ]
        }
      ]
    }
  ]
}
