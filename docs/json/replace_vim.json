{
  "title": "Replacekey_vim",
  "rules": [
    {
      "description": "Leftoption + Space to Enter_!Finder(Replacekey)",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "spacebar",
            "modifiers": {
              "mandatory": [
                "left_alt"
              ],
              "optional": [
                "any"
              ]
            }
          },
          "to": [
            {
              "key_code": "return_or_enter"
            }
          ],
          "conditions": [
            {
              "type": "frontmost_application_unless",
              "bundle_identifiers": [
                "^com\\.apple\\.finder$"
              ]
            }
          ]
        }
      ]
    },
    {
      "description": "LeftCommand + space_BackSpace_!Finder(Replacekey)",
      "manipulators": [
        {
