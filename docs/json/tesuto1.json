{
  "title": "tesuto_case1",
  "rules": [
    {
      "description": "command-EでFinderを開く",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "e",
            "modifiers": {
              "mandatory": [
                "command"
              ],
              "optional": [
                "any"
              ]
            }
          },
          "to": [
            {
              "shell_command": "open -a 'finder'"
            }
          ]
        }
      ]
    }
  ]
}
