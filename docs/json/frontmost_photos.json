{
  "title": "FrontmostIfPhotes98",
  "rules": [
    {
      "description": "RightCommand + Space_delete(photos)",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "spacebar",
            "modifiers": {
              "mandatory": [
                "right_gui"
              ]
            }
          },
          "to": [
            {
              "key_code": "delete_forward"
            }
          ],
          "conditions": [
            {
              "type": "frontmost_application_if",
              "bundle_identifiers": [
                "^com.apple.Photos$"
              ]
            }
          ]
        }
      ]
    }
  ]
}
