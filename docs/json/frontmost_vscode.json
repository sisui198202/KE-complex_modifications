{
  "title": "Frontmost-if-vscode",
  "rules": [
    {
      "description": "LeftCommand + N_Home-vscode",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "n",
            "modifiers": {
              "mandatory": [
                "left_control"
              ]
            }
          },
          "to": [
            {
              "key_code": "up_arrow",
              "modifiers": [
                "right_command"
              ]
            }
          ],
          "conditions": [
            {
              "type": "frontmost_application_if",
              "bundle_identifiers": [
                "^com.microsoft.VSCode$"
              ]
            }
          ]
        }
      ]
    },
    {
      "description": "LeftCommand + M_End-vscode",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "m",
            "modifiers": {
              "mandatory": [
                "left_control"
              ]
            }
          },
          "to": [
            {
              "key_code": "down_arrow",
              "modifiers": [
                "right_command"
              ]
            }
          ],
          "conditions": [
            {
              "type": "frontmost_application_if",
              "bundle_identifiers": [
                "^com.microsoft.VSCode$"
              ]
            }
          ]
        }
      ]
    }
  ]
}
