{
  "title": "FrontmostIfVscode_ver1",
  "rules": [
    {
      "description": "LeftCommand + N_Home(vscode)",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "n",
            "modifiers": {
              "mandatory": [
                "left_control"
              ]
            }
          },
          "to": [
            {
              "key_code": "up_arrow",
              "modifiers": [
                "right_gui"
              ]
            }
          ],
          "conditions": [
            {
              "type": "frontmost_application_if",
              "bundle_identifiers": [
                "^com\\.microsoft\\.VSCode"
              ]
            }
          ]
        }
      ]
    },
    {
      "description": "LeftCommand + M_End(vscode)",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "m",
            "modifiers": {
              "mandatory": [
                "left_control"
              ]
            }
          },
          "to": [
            {
              "key_code": "down_arrow",
              "modifiers": [
                "right_gui"
              ]
            }
          ],
          "conditions": [
            {
              "type": "frontmost_application_if",
              "bundle_identifiers": [
                "^com\\.microsoft\\.VSCode"
              ]
            }
          ]
        }
      ]
    },
    {
      "description": "LeftCommand + CloseBracket_unindent",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "close_bracket",
            "modifiers": {
              "mandatory": [
                "left_control"
              ]
            }
          },
          "to": [
            {
              "key_code": "1",
              "modifiers": [
                "left_control"
              ]
            }
          ]
        }
      ]
    },
    {
      "description": "RightCommand + Slash_SelectAllSameWord2(vscode)",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "slash",
            "modifiers": {
              "mandatory": [
                "right_gui"
              ]
            }
          },
          "to": [
            {
              "key_code": "2",
              "modifiers": [
                "right_control"
              ]
            }
          ]
        }
      ]
    },
    {
      "description": "LeftAlt + b_DebugStart (vscode)",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "b",
            "modifiers": {
              "mandatory": [
                "left_alt"
              ]
            }
          },
          "to": [
            {
              "key_code": "b",
              "modifiers": [
                "right_alt"
              ]
            }
          ]
        }
      ]
    },
    {
      "description": "LeftAlt + T_Terminal Focus (vscode)",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "i",
            "modifiers": {
              "mandatory": [
                "left_alt"
              ]
            }
          },
          "to": [
            {
              "key_code": "4",
              "modifiers": [
                "left_control",
                "left_alt"
              ]
            }
          ]
        }
      ]
    },
    {
      "description": "RightCommand + comma_Terminal Panel Max Toggle (vscode)",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "comma",
            "modifiers": {
              "mandatory": [
                "right_gui"
              ]
            }
          },
          "to": [
            {
              "key_code": "o",
              "modifiers": [
                "left_control",
                "left_alt"
              ]
            }
          ]
        }
      ]
    },
    {
      "description": "Terminal Clear (vscode)",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "u",
            "modifiers": {
              "mandatory": [
                "left_alt"
              ]
            }
          },
          "to": [
            {
              "key_code": "2",
              "modifiers": [
                "right_control",
                "left_alt"
              ]
            }
          ]
        }
      ]
    }
  ]
}
