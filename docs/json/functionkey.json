          "conditions": [
            {
              "type": "variable_if",
              "name": "f1 pressed",
              "value": 1
            },
            {
              "type": "frontmost_application_unless",
              "bundle_identifiers": [
                "^com.parallels.winapp.7d56d4002964a683f165ae699d4ad1ba.dc4dfc2676c848d5bb27640140167936$"
