{
  "title": "frontmost-unless1",
  "rules": [
    {
      "description": "LeftCommand + N_Home",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "n",
            "modifiers": {
              "mandatory": [
                "left_control"
              ]
            }
          },
          "to": [
            {
              "key_code": "left_arrow",
              "modifiers": [
                "fn"
              ]
            }
          ],
          "conditions": [
            {
              "type": "frontmost_application_unless",
              "bundle_identifiers": [
                "^com.microsoft.VSCode$"
              ]
            }
          ]
        }
      ]
    }
  ]
}
