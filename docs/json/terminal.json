{
  "title": "Terminal",
  "rules": [
    {
      "description": "Leftcommand + R_miを開く",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "r",
            "modifiers": {
              "mandatory": [
                "left_control"
              ]
            }
          },
          "to": [
            {
              "shell_command": "open -a 'mi'"
            }
          ]
        }
      ]
    },
    {
      "description": "LeftAlt + J_chromeを開く",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "j",
            "modifiers": {
              "mandatory": [
                "left_alt"
              ]
            }
          },
          "to": [
            {
              "shell_command": "open -a 'Google Chrome.app'"
            }
          ]
        }
      ]
    }
  ]
}
