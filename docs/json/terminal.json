{
  "title": "Terminal",
  "rules": [
    {
      "description": "Leftcommand + R_miを開く(Terminal)",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "r",
            "modifiers": {
              "mandatory": [
                "left_control"
              ]
            }
          },
          "to": [
            {
              "shell_command": "open -a 'mi'"
            }
          ]
        }
      ]
    },
    {
      "description": "LeftAlt + J_chromeを開く(Terminal)",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "j",
            "modifiers": {
              "mandatory": [
                "left_alt"
              ]
            }
          },
          "to": [
            {
              "shell_command": "open -a 'Google Chrome.app'"
            }
          ]
        }
      ]
    },
    {
      "description": "LeftAlt + K_vscodeを開く(Terminal)",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "k",
            "modifiers": {
              "mandatory": [
                "left_alt"
              ]
            }
          },
          "to": [
            {
              "shell_command": "open -a 'Visual Studio Code.app'"
            }
          ]
        }
      ]
    },
    {
      "description": "LeftAlt + semicolon_Path Finderを開く(Terminal)",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "semicolon",
            "modifiers": {
              "mandatory": [
                "left_alt"
              ]
            }
          },
          "to": [
            {
              "shell_command": "open -a 'Path Finder.app'"
            }
          ]
        }
      ]
    },
    {
      "description": "LeftCommand + X_ターミナルを開く(Terminal)",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "x",
            "modifiers": {
              "mandatory": [
                "left_control"
              ]
            }
          },
          "to": [
            {
              "shell_command": "open -a 'Terminal.app'"
            }
          ]
        }
      ]
    },
    {
      "description": "LeftAlt + l_vivaldiを開く(Terminal)",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "l",
            "modifiers": {
              "mandatory": [
                "left_alt"
              ]
            }
          },
          "to": [
            {
              "shell_command": "open -a 'Vivaldi.app'"
            }
          ]
        }
      ]
    }
  ]
}
