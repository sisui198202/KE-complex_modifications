{
  "title": "iterm2Script",
  "rules": [
    {
      "description": "Leftcommand + R_miActiveWindow(iterm2)",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "r",
            "modifiers": {
              "mandatory": [
                "left_control"
              ]
            }
          },
          "to": [
            {
              "shell_command": "open -a 'mi'"
            }
          ]
        }
      ]
    },
    {
      "description": "LeftAlt + J_ChromeActiveWindow(iterm2)",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "j",
            "modifiers": {
              "mandatory": [
                "left_alt"
              ]
            }
          },
          "to": [
            {
              "shell_command": "open -a 'Google Chrome.app'"
            }
          ]
        }
      ]
