{
  "title": "FrontmostIfPararell",
  "rules": [
    {
      "description": "保留_RightAlt + W_ActiveParellDeskWindows(Pararell)",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "w",
            "modifiers": {
              "mandatory": [
                "right_alt"
              ],
              "optional": [
                "caps_lock"
              ]
            }
          },
          "to": [
            {
              "shell_command": "open -a 'mi'"
            }
          ],
          "conditions": [
            {
              "type": "frontmost_application_if",
              "bundle_identifiers": [
                "^com.parallels.desktop.console$"
              ]
            }
          ]
        }
      ]
    }
  ]
}
